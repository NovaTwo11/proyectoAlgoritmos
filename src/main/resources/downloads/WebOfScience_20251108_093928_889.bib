@article{ WOS:001154667400001,
Author = {Mattalo, Brandon},
Title = {Artificial Intelligence: The Future of Pedagogy},
Journal = {JOURNAL OF LEGAL STUDIES EDUCATION},
Year = {2024},
Volume = {41},
Number = {1},
Pages = {49-71},
Month = {DEC},
Abstract = {While it is important to research the negative impact of generative
   artificial intelligence on academic integrity, academics should focus
   most of their efforts on the opportunities these technologies present
   for improving pedagogical practices. In this note, I attempt to flip the
   narrative from one of fear to one of opportunity. I suggest that
   academics should research the use of generative AI to improve teaching
   effectiveness and efficiency. I offer various practical suggestions on
   how these tools can be used to advance pedagogical practices, with
   specific business law examples.},
