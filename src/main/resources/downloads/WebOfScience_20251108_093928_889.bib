@article{ WOS:001170073000001,
Author = {Oswick, Cliff},
Title = {Generative Artificial Intelligence and Generative Conversations:
   Contrasting Futures for Organizational Change?},
Journal = {JOURNAL OF APPLIED BEHAVIORAL SCIENCE},
Year = {2024},
Volume = {60},
Number = {2},
Pages = {225-229},
Month = {JUN},
Abstract = {To what extent are ``generative AI{''} (as a machine-based form of
   decision-making) and ``generative dialogue{''} (as a human-based form of
   decision-making) complimentary or competing? What takes precedence in
   generative change processes? More fundamentally, should generative
   artificial intelligence (AI) processes assist human decision-making or
   should human processes assist generative AI decision-making? The
   possible implications of these questions are explored. Moreover, it is
   posited that as generative AI evolves and gains more traction in
   organizational change initiatives, it is important that it is deployed
   in ways that support generative conversations rather than ways that
   undermine or replace them.},
