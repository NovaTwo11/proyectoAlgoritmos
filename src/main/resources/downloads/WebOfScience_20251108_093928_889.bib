@article{ WOS:001452560900001,
Author = {Roberts, Lynden J. and Jayasena, Rajiv and Khanna, Sankalp and Arnott,
   Leslie and Lane, Paul and Bain, Chris},
Title = {Challenges for implementing generative artificial intelligence (GenAI)
   into clinical healthcare},
Journal = {INTERNAL MEDICINE JOURNAL},
Year = {2025},
Volume = {55},
Number = {7},
Pages = {1063-1069},
Month = {JUL},
Abstract = {Generative artificial intelligence (GenAI) is a form of deep learning AI
   based on inference that offers significant potential in healthcare. It
   has versatile capabilities: GenAI excels in complex human language
   communication, synthesising information from large and diverse datasets
   and performing broad, complex tasks reliably. Other important
   capabilities include scalability, `always on' and cost effectiveness.
   Taken together, GenAI technology appears to possess considerable
   potential for healthcare. However, the implementation poses several
   challenges, including technological problems, regulatory considerations,
   workforce impact and building trust. Using evidence and expert opinion
   to explore these issues, the review aims to inform clinical experts
   about this rapidly evolving field.},
