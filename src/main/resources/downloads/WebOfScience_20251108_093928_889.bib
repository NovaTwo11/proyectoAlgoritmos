@article{ WOS:001583246600006,
Author = {Drusinsky, Doron and Michael, James Bret},
Title = {Generative AI Regulation Is an Undecidable Problem},
Journal = {COMPUTER},
Year = {2025},
Volume = {58},
Number = {10},
Pages = {122-130},
Month = {OCT},
Abstract = {Using computability theory, this article demonstrates that while
   single-prompt generative artificial intelligence (GAI) responses can be
