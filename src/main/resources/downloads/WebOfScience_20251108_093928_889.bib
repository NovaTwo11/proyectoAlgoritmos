@article{ WOS:001437818500001,
Author = {Hanycz, Shaun A. and Antiperovitch, Pavel},
Title = {A practical review of generative AI in cardiac electrophysiology medical
   education},
Journal = {JOURNAL OF ELECTROCARDIOLOGY},
Year = {2025},
Volume = {90},
Month = {MAY-JUN},
Abstract = {Generative artificial intelligence (AI) is a component of artificial
   intelligence that creates synthetic multi-modal output in the form of
   text, images, and audio. Multiple approaches have been implemented into
   teaching surface ECG interpretation. However, learner performance
   remains poor. Generative AI in the form of Generative Adversarial
   Networks (GANs) is a novel AI model that has the potential to augment
   trainee ECG interpretation via creation of synthetic ECGs and anatomical
   depiction of conduction defects. Generative AI may be implemented in
   medical education to customize trainee surface ECG interpretation to
   improve learning and retention.},
