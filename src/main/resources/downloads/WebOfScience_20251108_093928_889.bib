}

@article{ WOS:000552632500009,
Author = {Elgammal, Ahmed and Mazzone, Marian},
Title = {Artists, Artificial Intelligence and Machine-based Creativity in
   Playform},
Journal = {ARTNODES},
Year = {2020},
Number = {26},
Abstract = {Artificial intelligence researchers and artists have trained machines
   and generative processes to produce visually interesting and novel
   works, thereby devising machinic means of creativity. At Artrendex,
   Playform was developed as an easy-to-use programme specifically to be
   used by a broad range of artists, from beginners to those with advanced
   technical skills. This essay focuses on the motivations behind the
   development of Playform and the early reception and use of it by some
   artists. Our aim is to better understand both human and machine-based
   creativity at their intersection in an art-generating system such as
   Playform.},
