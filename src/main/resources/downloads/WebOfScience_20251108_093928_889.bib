}

@article{ WOS:001474696500001,
Author = {Martinez, Javier Roman and Robles, David Triana and Charchmi, Mouhcine
   El Oualidi and Estevez, Ines Salamanca and DeCastro-Garcia, Noemi},
Title = {Generative Artificial Intelligence and Machine Translators in Spanish
   Translation of Early Vulnerability Cybersecurity Alerts},
Journal = {APPLIED SCIENCES-BASEL},
Year = {2025},
Volume = {15},
Number = {8},
Month = {APR 8},
Abstract = {The increasing reliance on artificial intelligence in cybersecurity has
   broadened the role of generative artificial intelligence in tasks such
   as text generation and translation. This study assesses the
   effectiveness of generative artificial intelligence and conventional
   translation tools in translating early vulnerability alerts from English
   to Spanish-a critical process for ensuring the timely dissemination of
   cybersecurity information. Utilizing a dataset provided by the Spanish
   National Cybersecurity Institute, translations were generated using
   various systems and evaluated through linguistic assessment metrics,
   including methods measuring lexical similarity and others capturing
   semantic meaning beyond direct word matching. Additionally, word
   embeddings were employed to enhance the accuracy of semantic similarity
   analysis. The results indicate that conventional translation tools
   generally exhibit greater accuracy and structural fidelity, whereas
   generative artificial intelligence produces more natural-sounding
   translations. However, this flexibility results in greater variability
   in translation quality. The findings suggest that while generative
   artificial intelligence may serve as a valuable complement to
   traditional tools, its inconsistencies may limit its suitability for
   highly technical content that demands precision. This study underscores
   the importance of integrating both approaches to improve the accuracy
   and accessibility of cybersecurity alerts across different languages.},
DOI = {10.3390/app15084090},
Article-Number = {4090},
EISSN = {2076-3417},
ResearcherID-Numbers = {DeCastro-García, Noemí/P-7879-2017},
Unique-ID = {WOS:001474696500001},
}

@article{ WOS:001564253700002,
Author = {Chan, Hau-Ling and Siqin, Tana},
Title = {Supply Chain Management With Generative Artificial Intelligence and
   Internet of Behaviors},
Journal = {IEEE TRANSACTIONS ON ENGINEERING MANAGEMENT},
Year = {2025},
Volume = {72},
Pages = {3650-3666},
Abstract = {Generative artificial intelligence has recently emerged as a
   transformative technology which is able to analyze vast datasets and
   generate valuable insights, particularly in supply chain management.
   Simultaneously, the proliferation of Internet of Behaviors which
   collects and analyzes data on human behaviors enables businesses to gain
   a deeper understanding of user preferences and habits. Despite the
   growing adoption of generative artificial intelligence and Internet of
   Behaviors, their interaction and combined impacts on supply chain
   management remain underexplored. This study aims to address this gap by
   examining how generative artificial intelligence and Internet of
   Behaviors can be leveraged to enhance business operations. To be
   specific, we first review the relevant literature to identify the key
   issues and challenges associated with generative artificial intelligence
   and Internet of Behaviors. We then discuss real-world applications to
   illustrate the practical value these technologies bring to businesses.
   Based on the collected literature and industry practices, we propose a
   framework for businesses to integrate generative artificial intelligence
   and Internet of Behaviors into their supply chain management. This
   framework highlights the significant opportunities for businesses to
   enhance their supply chain performance by adopting these advanced
   technologies. Finally, we build a future research agenda and draw our
   conclusion.},
