@article{ WOS:001161626700013,
Author = {Rosignoli, Silvina},
Title = {Generative Artificial Intelligence and the Training of Trainers
   Contributions from the perspective of Educational Technology},
Journal = {QUESTION},
Year = {2023},
Volume = {3},
Number = {76},
Month = {DEC},
Abstract = {This article represents an initial progress of an ongoing master's
   thesis in Educational Technology, focused on the training of trainers
   for the integration of Generative Artificial Intelligence (GAI) in
   university teaching. The development of a theoretical framework under
   construction is addressed and preliminary inquiries to university
   professors and experts in the field are reported, highlighting the
   didactic challenges that the integration of these technologies present
   for teaching. The need for critical and purposeful reflection on the
   inclusion of these technologies in university teaching is highlighted.
   Some questions are left open.},
