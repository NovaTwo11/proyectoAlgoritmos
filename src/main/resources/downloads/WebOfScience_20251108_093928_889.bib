Unique-ID = {WOS:001442453900001},
}

@article{ WOS:001472559800001,
Author = {Nazareth, Derek L. and Choi, Jae},
Title = {Deterrence and Generative Artificial Intelligence Use in Universities},
Journal = {JOURNAL OF COMPUTER INFORMATION SYSTEMS},
Year = {2025},
Month = {2025 APR 21},
Abstract = {Generative Artificial Intelligence (AI) tools are becoming increasingly
   popular across various domains, and education is no exception. However,
   adopting AI tools in educational settings presents several challenges,
   including their potential misuse concerning academic integrity. This
   study examines the use of generative AI tools in the higher education
   context, and the effect of deterrent options including sanctions, when
   they are inappropriately employed by students. Using a system dynamics
   simulation model, the effect of generative AI tool quality, detection
   tool effectiveness, and sanction severity are systematically examined.
   The results provide insights into managing the opportunities and
   challenges associated with these tools and offer educators and
   administrators potential strategies and recommendations for dealing with
   these new technologies in the higher education context.},
DOI = {10.1080/08874417.2025.2492205},
EarlyAccessDate = {APR 2025},
