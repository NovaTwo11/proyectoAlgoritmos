   Nguyen, Tran Thai Ha/HMO-7569-2023
   },
ORCID-Numbers = {Wang, Cheng-Yen/0000-0003-0900-3905
   Sun, Rongjian/0009-0004-1201-7851},
Unique-ID = {WOS:001531540800001},
}

@article{ WOS:001489251600001,
Author = {Atkinson, Angela G. and Lia, Hillary and Navarro, Sergio M.},
Title = {Advancing scientific writing with artificial intelligence: expanding the
   research toolkit},
Journal = {GLOBAL SURGICAL EDUCATION - JOURNAL OF THE ASSOCIATION FOR SURGICAL
   EDUCATION},
Year = {2024},
Volume = {3},
Number = {1},
Month = {JUN 26},
Abstract = {Generative artificial intelligence (AI) is revolutionizing scientific
   writing by offering novel tools that integrate into various stages of
   the research process. This paper explores the application of AI tools in
   scientific writing, focusing on literature review, non-published work,
   and manuscript preparation, submission, peer review, and publication.
   Detailed use-case descriptions can serve as a practical tutorial for
   novice users to navigate and leverage AI tools effectively in the
   rapidly evolving landscape of generative AI. The paper also discusses
   current policy around use of AI tools regarding scientific writing,
   particularly the risks of inaccuracy, plagiarism, and confidentiality.
   As these tools evolve and integrate more seamlessly into the
   researcher's workflow, communication through scientific writing can
   become more expeditious, lending to increased time for designing and
   implementing projects.},
