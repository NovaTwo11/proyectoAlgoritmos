@article{ WOS:001136768600001,
Author = {Wan, Yuwei and Moorhouse, Benjamin Luke},
Title = {Using Call Annie as a Generative Artificial Intelligence Speaking
   Partner for Language Learners},
Journal = {RELC JOURNAL},
Year = {2025},
Volume = {56},
Number = {2},
Pages = {489-498},
Month = {AUG},
Abstract = {Developing English speaking skills can be challenging for many English
   language learners. The advent of generative artificial intelligence
   (GAI) has prompted the emergence of a growing number of artificial
   intelligence (AI)-powered chatbots designed to tackle these challenges.
   One popular tool is `Call Annie,' a GAI video chatbot that can act as a
   virtual assistant, enabling users to engage in immersive video calls
   with AI avatars. This technology review discusses its functionality, how
   it can be used in supporting learners' language development, how
   teachers can collaborate with it in class and its potential limitations.},
