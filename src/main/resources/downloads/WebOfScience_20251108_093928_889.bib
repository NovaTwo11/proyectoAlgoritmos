@article{ WOS:001508915900009,
Author = {Klaes, Matthew R.},
Title = {A Grammy for Fake Drake? A Federal Right of Publicity in Professional
   Singers' Voices to Protect Vocal Artists from Generative AI},
Journal = {IOWA LAW REVIEW},
Year = {2025},
Volume = {110},
Number = {3},
Pages = {1421-1446},
Month = {MAR},
Abstract = {Artificial intelligence is an increasing presence in our society. There
   are currently no adequate protections against generative artificial
   intelligence creating a digital replica of a singer's voice. Working
   through the history of the right of publicity, specifically the right of
   publicity in an individual's voice, this Note argues that the federal
   government should enact a federal right of publicity for professional
   singers in their voice. Understanding who is protected by this right,
   how long they are protected, and what remedies should be available leads
   to proposed language that allows for generative artificial intelligence
   to be used but protects professional singers from being replaced.},
ISSN = {0021-0552},
Unique-ID = {WOS:001508915900009},
}

