Title = {Study of Trends in Academic Research Using Generative Artificial
   Intelligence: the ACDL Method},
Journal = {NAVUS-REVISTA DE GESTAO E TECNOLOGIA},
Year = {2024},
Volume = {15},
Month = {JAN-DEC},
Abstract = {The use of information technology, specifically Generative Artificial
   Intelligence, in research conducted at higher education institutions is
   justified by the vast volume of accessible data and information that
   would otherwise require significant time to analyze. The rapid access to
   information underscores the need for tools capable of capturing ideas
   that have the potential to become reality. Therefore, the objective of
   this research is to propose an itinerary for surveying trends in
   academic research using iterations with Generative Artificial
   Intelligence tools, inspired by the Delphi Method. The research
   methodology is qualitative, comprising a literature review of scientific
   articles and surveys conducted with experts in University Management to
   explore trends related to the future of universities. The results
   perceived during the research indicate the possibility of analyzing
   trends regarding university management through Generative Artificial
   Intelligence tools. Preliminary conclusions from this exploratory study
   were presented at academic events and received positive feedback from
   specialized critics.},
DOI = {10.22279/navus.v15.2005},
Article-Number = {2005},
ISSN = {2237-4558},
Unique-ID = {WOS:001386505400006},
}

