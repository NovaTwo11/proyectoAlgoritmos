Unique-ID = {WOS:001313366100001},
}

@article{ WOS:001537860000001,
Author = {Kim, Hayeon and Hwang, Jaunghyun and Kim, Taeyoung and Choi, Mideum and
   Lee, Daeho and Ko, Jeongmin},
Title = {Impact of Generative Artificial Intelligence on Learning: Scaffolding
   Strategies and Self-Directed Learning Perspectives},
Journal = {INTERNATIONAL JOURNAL OF HUMAN-COMPUTER INTERACTION},
Year = {2025},
Month = {2025 JUL 26},
Abstract = {Generative artificial intelligence (GenAI) has penetrated various
   industries, including education. However, little research has studied
   the impact of such learning experiences in education. This study
   measured how the learning experiences of users varied depending on the
   type of scaffolding educational strategy provided by GenAI, and it
   identified whether learners' self-directedness moderated the effect of
   GenAI on learning satisfaction. It was determined that learning flow and
   self-efficacy increased learning satisfaction when GenAI was used with
   cognitive scaffolding methods. In addition, the effect of learning flow
   on learning satisfaction was greater for learners with strong
   self-directedness, while the effect of self-efficacy on learning
   satisfaction was more relevant for learners with weak self-directedness.},
DOI = {10.1080/10447318.2025.2531267},
EarlyAccessDate = {JUL 2025},
ISSN = {1044-7318},
EISSN = {1532-7590},
ORCID-Numbers = {Ko, Jeongmin/0000-0002-8226-8056},
Unique-ID = {WOS:001537860000001},
}

