}

@article{ WOS:001532839600004,
Author = {Fan, Lei and Deng, Kunyang and Liu, Fangxue},
Title = {Educational impacts of generative artificial intelligence on learning
   and performance of engineering students in China},
Journal = {SCIENTIFIC REPORTS},
Year = {2025},
Volume = {15},
Number = {1},
Month = {JUL 22},
Abstract = {With the rapid advancement of generative artificial intelligence (AI),
   its potential applications in higher education have attracted
   significant attention. This study investigated how 148 students from
   diverse engineering disciplines and regions across China used generative
   AI, focusing on its impact on their learning experience and the
   opportunities and challenges it poses in engineering education. Based on
   the surveyed data, we explored four key areas: the frequency and
   application scenarios of AI use among engineering students, its impact
   on students' learning and performance, commonly encountered challenges
   in using generative AI, and future prospects for its adoption in
   engineering education. The results showed that more than half of the
   participants reported a positive impact of generative AI on their
   learning efficiency, initiative, and creativity, with nearly half
   believing it also enhanced their independent thinking. However, despite
   acknowledging improved study efficiency, many felt their actual academic
   performance remained largely unchanged and expressed concerns about the
   accuracy and domain-specific reliability of generative AI. Our findings
   provide a first-hand insight into the current benefits and challenges
   generative AI brings to students, particularly Chinese engineering
   students, while offering several recommendations-especially from the
   students' perspective-for effectively integrating generative AI into
   engineering education.},
DOI = {10.1038/s41598-025-06930-w},
Article-Number = {26521},
ISSN = {2045-2322},
Unique-ID = {WOS:001532839600004},
}

@article{ WOS:001503267000001,
Author = {Oubibi, Mohamed},
Title = {Generative artificial intelligence: postgraduate student's digital
   adaptability, engagement and academic confidence},
Journal = {JOURNAL OF COMPUTERS IN EDUCATION},
Year = {2025},
Month = {2025 JUN 6},
Abstract = {Generative Artificial Intelligence (AI) is rapidly becoming a
   transformative force across various disciplines. This research delves
   into the integration of generative AI tools, such as advanced text
   generation models, within the context of creative and academic writing
   among 109 postgraduate students in Thailand. These students were
   randomly assigned to groups that incorporated generative AI tools in
   both guided and independent writing activities. The study involved the
   development of hybrid learning modules that seamlessly combine
   traditional and digital methods. Additionally, a comprehensive survey
   was conducted to assess the students' progress in areas such as creative
   thinking, writing fluency, digital readiness, and academic confidence.
   The findings from this study reveal substantial enhancements in multiple
   dimensions of student engagement, including creative output, writing
   fluidity, and technological adaptability. The research also examines the
   influence of generative AI on academic performance, demonstrating a
   positive correlation with increased student creativity, writing
   confidence, and overall academic well-being. By enhancing students'
   creative capabilities, digital literacy, and familiarity with AI-driven
   tools, educational institutions can significantly improve academic
   performance, boost confidence in writing, and cultivate a more
   innovative academic environment.},
DOI = {10.1007/s40692-025-00364-z},
