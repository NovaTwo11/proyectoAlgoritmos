@article{ WOS:001342003700001,
Author = {Fern, James},
Title = {A More-than-Human Ecology: Evolving Generative Artificial Intelligence
   in Higher Education},
Journal = {EDUCATION SCIENCES},
Year = {2024},
Volume = {14},
Number = {10},
Month = {OCT},
Abstract = {The significant improvements in generative artificial intelligence
   (GenAI) observed in recent years present higher education with both an
   opportunity and a significant challenge. Its successful integration will
   require careful planning and sound pedagogical underpinnings, both in
   regard to learning and teaching as well as assessment and feedback.
