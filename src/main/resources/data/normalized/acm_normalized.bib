@inproceedings{10.1145/3732801.3732826, title = {The Impact of Chatbot-assisted Learning Model on Higher-Order Thinking Skills (HOTS) in a 3D Animation Design Course}, booktitle = {Proceedings of the 2025 2nd International Conference on Informatics Education and Computer Technology Applications}, pages = {122–127}, year = {2025}, isbn = {9798400712432}, doi = {10.1145/3732801.3732826}, url = {https://doi.org/10.1145/3732801.3732826}, author = {Gu, Yadi}, keywords = {3D Animation Design, Chatbot, Generative artificial intelligence, Higher-order thinking skills}, abstract = {Higher-order thinking skills in global education are becoming more and more important. With the launch of ChatGPT, the potential of chatbots in education has been widely explored. However, the impact of chatbot-assisted learning on higher-order thinking skills is not yet known. In this study, a chatbot named C4D Helper was developed by using Coze, a chatbot creation platform in China like Google's Dialogflow. A chatbot-assisted learning model was designed based on the Design Thinking framework to cultivate students' higher-order thinking skills. In order to examine the effectiveness of this model, a pre-posttest quasi-experiment lasting for 5 weeks was conducted. The students in two classes, majoring in Digital Media Technology at Kashi University, were randomly assigned to control group(CG, n=31) and experimental group (EG, n=31). Students in CG learned by traditional teaching model while students in EG learned by chatbot-assisted learning model. The results showed that the chatbot-assisted learning model can significantly improve higher-order thinking skills, including problem-solving skills, critical thinking, creative thinking, and cooperation skills.} }
@inproceedings{10.1145/3745238.3745515, title = {Application of Artificial Intelligence in Traditional Ceramic Art}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1771–1777}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745515}, url = {https://doi.org/10.1145/3745238.3745515}, author = {Shang, Xiang}, keywords = {artificial intelligence, intangible cultural heritage, machine learning algorithms, the techniques of Chinese fine brushwork flower and bird painting, underglaze five-color ceramics}, abstract = {This article analyses the current research status of artificial intelligence in the field of ceramic art at home and abroad, as well as the problems and reasons of traditional ceramic art, and thoroughly studies the techniques of Chinese fine brushwork flower and bird painting, and innovatively integrates it into the creation of underglaze colourful ceramic works. At the same time, artificial intelligence technology was introduced, focusing on how to use artificial intelligence to help traditional ceramic art creation to improve its artistic expression and production efficiency. This paper uses convolutional neural network (CNN) as the infrastructure of the aesthetic evaluation model of underglaze colored ceramic art; selects several underglaze colored ceramic art design schemes generated based on generative adversarial network (GAN) to show their innovative design in modeling, decoration and color; color prediction and design scheme classification and sorting by support vector machine (SVM) and random forest (RF) machine learning algorithms. Combining the functions of artificial intelligence such as image recognition, data analysis and generation, and design, we actively explore the path of innovative development of traditional ceramic art in the context of the new era, and strive to achieve the deep integration of traditional craftsmanship and modern technology, thereby injecting new vitality into the inheritance and development of ceramic art and promoting it to a new stage of development.} }
@inproceedings{10.1145/3759275.3759296, title = {Acceptance of Generative AI Tools: An Extended UTAUT and SEM-ANN Hybrid Model Approach with Chinese University Students}, booktitle = {Proceedings of the 2025 2nd International Conference on Digital Systems and Design Innovation}, pages = {138–149}, year = {2025}, isbn = {9798400719554}, doi = {10.1145/3759275.3759296}, url = {https://doi.org/10.1145/3759275.3759296}, author = {Cheng, Menghan and Lu, Zhaolin}, keywords = {Artificial Intelligence (AI), Artificial Neural Network, Explainable artificial intelligence, Higher education, Structural Equation Modeling (SEM), UTAUT model}, abstract = {University students' adoption of generative AI tools in educational contexts is investigated in this research through two complementary studies. A Jordanian-based cross-sectional study (N=95) initially established the middle students' familiarity (M=3.14, SD=0.81) with generative AI writing tools, along with significant concerns about misinformation/data security (M=3.35, SD=0.85), but high awareness regarding advantages for creativity simulation and innovation facilitation (M=3.62, SD=0.81). Second, a Chinese UTAUT-based research (N=361) employing SEM-ANN hybrid analysis found that DeepSeek adoption intention is significantly determined by Perceived Price (β=0.346), Performance Expectancy (β=0.244), Effort Expectancy (β=0.233), eXplainable AI (β=0.126), and Facilitating Conditions (β=0.084), while Social Influence did not appear to be significant (β=−0.019). Moderator analysis found gender and education level to significantly moderate some predictor-intention relationships. Synthetically, these findings demonstrate: (a) cultural consensus in recognition of AI's value for learning in various capacities despite technical challenges, (b) the critical role of cost disclosure, usability, and explainability in adoption, and (c) stratified intervention needs to address technical instruction, ethical literacy, cost reduction, and maximization of usability – particularly through personalized learning deployments. The study supports the UTAUT's efficacy for use in AI adoption studies while enriching methodology integration through SEM-ANN methods.} }
@inbook{10.1145/3756423.3756511, title = {The Role of Artificial Intelligence in 3D Printing Systems}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing}, pages = {531–535}, year = {2025}, isbn = {9798400714351}, url = {https://doi.org/10.1145/3756423.3756511}, author = {Ng, Tin Hon}, abstract = {This paper explores the role of artificial intelligence (AI) in optimizing the 3D printing industry. AI is developing fast, with a large number of applications shown, such as polishing the statement. The rapid development of AI has allowed its integration into various sectors, including 3D printing. While the applications of AI in many industrial systems have been identified by existing studies, the applications of this emerging technology in another promising technology – 3D printing – have been largely unexplored. This points out the necessity of conducting researches on this issue to facilitate the further development of both AI and 3D printing technologies. This study examines the application of AI across different phases of 3D printing, such as design, usage, production, and maintenance. AI enhances the efficiency of 3D printing by providing generative design alternatives, automating parameter settings, optimizing process controls, and improving product quality through real-time monitoring. AI's ability to store and analyze vast amounts of data enables better decision-making, predictive maintenance, and customized production, reducing costs and enhancing production capabilities. However, AI implementation in 3D printing also faces challenges, including system complexity, data privacy concerns, and the need for extensive sensor networks. As AI technologies continue to evolve, they promise further advancements in flexibility, precision, and cost-efficiency, potentially revolutionizing the 3D printing industry. This paper provides a comprehensive analysis of AI's applications in 3D printing, offering insights into how AI can drive future innovation in additive manufacturing.} }
@inproceedings{10.1145/3708657.3708770, title = {A Review of Artificial Intelligence-Generated Content Detection}, booktitle = {Proceedings of the 2024 10th International Conference on Communication and Information Processing}, pages = {705–709}, year = {2025}, isbn = {9798400717444}, doi = {10.1145/3708657.3708770}, url = {https://doi.org/10.1145/3708657.3708770}, author = {Song, Chunli and Huang, Ying and Wang, Peiyang and Zhang, Zaizai}, keywords = {AIGC detection, Neural networks, Review}, abstract = {With the rapid development of Artificial Intelligence-Generated Content (AIGC) technology, its extensive application in areas such as image generation has attracted widespread attention. However, the proliferation of generated images has also raised issues such as information distortion, which poses challenges to the protection of original content and the assurance of information authenticity. As a result, many detection methods have been proposed in recent years to identify generated images. By analysing existing AIGC detection methods, we categorise them into four groups: frequency domain features, traditional models, image reconstruction and multi-modal large models. Through comparative analysis, this paper highlights the advantages and limitations of current detection methods and outlines potential research directions for the future. Our aim is to provide valuable insights to researchers in related fields, thereby promoting the further development of AIGC detection technology and enhancing our ability to address the challenges posed by AIGC.} }
@inproceedings{10.1145/3719236.3719242, title = {Conceptual Implementation of a Digital Cultural Heritage Application using Generative AI}, booktitle = {Proceedings of the 21st International Conference on Culture and Computer Science: From Humanism to Digital Humanities}, year = {2025}, isbn = {9798400710322}, doi = {10.1145/3719236.3719242}, url = {https://doi.org/10.1145/3719236.3719242}, author = {Schauer, Sophie and Simbeck, Katharina}, keywords = {Cultural Heritage, Generative Artificial Intelligence, Museum Mediation, Digitalisation, Mobile Application, Human-Computer Interaction.}, abstract = {This paper explores the conceptual implementation of an application design, previously developed by communication design students. In an increasingly digitalised world, making cultural heritage accessible and approachable to diverse visitors is critical. The concept idea centres on an interactive video call with the patron of a Renaissance sandstone altar exhibited at the museum. The technical implementation includes generative AI tools for text, audio and animation production. Issues such as ensuring the accuracy of information, maintaining user privacy and preventing biases in AI-generated content are identified. The importance of co-creation with experts and adherence to ethical standards are highlighted to create a reliable educational tool. This implementation proposal forms the foundation for further development and future deployment as part of the museum’s permanent exhibition, aiming to transform how visitors experience and interact with cultural heritage artefacts.} }
@inproceedings{10.1145/3641308.3677403, title = {Inside Out: Emotion GaRage Vol. V}, booktitle = {Adjunct Proceedings of the 16th International Conference on Automotive User Interfaces and Interactive Vehicular Applications}, pages = {260–263}, year = {2024}, isbn = {9798400705205}, doi = {10.1145/3641308.3677403}, url = {https://doi.org/10.1145/3641308.3677403}, author = {Dong, Jiayuan and Gowda, Nikhil and Wang, Yiyuan and Choe, Mungyeong and Alsaid, Areen and Alvarez, Ignacio and Krome, Sven and Jeon, Myounghoon}, keywords = {affective external human-machine interaction designs, emotions, empathic in-vehicle interfaces, generative artificial intelligence}, abstract = {The rapid advancement of automated vehicles has aroused the curiosity of researchers in the automotive field. Understanding the emotional aspects of this technology is critical to improving human-vehicle interactions. The topics of the proposed workshop will be expanded from internal to external empathetic interface designs of automated vehicles. The workshop will gather researchers and practitioners to brainstorm and design affective internal and external interfaces for automated vehicles, targeting specific use cases within the social context. During the workshop, participants will use an affective design tool and generative AI to prototype affective interface designs in automated vehicles. With this creative approach, we aim to expand the knowledge of affective eHMIs in addition to in-vehicle designs and understand social factors that contribute to the user perceptions of automated vehicles.CCS CONCEPTS •Human-centered computing∼Human computer interaction (HCI)∼HCI theory, concepts and models} }
@inproceedings{10.1145/3641519.3657489, title = {Filter-Guided Diffusion for Controllable Image Generation}, booktitle = {ACM SIGGRAPH 2024 Conference Papers}, year = {2024}, isbn = {9798400705250}, doi = {10.1145/3641519.3657489}, url = {https://doi.org/10.1145/3641519.3657489}, author = {Gu, Zeqi and Yang, Ethan and Davis, Abe}, keywords = {Diffusion Models, Generative Artificial Intelligence, Image Synthesis, Style Transfer}, abstract = {Recent advances in diffusion-based generative models have shown incredible promise for zero shot image-to-image translation and editing. Most of these approaches work by combining or replacing network-specific features used in the generation of new images with those taken from the inversion of some guide image. Methods of this type are considered the current state-of-the-art in training-free approaches, but have some notable limitations: they tend to be costly in runtime and memory, and often depend on deterministic sampling that limits variation in generated results. We propose Filter-Guided Diffusion (FGD), an alternative approach that leverages fast filtering operations during the diffusion process to support finer control over the strength and frequencies of guidance and can work with non-deterministic samplers to produce greater variety. With its efficiency, FGD can be sampled over multiple seeds and hyperparameters in less time than a single run of other SOTA methods to produce superior results based on structural and semantic metrics. We conduct extensive quantitative and qualitative experiments to evaluate the performance of FGD in translation tasks and also demonstrate its potential in localized editing when used with masks.} }
@inproceedings{10.1145/3677389.3702596, title = {JCDL 2024 Workshop: Generative AI for Resource Discovery in Libraries}, booktitle = {Proceedings of the 24th ACM/IEEE Joint Conference on Digital Libraries}, year = {2025}, isbn = {9798400710933}, doi = {10.1145/3677389.3702596}, url = {https://doi.org/10.1145/3677389.3702596}, author = {Chen, Yinlin and Xie, Zhiwu and Yang, Le}, keywords = {generative AI, large language model, retrieval-augmented generation}, abstract = {This workshop delves into the transformative role of Generative AI technologies in digital libraries, emphasizing advancements in resource discovery and user engagement. Participants will explore how cutting-edge large language models such as GPT-4 and Llama are leveraged to deliver highly personalized resource recommendations and improve the efficiency and precision of information retrieval processes. Through showcases of capstone projects developed as part of the AI Incubator Program, hands-on sessions, and collaborative discussions, attendees will gain practical insights into deploying AI-driven solutions that streamline library operations and elevate user experience.} }
@inbook{10.1145/3676641.3716267, title = {PIM Is All You Need: A CXL-Enabled GPU-Free System for Large Language Model Inference}, booktitle = {Proceedings of the 30th ACM International Conference on Architectural Support for Programming Languages and Operating Systems, Volume 2}, pages = {862–881}, year = {2025}, isbn = {9798400710797}, url = {https://doi.org/10.1145/3676641.3716267}, author = {Gu, Yufeng and Khadem, Alireza and Umesh, Sumanth and Liang, Ning and Servot, Xavier and Mutlu, Onur and Iyer, Ravi and Das, Reetuparna}, abstract = {Large Language Model (LLM) inference uses an autoregressive manner to generate one token at a time, which exhibits notably lower operational intensity compared to earlier Machine Learning (ML) models such as encoder-only transformers and Convolutional Neural Networks. At the same time, LLMs possess large parameter sizes and use key-value caches to store context information. Modern LLMs support context windows with up to 1 million tokens to generate versatile text, audio, and video content. A large key-value cache unique to each prompt requires a large memory capacity, limiting the inference batch size. Both low operational intensity and limited batch size necessitate a high memory bandwidth. However, contemporary hardware systems for ML model deployment, such as GPUs and TPUs, are primarily optimized for compute throughput. This mismatch challenges the efficient deployment of advanced LLMs and makes users to pay for expensive compute resources that are poorly utilized for the memory-bound LLM inference tasks.We propose CENT, a CXL-ENabled GPU-Free sysTem for LLM inference, which harnesses CXL memory expansion capabilities to accommodate substantial LLM sizes, and utilizes near-bank processing units to deliver high memory bandwidth, eliminating the need for expensive GPUs. CENT exploits a scalable CXL network to support peer-to-peer and collective communication primitives across CXL devices. We implement various parallelism strategies to distribute LLMs across these devices. Compared to GPU baselines with maximum supported batch sizes and similar average power, CENT achieves 2.3x higher throughput and consumes 2.3x less energy. CENT reduces the Total Cost of Ownership (TCO), generating 5.2x more tokens per dollar than GPUs.} }
@inproceedings{10.1145/3689484.3690738, title = {Automated Generation of Code Contracts: Generative AI to the Rescue?}, booktitle = {Proceedings of the 23rd ACM SIGPLAN International Conference on Generative Programming: Concepts and Experiences}, pages = {1–14}, year = {2024}, isbn = {9798400712111}, doi = {10.1145/3689484.3690738}, url = {https://doi.org/10.1145/3689484.3690738}, author = {Greiner, Sandra and B\"uhlmann, Noah and Ohrndorf, Manuel and Tsigkanos, Christos and Nierstrasz, Oscar and Kehrer, Timo}, keywords = {Design by Contract, Generative AI, Large Language Models, Software Verification}, abstract = {Design by Contract represents an established, lightweight paradigm for engineering reliable and robust software systems by specifying verifiable expectations and obligations between software components. Due to its laborious nature, developers hardly adopt Design by Contract in practice. A plethora of research on (semi-)-automated inference to reduce the manual burden has not improved the adoption of so-called code contracts in practice. This paper examines the potential of Generative AI to automatically generate code contracts in terms of pre- and postconditions for any Java project without requiring any additional auxiliary artifact. To fine-tune two state-of-the-art Large Language Models, CodeT5 and CodeT5+, we derive a dataset of more than 14k Java methods comprising contracts in form of Java Modeling Language (JML) annotations, and train the models on the task of generating contracts. We examine the syntactic and semantic validity of the contracts generated for software projects not used in the fine-tuning and find that more than 95% of the generated contracts are syntactically correct and exhibit remarkably high completeness and semantic correctness. To this end, our fully automated method sets the stage for future research and eventual broader adoption of Design by Contract in software development practice.} }
@inproceedings{10.1145/3675888.3676033, title = {Generative AI for Elderly Well-being through the Computer as Social Actor Paradigm}, booktitle = {Proceedings of the 2024 Sixteenth International Conference on Contemporary Computing}, pages = {65–72}, year = {2024}, isbn = {9798400709722}, doi = {10.1145/3675888.3676033}, url = {https://doi.org/10.1145/3675888.3676033}, author = {Pramod, Dhanya and Patil, Kanchan Pranay}, abstract = {Artificial intelligence and machine learning (AI/ML) technologies like generative AI solutions are proliferating in the real-world healthcare sector. The purpose of this research is to investigate social norms, expectations, and standards of the elderly population for improving trust relationships while interacting with generative AI. The study is based on the CASA paradigm to gain a better understanding of the trust dynamics in human-computer communication to improve the adoption of GAI for elders' health and well-being. We validated the conceptual model with empirical data from 287 elderly users collected through an online and offline survey tool. Quantitative responses received were analysed using structural equation modeling. The study highlights how multimodal interaction, empathy, personalization, augmentation, bias stereotyping, and privacy and security affect the extent to which elderly consumers perceive GAI as trustworthy. Findings indicate that multimodal interaction, personalization, augmentation, and bias stereotyping significantly influenced the trust relationship between the elderly population and GAI. However, empathy privacy, and security were found to be insignificant in trust relationships. Further trust relationships significantly impacted GAI usage. The research provides strong theoretical and practical implications as all the stakeholders like healthcare professionals, patients/users, caregivers, and technology developers can be involved in building applications that cater to diverse needs and promote positive social interactions that can enhance GAI trust and usage.} }
@inproceedings{10.1145/3677892.3677941, title = {A Bibliometric Comparison of Chinese and International Research in the Field of Generative AI}, booktitle = {Proceedings of the 2024 International Conference on Digital Society and Artificial Intelligence}, pages = {303–310}, year = {2024}, isbn = {9798400709838}, doi = {10.1145/3677892.3677941}, url = {https://doi.org/10.1145/3677892.3677941}, author = {Hu, Changping and Yang, Jie}, abstract = {Rapid advancements in artificial intelligence incited a global fervor towards further AI research. Being the forefront of modern AI advancements, the current state and boundaries of generative AI research lended itself to further examination, especially the differences regarding topics and quality of research between International and Chinese academia. Through a bibliometric study examining the body of research contained within the China National Knowledge Infrastructure and Web of Science, the results suggested that Chinese academia have four areas of improvement: (1) determining the sample, (2) expanding the limits, (3) examining moral efficacy, and (4) researching foundational knowledge.} }
@inproceedings{10.1145/3736426.3736509, title = {Rethinking Unstructured Design Based on Knowledge Mapping Analysis of Artificial Intelligence Bodies for Domestic Art and Design in China}, booktitle = {Proceedings of the 2025 International Conference on Digital Management and Information Technology}, pages = {532–544}, year = {2025}, isbn = {9798400714238}, doi = {10.1145/3736426.3736509}, url = {https://doi.org/10.1145/3736426.3736509}, author = {Liu, Lijuan and Yuan, Yuan and Zhan, Yuhan and Zheng, Yi}, keywords = {art and design artifacts, knowledge graph analysis, unstructured design}, abstract = {This paper constructs a “technology-culture” double helix mechanism through a cross-dimensional study of 828 documents and 50 Chinese domestic art design AI tools from 2012 to 2025, combined with knowledge graph analysis and market research. We propose an unstructured design paradigm that combines algorithmic aesthetics with cultural gene encoding, aiming to break the paradox of algorithmic optimization and cultural adaptation in the era of artificial intelligence and reconstruct the methodology of human-machine collaborative design. The study shows that the paradigm can establish a cognitive framework for designers to collaborate with AI and help transform design thinking and construct new design relationships.} }
@inproceedings{10.1145/3613904.3642393, title = {Putting Things into Context: Generative AI-Enabled Context Personalization for Vocabulary Learning Improves Learning Motivation}, booktitle = {Proceedings of the 2024 CHI Conference on Human Factors in Computing Systems}, year = {2024}, isbn = {9798400703300}, doi = {10.1145/3613904.3642393}, url = {https://doi.org/10.1145/3613904.3642393}, author = {Leong, Joanne and Pataranutaporn, Pat and Danry, Valdemar and Perteneder, Florian and Mao, Yaoli and Maes, Pattie}, keywords = {education, generative artificial intelligence, learning, vocabulary}, abstract = {Fostering students’ interests in learning is considered to have many positive downstream effects. Large language models have opened up new horizons for generating content tuned to one’s interests, yet it is unclear in what ways and to what extent this customization could have positive effects on learning. To explore this novel dimension, we conducted a between-subjects online study (n=272) featuring different variations of a generative AI vocabulary learning app that enables users to personalize their learning examples. Participants were randomly assigned to control (sentence sourced from pre-existing text) or experimental conditions (generated sentence or short story based on users’ text input). While we did not observe a difference in learning performance between the conditions, the analysis revealed that generative AI-driven context personalization positively affected learning motivation. We discuss how these results relate to previous findings and underscore their significance for the emerging field of using generative AI for personalized&nbsp;learning.} }
@inproceedings{10.1145/3746469.3746605, title = {Reconstruction of Writing instruction with Generative AI in SAMR Framework: A Multimodal Empirical Study}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area Education Digitalization and Computer Science International Conference}, pages = {877–881}, year = {2025}, isbn = {9798400713811}, doi = {10.1145/3746469.3746605}, url = {https://doi.org/10.1145/3746469.3746605}, author = {Chang, Le}, keywords = {Generative AI, Multimodal empirical research, SAMR framework, Writing instruction}, abstract = {As a case of education digital transformation, traditional English writing pedagogy is difficult to truly encourage the interest of the students on writing and develop writing ability. A joint of generative AI and SAMR model offers innovation potential to writing teaching, but fewer studies exist on its implementation in vocational English writing teaching. This study enlisted students from two comparable classes of writing courses in a vocational school. The control group was instructed in generative AI supported writing instruction based on the SAMR model, while the control group was instructed in traditional writing instruction. Writing tests were given before and after teaching, and score data were collected and analyzed in terms of several aspects. The test results showed that the experimental group students' writing scores improved significantly with a dense distribution of scores and the number of students scoring high. The improvement in the control group was smaller, with a diffuse distribution and wide fluctuation. Independent sample t-tests also confirmed the effectiveness of the new teaching method. The study provides strong evidence for the reform of English writing education in vocational colleges.} }
@inproceedings{10.1109/ICSE-Companion66252.2025.00043, title = {Interactions with Generative AI: Wearables to Measure Developer Experience and Productivity Objectively}, booktitle = {Proceedings of the IEEE/ACM 47th International Conference on Software Engineering: Companion Proceedings}, pages = {148–150}, year = {2025}, isbn = {9798331536831}, doi = {10.1109/ICSE-Companion66252.2025.00043}, url = {https://doi.org/10.1109/ICSE-Companion66252.2025.00043}, author = {Brandebusemeyer, Charlotte}, keywords = {generative AI, developer experience, well-being, productivity, cognitive load, wearables, software engineering, real-life setting, psychophysiology}, abstract = {With recent advances in AI, many technology firms and developers are exploring whether Generative AI (GenAI) tools can help improve productivity, code quality and the overall developer experience. Current evaluations primarily depend on subjective questionnaire data or code quality metrics. A supplemental objective, continuous, real-time developer-centered measure is missing. Wearables with body sensors that measure the physiological activity of a person have the aforementioned benefits. With their help, impeding factors from GenAI tools on a developer's well-being and cognitive load may be detected. Implications for productivity may be deduced from these cognitive load measures. This research aims to use body sensor data to (1) assess the ability of wearables to measure developers' cognitive load in their everyday working context to then (2) evaluate developer experience, productivity and GenAI interactions in real-life work environments. A holistic mixed-method approach by combining subjective and objective measures in a developer-centered manner is taken.} }
@inbook{10.1145/3756423.3756491, title = {Design of intelligent campus management system driven by artificial intelligence}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing}, pages = {413–418}, year = {2025}, isbn = {9798400714351}, url = {https://doi.org/10.1145/3756423.3756491}, author = {Zhou, Qinglian and Peng, Jiazheng and Ma, Wenwei}, abstract = {Insufficient Informatization, low function utilization, performance limitation, data management and data security limit the application of the intelligent campus management system. To meet these challenges, this paper has developed a new system using artificial intelligence in which the overall design framework, hardware configuration, software programming,network topology, and implantation of AI functions are discussed.Following several testing rounds, the new system has developed immensely in its features,performance and security. The average response time under 5000 concurrent users is 2.168 seconds, serving 3180 requests per second and the CPU usage is not higher than 72.3% and memory stays under 58.7%. In a 24-hour stress test, the system achieved an availability rate of up to 99.97%, and performance was 4.7 times faster for response times and 6.2 times for processing compared to traditional systems. The facial recognition module achieves accuracy rate of 99.12% in uncomplicated condition, and keeps the accuracy rate associate with the one in low light conditions at 97.34%, exhibiting rather high robustness for pose variation and partially blocked faces, and the robustness has increased by 18.7%.} }
@inproceedings{10.1145/3607827.3616847, title = {Large Generative Models Meet Multimodal Video Intelligence}, booktitle = {Proceedings of the 1st Workshop on Large Generative Models Meet Multimodal Applications}, pages = {1}, year = {2023}, isbn = {9798400702839}, doi = {10.1145/3607827.3616847}, url = {https://doi.org/10.1145/3607827.3616847}, author = {Shou, Mike Zheng}, keywords = {diffusion model, large generative models, multimodal, vision-language}, abstract = {In this talk, I would like to share my recent research around multimodal video intelligence in the era of large generative models. I will first talk about video-language pretraining techniques (All-in-one, EgoVLP) that use one single model to power various understanding tasks ranging from retrieval to QA. Then I will introduce challenges and our efforts of adapting these large pretrained models to AI Assistant, such a real-world application (AssistQ, AssistGPT). Finally I will delve into the reverse problem i.e. given open-world textual description, how to generate videos with diffusion models (Tune-A-Video, Show-1).} }
@inproceedings{10.1145/3746709.3746884, title = {Application research of evolutionary generative model for financial information sentiment retrieval based on interactive genetic algorithm}, booktitle = {Proceedings of the 2025 6th International Conference on Computer Information and Big Data Applications}, pages = {1031–1036}, year = {2025}, isbn = {9798400713163}, doi = {10.1145/3746709.3746884}, url = {https://doi.org/10.1145/3746709.3746884}, author = {Li, Hui and Peng, Juan and Li, Xianhao and Yang, Xingrui and Wang, Pan and Fan, Xiaohu}, keywords = {Generative patterns are evolutionary, Interactive genetic algorithm, Generative artificial intelligence, Financial information, Emotion retrieval}, abstract = {Financial texts contain rich emotional information, which is of great significance for capturing emotional fluctuations in financial markets, assisting investors in decision-making and implementing financial risk management. However, sentiment information of financial texts requires a lot of domain expert knowledge, and manual retrieval is expensive. In this paper, we propose the application of an evolutionary generative model for sentiment retrieval of financial information based on interactive genetic algorithms from the perspective of user implicit modeling. The uncertain preference estimation of evolutionary individuals is realized. The user preference model is adjusted according to the uncertainty evaluation, and the user preference is dynamically tracked to obtain the fitness value of the evolutionary individual for evolutionary optimization. The experimental results show that the proposed method can implicitly perceive user preferences, reduce user explicit evaluation, reduce user fatigue, and improve the search performance of the algorithm.} }
@inproceedings{10.1145/3717383.3719313, title = {Workshop Report on Generative AI-based Software Engineering (GenAI4SE 2nd Edition)}, booktitle = {Proceedings of the 18th Innovations in Software Engineering Conference}, year = {2025}, isbn = {9798400714245}, doi = {10.1145/3717383.3719313}, url = {https://doi.org/10.1145/3717383.3719313}, author = {Medicherla, Raveendra Kumar and Rajbhoj, Asha and Patwardhan, Manasi and Sanagavarapu, Lalit Mohan and Sharma, Vibhu Saujanya and Naik, Ravindra and Aggarwal, Pooja}, keywords = {Machine learning, Knowledge representation, Agentic AI, Debugging, Programming language translation}, abstract = {We have organized the Generative AI based Software Engineering workshop, co-located with the 18th Innovations in Software Engineering Conference (ISEC) at Kurukshetra, India on 20th Feb. 2025. We describe the objectives and brief contents of the workshop and are hoping that the execution of the planned contents during the workshop will meet the set objectives.} }
@inproceedings{10.1145/3745238.3745525, title = {Can generative AI features increase users' continued use intention?: The mediating role of user perceived value and user satisfaction}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1838–1845}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745525}, url = {https://doi.org/10.1145/3745238.3745525}, author = {An, Hui and Guo, Zhenpeng and Shi, Yunxia}, keywords = {Generative AI features, User Continued use intention, User Perceived value, User satisfaction}, abstract = {The emergence of generative AI has become a hot topic in academia and industry. Generative AI is also gradually becoming an interaction mode between enterprises and customers, improving customer experience and influencing their continued use intention. However, there is still uncertainty as to whether users are willing to use generative AI-driven products and services for a long time. Previous studies have mostly been based on traditional industries. How to enhance users’ continued use intention from the perspective of AI has become a widely concerned issue in academia and industry.Based on the Technology Acceptance Model (TAM) and Expectation Confirmation Theory (ECT), this study explores the impact of generative AI features on users’ continued use intention by improving the two paths of user perceived value and user satisfaction. 306 valid questionnaires were used, and SPSS 27.0 and SEM were used for data analysis and hypothesis testing. The results show that (1) generative AI features have a significant positive impact on users’ continued use intention; (2) user perceived value and user satisfaction play a mediating role in the relationship between generative AI features and users’ continued use intention. This finding improves users’ continued use intention of generative AI features, which is of great significance for understanding the marketing layout of generative AI for enterprises.} }
@inproceedings{10.1145/3664647.3689003, title = {A Solution to ACMMM 2024 on Artificial Intelligence Generated Image Detection}, booktitle = {Proceedings of the 32nd ACM International Conference on Multimedia}, pages = {11475–11477}, year = {2024}, isbn = {9798400706868}, doi = {10.1145/3664647.3689003}, url = {https://doi.org/10.1145/3664647.3689003}, author = {Li, ShiHang and Wu, Haishan and Wang, Biao}, keywords = {ai generated discrimination, data balancing, diffusion model, generative model, model ensemble}, abstract = {In this rapidly evolving era of AI technology, image generation models are pushing the boundaries of art, design, and even information dissemination at an unprecedented pace. However, alongside this technological advancement comes the growing challenge of combating misinformation and image manipulation. One major difficulty in determining AI-generated images lies in dealing with various generation models. In this report, our core solution can be summarized as ''balancing data sources is key to enhancing the generalization capability of the detection model.'' In the competition, we achieved a score of 0.9558 on the B leaderboard, ultimately securing the second place.} }
@inproceedings{10.1145/3745238.3745514, title = {AIGC empowers the teaching of artificial intelligence data processing courses}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1765–1770}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745514}, url = {https://doi.org/10.1145/3745238.3745514}, author = {Yang, Yingyao}, keywords = {AIGC technology, Artificial intelligence, Course teaching, Data processing}, abstract = {With the rapid development of Artificial Intelligence technology, AIGC (Artificial Intelligence Generated Content) technology is increasingly widely used in the field of education. Especially in the course of artificial intelligence data processing, AIGC technology can not only provide rich and diverse teaching content and personalized learning plan, but also help students better understand and master data processing skills through intelligent tutoring and real-time feedback mechanism. This paper aims to explore the application of AIGC technology in the teaching of artificial intelligence data processing course, combined with the relevant knowledge of computer, analyze how to improve the teaching efficiency and learning experience of AIGC technology, as well as the challenges and future development direction.} }
@inbook{10.1145/3715668.3736365, title = {Towards a Working Definition of Designing Generative User Interfaces}, booktitle = {Companion Publication of the 2025 ACM Designing Interactive Systems Conference}, pages = {489–495}, year = {2025}, isbn = {9798400714863}, url = {https://doi.org/10.1145/3715668.3736365}, author = {Lee, Kyungho}, abstract = {Generative UI is transforming interface design by facilitating AI-driven collaborative workflows between designers and computational systems. This study establishes a working definition of Generative UI through a multi-method qualitative approach, integrating insights from a systematic literature review of 127 publications, expert interviews with 18 participants, and analyses of 12 case studies. Our findings identify five core themes that position Generative UI as an iterative and co-creative process. We highlight emerging design models, including hybrid creation, curation-based workflows, and AI-assisted refinement strategies. Additionally, we examine ethical challenges, evaluation criteria, and interaction models that shape the field. By proposing a conceptual foundation, this study advances both theoretical discourse and practical implementation, guiding future HCI research toward responsible and effective generative UI design practices.} }
@inproceedings{10.1145/3613905.3643979, title = {Implications of Regulations on the Use of AI and Generative AI for Human-Centered Responsible Artificial Intelligence}, booktitle = {Extended Abstracts of the CHI Conference on Human Factors in Computing Systems}, year = {2024}, isbn = {9798400703317}, doi = {10.1145/3613905.3643979}, url = {https://doi.org/10.1145/3613905.3643979}, author = {Constantinides, Marios and Tahaei, Mohammad and Quercia, Daniele and Stumpf, Simone and Madaio, Michael and Kennedy, Sean and Wilcox, Lauren and Vitak, Jessica and Cramer, Henriette and Bogucka, Edyta Paulina and Baeza-Yates, Ricardo and Luger, Ewa and Holbrook, Jess and Muller, Michael and Blumenfeld, Ilana Golbin and Pistilli, Giada}, keywords = {AI ethics, human-centered AI, large language models, regulations, responsible AI}, abstract = {With the upcoming AI regulations (e.g., EU AI Act) and rapid advancements in generative AI, new challenges emerge in the area of Human-Centered Responsible Artificial Intelligence (HCR-AI). As AI becomes more ubiquitous, questions around decision-making authority, human oversight, accountability, sustainability, and the ethical and legal responsibilities of AI and their creators become paramount. Addressing these questions requires a collaborative approach. By involving stakeholders from various disciplines in the 2nd edition of the HCR-AI Special Interest Group (SIG) at CHI 2024, we aim to discuss the implications of regulations in HCI research, develop new theories, evaluation frameworks, and methods to navigate the complex nature of AI ethics, steering AI development in a direction that is beneficial and sustainable for all of humanity.} }
@inproceedings{10.1145/3695080.3695149, title = {Risk analysis and control of large artificial intelligence models}, booktitle = {Proceedings of the 2024 International Conference on Cloud Computing and Big Data}, pages = {402–408}, year = {2024}, isbn = {9798400710223}, doi = {10.1145/3695080.3695149}, url = {https://doi.org/10.1145/3695080.3695149}, author = {Li, Shilong and Zhang, Li and Gu, Mengchen and Chan, Jian and Wu, Qi}, abstract = {With the rapid development of artificial intelligence(AI) technology, large AI models have demonstrated immense potential and value in numerous fields. However, the security risks associated with large AI models have also increasingly garnered widespread attention. This paper first outlines the development status and characteristics of large AI models and provides a detailed classification and discussion of the security risks. Next, the paper analyzes the causes of these risks, delving into technical, talent, and policy perspectives. Lastly, the paper proposes countermeasures for the secure development of large AI models, including improving technical regulation, enhancing policy regulations, strengthening talent cultivation, and reinforcing international cooperation, with the aim of providing a theoretical reference for the healthy development of large AI models.} }
@inproceedings{10.1145/3762329.3762342, title = {Feasibility Research on Application of Artificial Intelligence Technology in Enterprises Based on Deep Learning}, booktitle = {Proceedings of the 2nd International Conference on Artificial Intelligence of Things and Computing}, pages = {65–70}, year = {2025}, isbn = {9798400718625}, doi = {10.1145/3762329.3762342}, url = {https://doi.org/10.1145/3762329.3762342}, author = {Wang, Yi and Zhang, Zhenyu}, keywords = {Artificial Intelligence Technology, Deep Learning, Feasibility Research, LSTM-Transformer}, abstract = {The industrial application of artificial intelligence technology is profoundly changing the competitive pattern of enterprises. However, small and medium-sized enterprises generally face bottlenecks such as high investment threshold, lack of talents and difficult scene adaptation in the process of technology landing. This study focuses on the feasibility discrimination problem of enterprise application of artificial intelligence technology, and proposes a deep learning framework based on LSTM-Transformer hybrid model. By integrating the three types of time series characteristics of enterprise basic information, business dynamics and technology application data, a multi-dimensional evaluation system is constructed. The model adopts layered architecture: LSTM layer captures short-term business fluctuations, Transformer layer mines long-term correlation between technical input and business effectiveness, and finally realizes two-classification of "feasible" and "temporarily infeasible" through Softmax. Based on the six-year data of 2006 enterprises, the mixed model accuracy reached 89.06% (±0.31%) and F1 score was 87.88% (±0.36%), which was significantly better than the single model and CNN-LSTM baseline. Confusion matrix shows that 93.7% of high recall guarantees the identification of potential enterprises, and 74.4% of specificity reveals negative class discrimination optimization space. This study provides a data-driven decision-making tool for SMEs to avoid technology investment risks.} }
@inproceedings{10.1145/3748825.3748926, title = {Research on Military Artificial Intelligence Risk Governance}, booktitle = {Proceedings of the 2025 2nd International Conference on Digital Society and Artificial Intelligence}, pages = {657–660}, year = {2025}, isbn = {9798400714337}, doi = {10.1145/3748825.3748926}, url = {https://doi.org/10.1145/3748825.3748926}, author = {Wang, Xuefeng and Yang, Jinling and Jia, Mingliang}, keywords = {Keywords—military artificial intelligence, international arms control, risk governance, strategic stability}, abstract = {The military application of artificial intelligence (AI) technology has become a key strategic competitive advantage, but the risks it poses, such as technological failure, ethical disorder, strategic imbalance, and legal gaps, pose a serious challenge to the global security governance system. Based on risk society theory, This paper systematically analyses the four-dimensional risk matrix of AI military applications and constructs a “technology-ethics-strategy-law” collaborative governance framework. The study proposes that “developmental governance” of AI military applications should be achieved through algorithmic auditing, international consensus on ethical guidelines, innovation in strategic stability mechanisms, and embedded legal regulation. The research conclusions provide theoretical support and policy guidance for improving the global AI arms control regime and maintaining strategic stability.} }
@proceedings{10.1145/3748825, title = {DSAI '25: Proceedings of the 2025 2nd International Conference on Digital Society and Artificial Intelligence}, year = {2025}, isbn = {9798400714337} }
@inproceedings{10.1145/3719384.3719439, title = {Challenges and Limitations of Using Artificial Intelligence Generated Content (AIGC) with ChatGPT in Programming Curriculum: A Systematic Literature Review}, booktitle = {Proceedings of the 2024 7th Artificial Intelligence and Cloud Computing Conference}, pages = {372–378}, year = {2025}, isbn = {9798400717925}, doi = {10.1145/3719384.3719439}, url = {https://doi.org/10.1145/3719384.3719439}, author = {Chang, Chi In and Choi, Wan Chong and Choi, Iek Chong}, keywords = {Artificial intelligence generated content, Challenges of AIGC, ChatGPT, Large language models, Programming curriculum, Systematic literature review}, abstract = {This systematic literature review examined the challenges and limitations of integrating Artificial Intelligence Generated Content (AIGC) tools into programming curricula. Following Kitchenham's framework, a comprehensive search was conducted across ACM Digital Library and Web of Science databases to select 22 relevant peer-reviewed papers. Key issues identified included increased plagiarism risk due to AI-generated unique code, accuracy and reliability concerns of AI outputs, privacy and data security risks, curriculum changes to incorporate prompt engineering and critical thinking, and challenges in traditional assessment methods. Additionally, the review highlighted the potential over-reliance on AI tools, which could hinder the development of fundamental programming skills. Implementation challenges and inadequate training for educators further complicate the integration process. Addressing these challenges is crucial for leveraging the benefits of AIGC tools while maintaining academic integrity and enhancing learning outcomes in programming education. Future research should focus on developing strategies to mitigate these issues and ensure the responsible use of AI in education.} }
@article{10.1145/3719209, title = {Knowledge Management in a World of Generative AI: Impact and Implications}, journal = {ACM Trans. Manage. Inf. Syst.}, volume = {16}, year = {2025}, issn = {2158-656X}, doi = {10.1145/3719209}, url = {https://doi.org/10.1145/3719209}, author = {Storey, Veda Catherine}, keywords = {Knowledge management, generative AI (GenAI), novice knowledge workers, expert knowledge workers, Framework for Knowledge Management in the GenAI Era, risk assessment, organizational knowledge, Knowledge shifting}, abstract = {The management of organizational and worker knowledge is a critical part of any organization. Knowledge management recognizes the explicit knowledge that workers contribute to an organization, as well as the organizational knowledge and skills acquired over time. The potential impact of generative AI (GenAI) raises many issues and opportunities related to effective knowledge management as organizations explore the impact and implications of its adoption. This research explores issues related to an increasing world of GenAI and proposes a Framework for Knowledge Management in the GenAI Era. Knowledge management workers, who are novices, might be more inclined to use generative AI for performing their tasks. Expert knowledge workers might focus on quality evaluation and quality assessment. Adoption of GenAI may help with the progression from novice to expert, while freeing up time and resources for other work. Challenges include the potential loss of critical organizational knowledge, definition of job-related requirements, expectations, and tasks, as well as over-reliance and loss of tacit or unarticulated knowledge. These challenges must be balanced against available access to large amounts of general and domain-specific knowledge from GenAI to capture the value realized from its adoption.} }
@inproceedings{10.1145/3745676.3745719, title = {Research on the Network Characteristics and Cooperation Models of Artificial Intelligence Group Standards in China—Based on the Analysis of the Artificial Intelligence Industry}, booktitle = {Proceedings of the 2025 2nd International Conference on Innovation Management and Information System}, pages = {296–302}, year = {2025}, isbn = {9798400715150}, doi = {10.1145/3745676.3745719}, url = {https://doi.org/10.1145/3745676.3745719}, author = {Shi, Boli and Nie, Zhaoying and Chen, Jing and Wang, Zhen}, keywords = {Artificial Intelligence, Cooperation Model, Group Standards, Network Characteristics, Social Network Analysis}, abstract = {This paper takes the group standards of China's artificial intelligence-related industries as the research object and analyzes the characteristics of participating drafting units and group standard organizations in the group standard cooperation network based on the structural characteristics such as centrality and network density obtained from social network analysis. The study finds that although the development time of group standards in China's artificial intelligence-related industries is short, the development is rapid, and the cooperation network has basically taken shape. The central positions of some group standard organizations and participating drafting units have been initially formed. However, it can also be seen that some group standard organizations, despite having a close relationship with participating drafting units, have not yet established connections with the overall network, indicating that the overall cooperation network is still loose, and only some group standard organizations play relatively key roles in constructing the network for the formulation of group standards in the artificial intelligence-related industries. In the process of promoting the maturity of the cooperation network of group standards in artificial intelligence-related industries, there are still many challenges and room for optimization. On the one hand, it is urgent to take effective measures to strengthen the connections among all entities within the network, reduce the looseness of the network, and improve the overall collaborative efficiency, such as establishing an information sharing platform. On the other hand, group standard organizations and participating drafting units in central positions should give full play to their leading and exemplary roles, and drive the common development of other entities within the network through means such as technical assistance and experience sharing. In addition, the government and industry associations should also play an active role by introducing targeted policies and regulations to guide the formulation and implementation of group standards for artificial intelligence, urging all parties involved to strengthen cooperation and build a more complete standard system. Looking ahead, with the continuous deepening of collaborative governance by all parties, the cooperation network of group standards in the artificial intelligence industry will continue to be optimized and upgraded. It will not only promote the high-quality development of China's artificial intelligence industry but also demonstrate China's wisdom in the field of global artificial intelligence standard setting and enhance China's international discourse power and influence in this field.} }
@inproceedings{10.1145/3708394.3708439, title = {How Artificial Intelligence has influenced Students' Learning: A Survey Exploring Attitudes towards Tool Usage}, booktitle = {Proceeding of the 2024 International Conference on Artificial Intelligence and Future Education}, pages = {260–267}, year = {2025}, isbn = {9798400710650}, doi = {10.1145/3708394.3708439}, url = {https://doi.org/10.1145/3708394.3708439}, author = {Xie, Yecheng and Lin, Weiyu and Yu, Xinyi}, keywords = {Artificial intelligence, comprehensive evaluation, education, students’ attitudes}, abstract = {Amid the convergence of traditional education and the rise of Artificial Intelligence (AI), the education sector faces a significant challenge. To comprehensively assess how AI has influenced students' learning, we apply a targeted questionnaire, gathering information about students' basic information, situation about internet usage, learning needs, and attitudes towards AI tools. Based on 4,606 valid responses, we construct a hierarchical evaluation model, and make correlation analysis accordingly. The result has shown that the influence of AI is general, particularly for students who are in lower-grade or majored in science and engineering fields, and the problem of lack of experience and professionalism is common. Objective conditions and motivation have played a crucial role. To make sensible use of AI in study, the educators may consider giving tips of transparent policy, offering personalized instructions, bettering the access of intelligent tools, and motivating students' self-directed learning.} }
@inproceedings{10.1145/3711670.3764633, title = {Artificial Intelligence and Rhetoric: Investigating Contextual Influence on Technological Design}, booktitle = {Proceedings of the 43rd ACM International Conference on Design of Communication}, pages = {158–162}, year = {2025}, isbn = {9798400714443}, doi = {10.1145/3711670.3764633}, url = {https://doi.org/10.1145/3711670.3764633}, author = {Tetu, Imari Cheyne and Ristich, Mike and McArdle, Casey and Potts, Liza}, keywords = {Algorithms, Artificial intelligence, Human-centered design, Rhetorical theory, User experience}, abstract = {This paper builds upon Lynette Hunter's foundational argument that artificial intelligence (AI) lacks rhetorical “stance” by emphasizing the essential role of context in both human reasoning and the development of technological systems. Drawing on rhetorical theory and cognitive science, particularly the interactionist model of reasoning by Dan Sperber and Hugo Mercier, we challenge reductive, context-free understandings of rationality embedded in AI and platform-driven algorithmic ecologies. We argue that contemporary digital technologies, especially algorithmic systems underpinning platforms, operate through a rhetoric that flattens nuance, shapes user behavior, and undermines democratic participation. By invoking Philip Wander's concept of “rhetorical contextualization,” the paper highlights how rhetorical inquiry can uncover what is omitted, censored, or rendered invisible in technological design. We advocate for a reimagined Semantic Web, one guided by critical, ethical, and context-aware frameworks, to realize AI's potential for human flourishing. Ultimately, we position rhetorical theory not only as a tool of critique, but as a blueprint for designing more just and responsive digital futures.} }
@inproceedings{10.1145/3748825.3748870, title = {Optimization Design of Virtual Product Display System Based on Artificial Intelligence Algorithm}, booktitle = {Proceedings of the 2025 2nd International Conference on Digital Society and Artificial Intelligence}, pages = {278–282}, year = {2025}, isbn = {9798400714337}, doi = {10.1145/3748825.3748870}, url = {https://doi.org/10.1145/3748825.3748870}, author = {Fu, Hanjiao}, keywords = {Artificial intelligence algorithms, image enhancement, image processing, optimization design, product display design, virtual reality, visual communication}, abstract = {In response to the challenges faced by general virtual product display systems when presenting artworks with high display effect requirements, such as the inability to fully showcase detailed features and poor display performance, this paper investigates and designs a virtual product display system based on visual communication and artificial intelligence algorithms. Based on the existing general display system, this systemintegrates image enhancement strategies from image processing technology and specifically employs advanced algorithms from the field of artificial intelligence, particularly Generative Adversarial Networks (GANs), to enhance image resolution while maintaining or improving detail representation. By introducing artificial intelligence algorithms, the system is able to handle image details, color adjustment and user interaction more intelligently, further enhancing the realism of the display effect and user experience. The experimental results show that the optimization system has a significant improvement in the overall display effect, detail performance and sense of reality, especially for the degree of product detail performance, the display effect is improved by 13.9%, reflecting the system's strong ability in detail display. At the same time, the application of artificial intelligence algorithms also provides the system with more intelligent interaction and recommendation functions, so that it can better meet the needs of users, and provide a more efficient and intelligent display solution for virtual products such as artworks that require higher display effects.} }
@inproceedings{10.1145/3745238.3745327, title = {Assessing AI Progress Through Entropy A Multidimensional Study on China's Provincial-Level Competitiveness in Artificial Intelligence}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {559–563}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745327}, url = {https://doi.org/10.1145/3745238.3745327}, author = {Hu, Bingli and Chen, Jiexiao and Chen, Zhe}, keywords = {Development level, Entropy method, Indicator system, Next Generation Artificial Intelligence}, abstract = {With the advent of the fourth industrial revolution, the rapid development and application of artificial intelligence technology has become the core driving force for global economic and industrial upgrading. The Chinese government has introduced a number of strategic policies to promote the development of new generation artificial intelligence and has made significant progress, but there are still large differences in the research and development, application and industrialization of AI technology in different regions. To this end, this research sorted out the definition of new generation artificial intelligence and established a comprehensive evaluation index system including four dimensions: development scale, economic benefits, innovation input and scientific and technological output. Based on panel data from 30 provinces, municipalities and autonomous regions, the entropy method was used to analyze the development level of new generation artificial intelligence in various regions from 2017 to 2021. The results show that the overall development level of China's new generation of artificial intelligence shows a steady growth trend. The development of AI in the eastern region leads the country, while the development level of AI in the central, western and northeastern regions is relatively low, and the gap is widening year by year.} }
@inproceedings{10.1145/3726010.3726098, title = {Overview of the Application of Artificial Intelligence in Music Creation}, booktitle = {Proceedings of the 2024 International Conference on Artificial Intelligence, Digital Media Technology and Interaction Design}, pages = {559–565}, year = {2025}, isbn = {9798400712845}, doi = {10.1145/3726010.3726098}, url = {https://doi.org/10.1145/3726010.3726098}, author = {Si, Yiming and Li, Xiaohui}, keywords = {Artificial Intelligence, Artistic creation, GAN, Music generation, RNN, Transformer models}, abstract = {Music creation is the task of generating the song or the music. This creation task is very valuable in media and entertainment field. Artificial Intelligence (AI) plays a vital role in the field of music development. AI-based music creation field combines cutting-edge technology with creativity. This paper presents the overview of AI in music creation. This research work discusses the application of AI algorithm such as Generative Adversarial Networks (GANs), Recurrent Neural Networks (RNNs), and Transformer models in music creation field. These algorithms are useful for music composition, generation, and analysis. Case studies were discussed in AI-enabled platforms to illustrate the importance of AI in this field. This paper presents a comparative analysis of several research works for music creation. This explores the strength, weaknesses and the practical applications. Further, this work highlights the difficulties and prospects in the field of AI-enabled music development.} }
@inproceedings{10.1145/3748825.3748856, title = {Optimizing Artificial Intelligence Algorithms for Enhanced Teaching and Digital Preservation of Folk Music}, booktitle = {Proceedings of the 2025 2nd International Conference on Digital Society and Artificial Intelligence}, pages = {185–190}, year = {2025}, isbn = {9798400714337}, doi = {10.1145/3748825.3748856}, url = {https://doi.org/10.1145/3748825.3748856}, author = {Zhang, Jie}, keywords = {Artificial Intelligence (AI), Artificial Intelligence Algorithms, Cultural Heritage, Cultural Sensitivity, Digital Archiving, Folk Music Preservation, Music Analysis, Music Education, Optimizing, Personalized Learning, Technology in Music, Traditional Music Education}, abstract = {This paper focuses on the implementation of optimization of artificial intelligence (AI) techniques used in enhancing teaching and digital preservation of folk music which is a part of intangible cultural heritage. The peculiarities of folk music, like the regional variety and oral transmission, pose a challenge to the traditional ways of education and safeguarding. Taking advantage of state-of-the-art computer technologies, such as deep learning and machine learning models, this study creates customized computational solutions to overcome the problem of data scarcity and cultural authenticity issue. Adopting a mixed-methods approach involving algorithm-building, data-augmentation as well as qualitative analysis by educators and learners, this research tests the effectiveness of AI in enhancing personalized learning experience and cultural fidelity. Findings show improved student engagement as well as algorithmic performance with the presence of persistent challenges including cultural bias and lack of enough training data. The paper ends with future research directions; highlighting the combination of emerging techniques in AI and immersive technologies to develop sustainable and culturally sensitive solutions to folk music education and preservation.} }
@inproceedings{10.1145/3644713.3644827, title = {The Relationship of the Global Index of Artificial Intelligence and the Level of Employment: A Cluster Approach in Assessing Cross-Country Differences}, booktitle = {Proceedings of the 7th International Conference on Future Networks and Distributed Systems}, pages = {682–688}, year = {2024}, isbn = {9798400709036}, doi = {10.1145/3644713.3644827}, url = {https://doi.org/10.1145/3644713.3644827}, author = {Zarova, Elena and Abdurakhmanova, Gulnora and Tursunov, Bobir}, abstract = {In this paper has been investigated the relationship of the global index of artificial intelligence and the level of employment: a cluster approach in assessing cross-country differences. In the calculations of the index under con-sideration, four countries of the post-Soviet period were presented, which were divided according to the results: three of them (Latvia, Russia and Estonia) were included in the second cluster, which has an intermediate level of the Global AI Index, and one country (Armenia) was included in the third cluster, value index for which it turned out to be minimal. For the most representative (second) cluster of countries, it was found that the strength of the connection between the assessment of artificial intelligence and the level of employment is weak; the ratio of these indicators is very different across countries.} }
@inproceedings{10.1145/3711403.3711419, title = {Risk Resolution of Wisdom Education in the Era of Digital Intelligence}, booktitle = {Proceedings of the 2024 7th International Conference on Educational Technology Management}, pages = {114–120}, year = {2025}, isbn = {9798400717468}, doi = {10.1145/3711403.3711419}, url = {https://doi.org/10.1145/3711403.3711419}, author = {Peng, Shaodong}, keywords = {Alienation of nature, Ethical anomie, Lack of soul, Pathological teaching, Risk resolution, The era of digital intelligence, wisdom education}, abstract = {Wisdom education in the era of digital intelligence emphasizes the use of intelligent technology to empower education. It can integrate data wisdom, educational wisdom, and cultural wisdom to achieve the precision, creation, and optimization of education, and cultivate learners' wisdom literacy. Due to the potential problems in algorithm, calculation data, and computing power and other aspects of problems in intelligent technology, as well as due to human-related factors such as emotional barriers and management's losing control, wisdom education in the era of digital intelligence faces many risks. This paper applies the methods of philosophical speculation, ethical analysis, emotional analysis, and teaching pathology analysis to study the four major risks in them. (alienation of educational nature, human-machine ethical anomie, lack of artificial emotion, and pathological teaching), and intending to find the resolutions of the corresponding risks in wisdom education. The supposed resolutions are empowering the human-like nature of educational artificial intelligence and combining it with moral education to obtain a complete educaitonal nature of wisdom education; strengthening the governance of human-machine ethics from the source of risk (participants) through the utilization of intelligent educational ethical norms; combining human emotions with artificial emotions to establish a soul that is inclined to goodness of intelligent education; inhibiting pathogenic factors and enhancing disease resistance factors treat teaching diseases and prevent effectively through various therapies.} }
@inproceedings{10.1145/3600211.3604746, title = {Governing Silicon Valley and Shenzhen: Assessing a New Era of Artificial Intelligence Governance in the US and China}, booktitle = {Proceedings of the 2023 AAAI/ACM Conference on AI, Ethics, and Society}, pages = {947–949}, year = {2023}, isbn = {9798400702310}, doi = {10.1145/3600211.3604746}, url = {https://doi.org/10.1145/3600211.3604746}, author = {Hine, Emmie}, keywords = {China, United States, artificial intelligence, geopolitics, governance}, abstract = {The United States and China are both striving to be the world leader in AI, with conflicting visions. However, the intensifying“clash of civilizations” narrative ignores factors integral to each country’s AI strategy. This project uses a philosophy-grounded framework and natural language processing (NLP) methods to analyze what policy differences exist and why they exist. It examines new developments and argues that while obstacles to cooperation still exist, ethical convergences offer hope.} }
@inproceedings{10.1145/3745238.3745412, title = {Development of organic pressure-sensitive adhesive under the background of artificial intelligence}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1111–1118}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745412}, url = {https://doi.org/10.1145/3745238.3745412}, author = {Shen, Qili and Liu, Guocong and Hou, Bolin and Qiang, Na and Yang, Xiaolong and Xu, Runfeng and Liu, Zhu and Fang, Yanxiong and Huang, Xin and Lu, Zhe}, keywords = {Artificial Intelligence Silicone pressure-sensitive adhesive, Silicone resin, Thiol-ene click chemistry, UV-curing}, abstract = {Silicone pressure-sensitive adhesives (SiPSAs) have demonstrated broad application potential in electronics and other fields due to their exceptional properties, such as temperature resistance, weather resistance, and chemical corrosion resistance. However, traditional SiPSAs are predominantly thermally cured solvent-based products, which not only exhibit slow curing rates but also release harmful solvents like toluene during application, causing environmental pollution. To address this issue, this study Under the background of artificial intelligence. synthesized thiol-functionalized MTQ silicone resins (MTQ-SH) with various M/(T+Q) ratios using γ-mercaptopropyltrimethoxysilane, tetraethyl orthosilicate, and hexamethyldisiloxane as raw materials. These resins were then photo-crosslinked with vinyl-terminated silicone oil to prepare UV-curable silicone PSAs (UV-SiPSAs). The results revealed that the optimal precursor ratio for UV-SiPSA preparation was M/T/Q = 1.00/0.21/1.58. When the photoinitiator TPO-L was added at 1 wt.% and the molar ratio of thiol groups to vinyl end groups was 1.2:1, the resulting UV-SiPSA exhibited an initial adhesion strength capable of holding a 22# steel ball and a holding time of 230 minutes at 25℃. This UV-curable PSA shows promising potential for replacing traditional thermally cured silicone PSAs in the protective film industry.} }
@inproceedings{10.1145/3657054.3657125, title = {Comparative Analysis of Generative AI Risks in the Public Sector}, booktitle = {Proceedings of the 25th Annual International Conference on Digital Government Research}, pages = {610–617}, year = {2024}, isbn = {9798400709883}, doi = {10.1145/3657054.3657125}, url = {https://doi.org/10.1145/3657054.3657125}, author = {Beltran, Marco Antonio and Ruiz Mondragon, Marina Ivette and Han, Seung Hun}, keywords = {GenAI, GenAI Policy and Regulation, Public sector, Risks}, abstract = {The landscape of artificial intelligence (AI) has experienced a monumental shift with the emerging of Generative AI (GenAI), which has demonstrated to be a transformative tool across diverse sectors. GenAI outputs can span various digital formats, including text, images, videos, and audio, generating particular interest in the public sector. The growing interest of governments in integrating GenAI technologies in public sector operations is marked by the creation of emerging governance instruments and the formulation of soft laws, like standards, principles, and guidelines. This study aims to delve into the intricacies and potential risks associated with the deployment of GenAI within government. Through a qualitative content analysis, the research meticulously examines GenAI usage guidelines issued by Australia, Canada, New Zealand, the United Kingdom, and South Korea. The objective is to discern the risks acknowledged by these countries' soft laws and compare them with the risks identified by scholars in the field. The performed comparative analysis across countries suggest that the use of GenAI in the public sector raises common risks such as information leakage, data privacy, security, and concerns over public trust. By elucidating the varied risk perceptions across different national contexts, this study provides theoretical and practical implications related to the risks of GenAI within the public sector. Moreover, it sets a foundation for future research and policy development, ensuring that generative AI is used as a force for good in public governance.} }
@inproceedings{10.1145/3701716.3715857, title = {Artificial Intelligence for Complex Network: Potential, Methodology and Application}, booktitle = {Companion Proceedings of the ACM on Web Conference 2025}, pages = {5–8}, year = {2025}, isbn = {9798400713316}, doi = {10.1145/3701716.3715857}, url = {https://doi.org/10.1145/3701716.3715857}, author = {Ding, Jingtao and Zheng, Yu and Wang, Huandong and Cannistraci, Carlo Vittorio and Gao, Jianxi and Li, Yong and Shi, Chuan}, keywords = {artificial intelligence, network science}, abstract = {This tutorial will explore the fascinating domain of empirical network modeling through artificial intelligence (AI) techniques, with applications across social media, web systems, and urban environments. Participants will gain valuable insights into incorporating advanced AI methods-such as graph machine learning, deep reinforcement learning, and generative models-within complex network science. The goal is to provide a comprehensive understanding of how these models can effectively represent, predict, and control empirical networked systems with heterogeneous structures and dynamic processes. The tutorial will begin by introducing essential background knowledge, outlining motivations and challenges, exploring recent methodological advances, and highlighting key applications.} }
@inbook{10.1145/3756423.3756543, title = {Research Progress on the Reliability of Aircraft Emergency Hatch Ejection Drive Mechanisms Integrated with Artificial Intelligence}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing}, pages = {729–734}, year = {2025}, isbn = {9798400714351}, url = {https://doi.org/10.1145/3756423.3756543}, author = {Shi, Junjie and Gao, Yong and Guo, Hantao}, abstract = {Artificial intelligence (AI) and advanced sensor technologies have developed into maintenance, reliability, and optimized aircraft emergency hatch ejection drive mechanisms. The review study narrates research on AI-driven approaches: machine learning, deep learning, federated learning, and generative algorithms (GAs) for fault diagnosis. Internet of Things (IoT) enabled real-time monitoring and intelligence maintenance techniques. Recurrent neural networks particularly long short-term memory (LSTM) models and ML techniques analyse the sensor data for prediction of stress, wear, and failure modes of components, achieving high accuracy and enabling proactive interventions. The results reveal that IoT networks coupled with AI approaches continuously monitor critical parameters including vibration and temperature. This study reveals that optimization techniques, genetic algorithms, and particle swarm optimization play significant roles in enhancing the structural design of components like gears and connecting rods that achieve weight reductions without compromising safety. This study identifies challenges of model interpretability, data scarcity, and regulatory compliance. These issues are addressed by proposing hybrid AI models and federated learning. Emerging trends include self-healing materials, quantum ML approaches, and automated maintenance systems, which could transform safety in the aviation industry. By bridging research gaps between engineering practices and data-driven insights, this study shows that AI can play a vital role in advancing the efficiency and reliability of drive mechanisms in next-generation aircraft, compliance with aviation protocols, and paving the way for adaptive and autonomous systems.} }
@inproceedings{10.1145/3729706.3729781, title = {AI-Driven Learning Approaches in the Era of Artificial Intelligence: Innovations in Design Education}, booktitle = {Proceedings of the 2025 4th International Conference on Cyber Security, Artificial Intelligence and the Digital Economy}, pages = {471–477}, year = {2025}, isbn = {9798400712715}, doi = {10.1145/3729706.3729781}, url = {https://doi.org/10.1145/3729706.3729781}, author = {Yang, Hsi-Hsun and Chou, Wei-Ting}, keywords = {Artificial intelligence, Content analysis, Curriculum innovation, Design education, Systematic review}, abstract = {Artificial Intelligence (AI) is integrated with various domains for design education — expanding creativity, problem-solving, design methodologies, research, etc. due to its ability to simulate human cognition and behavior. Using systematic review and content analysis, this study conducts a systematic review to understand the AI-powered curriculum innovation trends in design education. From the analyzed papers, five emerging themes were identified: (1) Art and Design Education, (2) Visual Communication and Graphic Design, (3) Integration and Innovation of AI in Design Education, (4) Design Thinking and AI for Education, and (5) Generative AI for Design and Fashion. The results suggest that AI has evolved from a foundational technical tool to a transformative force changing how we learn, update curricula, and assess knowledge. Although AI is a game changer in terms of efficiency and finding different creative paths, the ethics and influence on the design process will have to be examined thoroughly. Future research directions may include human-AI collaboration in pedagogy, the ethics of AI-generated design content, and learner adaptation to AI-enabled design education. This research offers the most recent review of AI in practice in design schools, helping educators and policymakers alike shape the role AI plays within the design education context.} }
@inproceedings{10.1145/3680127.3680194, title = {Using Generative AI Chatbot for enhancing people's digital participation}, booktitle = {Proceedings of the 17th International Conference on Theory and Practice of Electronic Governance}, pages = {36–40}, year = {2024}, isbn = {9798400717802}, doi = {10.1145/3680127.3680194}, url = {https://doi.org/10.1145/3680127.3680194}, author = {Pinto, Fernando and Macadar, Marie Anne}, keywords = {Digital participation, civic engagement, e-Participation, generative AI chatbot}, abstract = {Aiming to improve communication and increase the access to available channels and tools, chatbots, or conversational agents, are being used to support people´s digital participation and foster civic engagement. However, conversational agents attend only expected cases of participation, that is, those based on e-consulting, e-decision making and e-informing. Hence, chatbots don´t concern non mapped cases of self-mobilization, besides there are problems in human-computer communication and in the transmission of information caused by digital divide that hinder peoples ‘participation. Literature review indicates that generative AI chatbot shall be used in non-expected interactions between human and conversational agents. Therefore, this paper discusses how chatbots could interact within self-mobilizations’ types of digital participation, concerning obstacles like lack of infrastructure and digital illiteracy. Using design science research, we propose the GenPart, a generative AI-chatbot model for enhancing people´s digital participation. We claim that this artifact can be used by researchers and practitioners who want to promote the integration between people, organizations, and information in digital platforms, promoting citizen participation and civic engagement.} }
@inproceedings{10.1145/3759355.3759358, title = {RoboPianist - A Computer Vision Based Artificial Intelligence Application for Hand Gesture Recognition}, booktitle = {Proceedings of the Intelligent Robotics FAIR 2025}, pages = {22–28}, year = {2025}, isbn = {9798400715891}, doi = {10.1145/3759355.3759358}, url = {https://doi.org/10.1145/3759355.3759358}, author = {Xue, Luyao and Domonkos, M\'ark and Botzheim, J\'anos}, keywords = {Solf\`ege, Edutainment, Robotics, Computer Vision}, abstract = {In this paper, we present RoboPianist, a new artificial intelligence and robotics-based edutainment application for solf\`ege learning. This project aims to create an interactive gesture recognition system that can recognize gestures that are expressed similarly the way how solf\`ege is learned in music classes. For this, our setup consists of a small robotic arm that is connected to a computer to press and release the keys on an electric piano. The computer sends the controlling signals to the robot based on an artificial intelligence-based computer vision pipeline. The potential elements in the different stages of the pipeline were compared to each other in terms of accuracy, efficiency and model parameters, and were chosen accordingly. Our pipeline consists of models for three main functionalities: hand keypoint detection (MediaPipe); static hand signal recognition (a custom MLP model); and dynamic gesture recognition (a GRU model with semantic fusion). This way, our application can recognize the seven hand signals that are based on Kod\'aly’s work, and with the forth and back movement understand the intent of key-press and release of a music note. The system presented high accuracy in the recognition of the hand signals and also the gestures, thus enabling fluent usage in music education.} }
@inproceedings{10.1145/3625704.3625744, title = {Evaluation of e-learning platforms using artificial intelligence (AI) robots: Are the AI robots consistent}, booktitle = {Proceedings of the 7th International Conference on Education and Multimedia Technology}, pages = {96–100}, year = {2023}, isbn = {9798400709142}, doi = {10.1145/3625704.3625744}, url = {https://doi.org/10.1145/3625704.3625744}, author = {Chan, Victor K. Y.}, keywords = {E-learning platforms, artificial intelligence, consistency, evaluation, learning management systems}, abstract = {This article aims to explore the consistency between a few popular generative AI robots in the evaluation of e-learning platforms. The three robots adopted in the study were GPT-4, Sage, and Dragonfly, which were requested to award rating scores to the six major dimensions, namely (1) features and capabilities, (2) ease of use and customization, (3) cost, (4) security, (5) customer support, and (6) scalability, of 10 to 20 currently most popular e-learning platforms. For each of the three robots, the minimum, the maximum, the range, and the standard deviation of the rating scores for each of the six dimensions were computed across all the e-learning platforms. The rating score difference for each of the six dimensions between any pair of robots was calculated for each platform. The mean of the absolute value, the minimum, the maximum, the range, and the standard deviation of the differences for each dimensions between each pair of robots were calculated across all platforms. Finally, a Cronbach alpha coefficient of the rating scores was computed for each of the six dimensions between all the three robots across all the e-learning platforms. The computational results were to reveal whether the three robots accorded discrimination in evaluating each dimension across the platforms and whether there was consistency between the three robots in evaluating each dimension across the platforms. Among some auxiliary results, it was found that the evaluation by the three robots was severely inconsistent for the two dimensions cost and security, inconsistent to a lesser extent for the dimension scalability, and consistent for the remaining three dimensions.} }
@inproceedings{10.1145/3756580.3756634, title = {Artificial Intelligence Generated Content in College English Teaching: Model Construction, Practical Efficacy and Ethical Challenges}, booktitle = {Proceedings of the 2025 6th International Conference on Education, Knowledge and Information Management}, pages = {334–342}, year = {2025}, isbn = {9798400715624}, doi = {10.1145/3756580.3756634}, url = {https://doi.org/10.1145/3756580.3756634}, author = {Yang, Shuo}, keywords = {adaptive learning, artificial intelligence-generated content (AIGC), ethical governance, hybrid research, ternary collaborative framework, university English language teaching}, abstract = {This study explores the systematic integration of Artificial Intelligence Generated Content (AIGC) in university English language teaching, focusing on the core challenge of optimising the teaching closure loop. Using a mixed research method (questionnaire survey, classroom experiment and content analysis), AIGC was found to have significant efficacy in multimodal resource generation, personalised feedback and intercultural scene simulation. The study proposes a three-dimensional collaborative teaching framework: teacher-AIGC-learners, which reveals that the core value of AIGC is to liberate teachers from mechanical labour and achieve precise teaching interventions. Meanwhile, the study faces the ethical dilemmas of academic integrity and data privacy, and proposes a governance framework that combines technological constraints, institutional norms, and humanistic education. The experimental data show that AIGC assistance increases students' writing complexity by 27.9% and decreases their speaking anxiety index by 34%, but there are also problems such as weakened critical thinking due to technological dependence. This study provides a reusable pedagogical model and risk coping strategy for the digital transformation of education.} }
@inproceedings{10.1145/3715336.3735714, title = {Artificial Intelligence and other Speculative Metaphors}, booktitle = {Proceedings of the 2025 ACM Designing Interactive Systems Conference}, pages = {347–356}, year = {2025}, isbn = {9798400714856}, doi = {10.1145/3715336.3735714}, url = {https://doi.org/10.1145/3715336.3735714}, author = {Blythe, Mark and Lindley, Si\^an and Murray-Rust, Dave}, keywords = {Artificial Intelligence, Human AI Interaction, Metaphor Design, Speculative Design}, abstract = {The paper proposes “speculative metaphors” as constructs for reframing and critically engaging with ideas of artificial intelligence. It identifies a broad range of AI metaphors in the wider culture and technical literature and discusses metaphor design in terms of explanation, persuasion and speculation. To explore different metaphor design strategies, we use a custom GPT to generate a large number of variants on the “artificial intelligence” metaphor. The paper contributes a conceptual framing for such speculative metaphor drawing on ideas of knowledge and understanding, fusion and synthesis, collaboration and collectives. We argue that generating speculative metaphors provides a means of thinking critically about human-AI interaction.} }
@inbook{10.1145/3756423.3756561, title = {Theoretical framework and simulation experiment analysis of material organization performance prediction driven by artificial intelligence}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing}, pages = {838–842}, year = {2025}, isbn = {9798400714351}, url = {https://doi.org/10.1145/3756423.3756561}, author = {Fan, Ronghui and Chen, Dandan}, abstract = {With the rapid development of artificial intelligence technology, more and more fields are leveraging its powerful data processing and pattern recognition capabilities for research, and materials science is no exception. Driven by artificial intelligence, this study proposes a material microstructure-performance prediction model. By conducting simulation experiments, it explores the relationship between the material's microstructure and its macroscopic properties. The model predicts and analyzes the mechanical properties, thermal properties, and corrosion resistance of typical metal materials such as steel, aluminum alloys, and titanium alloys, verifying its high accuracy and reliability. The artificial intelligence-driven model can effectively fit test data with high prediction accuracy, and the errors fall within a reasonable range. The combination of artificial intelligence with simulated annealing and genetic algorithms further optimizes the performance of the prediction model.} }
@inproceedings{10.1145/3627673.3680117, title = {Generative AI and Retrieval-Augmented Generation (RAG) Systems for Enterprise}, booktitle = {Proceedings of the 33rd ACM International Conference on Information and Knowledge Management}, pages = {5599–5602}, year = {2024}, isbn = {9798400704369}, doi = {10.1145/3627673.3680117}, url = {https://doi.org/10.1145/3627673.3680117}, author = {Xu, Anbang and Yu, Tan and Du, Min and Gundecha, Pritam and Guo, Yufan and Zhu, Xinliang and Wang, May and Li, Ping and Chen, Xinyun}, keywords = {enterprise application, generation, rag, retrieval}, abstract = {This workshop introduces generative AI applications for enterprise, with a focus on retrieval-augmented generation (RAG) systems. Generative AI is a field of artificial intelligence that can create new content and solve complex problems. RAG systems are a novel generative AI technique that combines information retrieval with text generation to generate rich and diverse responses. RAG systems can leverage enterprise data, which is often specific, structured, and dynamic, to provide customized solutions for various domains. However, enterprise data also poses challenges such as scalability, security, and data quality. This workshop convenes researchers and practitioners to explore RAG and other generative AI systems in real-world enterprise scenarios, fostering knowledge exchange, collaboration, and identification of future directions. Relevant to the CIKM community, the workshop intersects with core areas of data science and machine learning, offering potential benefits across various domains.} }
@inproceedings{10.1145/3727993.3727999, title = {Artificial Intelligence Generated Content Neural Network Model for Enterprise Management Decision-Making System}, booktitle = {Proceedings of the 2024 4th International Conference on Computational Modeling, Simulation and Data Analysis}, pages = {27–32}, year = {2025}, isbn = {9798400711831}, doi = {10.1145/3727993.3727999}, url = {https://doi.org/10.1145/3727993.3727999}, author = {Wei, Xueling and Lin, Tao}, keywords = {Artificial Intelligence Generated Content, Business management decision-making, neural network model}, abstract = {Research was conducted on Artificial Intelligence Generated Content neural network model solutions for the technical issues of enterprise decision-making systems. The enterprise decision-making system utilizes Artificial Intelligence Generated Content, neural network technology, and algorithms to establish modules for neural network data preprocessing, anomaly detection, predictive analysis, decision support, adaptive machine learning, and visual decision-making models, integrating the generative and controllable aspects of artificial intelligence into the decision-making system. In simulation experiments, this system improved output accuracy by 21.6% compared to general information systems and provided 89.7 times more effective reference data, indicating that the proposed technical solution is feasible.} }
@article{10.1145/3672462, title = {Global Initiatives Towards Regulatory Frameworks for Artificial Intelligence (AI) in Higher Education}, journal = {Digit. Gov.: Res. Pract.}, volume = {6}, year = {2025}, doi = {10.1145/3672462}, url = {https://doi.org/10.1145/3672462}, author = {Mahrishi, Mehul and Abbas, Asad and Siddiqui, Mohammad Khubeb}, keywords = {Artificial intelligence, AI governance, educational innovation, ethics, governance, higher education, SDG4}, abstract = {Artificial intelligence (AI) integration into education has received significant global attention, sparking a need for comprehensive regulatory frameworks for governance. In this commentary, we first examine the role of AI in education and how it is integrated with the teaching and learning process. It also discusses the impact of AI on higher education through specific case studies and tries to illuminate the current/emerging trends, challenges, and potential future directions. Furthermore, it highlights insights from global initiatives, policy frameworks, and ethical standards adopted by prominent organizations to govern AI in higher education. The study concludes that the optimal use of these AI apps can only be harnessed through proper transparency and ethical balance.} }
@inbook{10.5555/3716662.3716684, title = {An FDA for AI? Pitfalls and Plausibility of Approval Regulation for Frontier Artificial Intelligence}, booktitle = {Proceedings of the 2024 AAAI/ACM Conference on AI, Ethics, and Society}, pages = {239–254}, year = {2025}, author = {Carpenter, Daniel and Ezell, Carson}, abstract = {Observers and practitioners of artificial intelligence (AI) have proposed an FDA-style licensing regime for the most advanced AI models, or 'frontier' models. In this paper, we explore the applicability of approval regulation - that is, regulation of a product that combines experimental minima with government licensure conditioned partially or fully upon that experimentation - to the regulation of frontier AI. There are a number of reasons to believe that approval regulation, simplistically applied, would be inapposite for frontier AI risks. Domains of weak fit include the difficulty of defining the regulated product, the presence of Knightian uncertainty or deep ambiguity about harms from AI, the potentially transmissible nature of risks, and distributed activities among actors involved in the AI lifecycle. We conclude by highlighting the role of policy learning and experimentation in regulatory development, describing how learning from other forms of AI regulation and improvements in evaluation and testing methods can help to overcome some of the challenges we identify.} }
@inproceedings{10.1145/3706599.3720238, title = {Dream Relay: Exploring Dream Sharing with Generative AI}, booktitle = {Proceedings of the Extended Abstracts of the CHI Conference on Human Factors in Computing Systems}, year = {2025}, isbn = {9798400713958}, doi = {10.1145/3706599.3720238}, url = {https://doi.org/10.1145/3706599.3720238}, author = {Wu, Zi-Xin and Lin, Li and Chuang, Ling-I and Shen, Jian and Lai, Ruei Ci and Liang, Rung-Huei and Cheng, Yu-Ting and Chen, Chien-Hsiung}, keywords = {Dreams, Dream sharing, Dream visualization, Generative AI, Dream privacy}, abstract = {To better understand people’s inner worlds, the HCI community has explored ways to understand dream experience through generative AI (GAI). This paper presented “dream relay,” a novel approach to sharing dreams by integrating GAI. In the relay, participants used a dream probe to record their dreams, which were then visualized by GAI and shared with the next person as stimuli. We conducted a probe-based, group autoethnography study to explore how dream sharing was influenced in the way of relay and with GAI. Results showed how participants recorded their dreams and perceived the AI-generated dream visuals. This study also highlighted the value of GAI in sparking imagination and alleviating personal privacy concerns, which increased the willingness to share. Finally, we discussed the human-AI collaboration in reshaping dreams, as well as the role of GAI in facilitating social interaction.} }
@inproceedings{10.1145/3745238.3745333, title = {Intelligence Measurement and Empirical Analysis of Digital Economy and Energy Efficiency in Ningxia under the Background of Big Data and Artificial Intelligence}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {603–609}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745333}, url = {https://doi.org/10.1145/3745238.3745333}, author = {Lu, Chuntao and Xue, Feng and He, Jia and Chen, Tao and Liu, Xiangyang}, keywords = {Big Data and Artificial Intelligence, Digital Economy, Energy Efficiency, Entropy Method}, abstract = {Under the dual driving forces of artificial intelligence algorithms and big data technology empowerment, the digital economy has emerged as a pivotal force in restructuring the global energy system. This study selects Ningxia Hui Autonomous Region as the research subject, establishing separate indicator systems for regional digital economy development and energy efficiency. The entropy method is employed to comprehensively evaluate development levels. Subsequently, panel data constructed from these measurements are utilized to empirically analyze the impact of digital economy development on energy efficiency using the Stata 16.0 software platform. The findings conclusively demonstrate that the digital economy significantly enhances energy efficiency in Ningxia. Based on these results, targeted policy recommendations are proposed.} }
@inproceedings{10.1145/3736181.3747154, title = {The Impact of Artificial Intelligence on Higher Education: Perspectives and Insights from Computer Science Faculty and Students in Ghana}, booktitle = {Proceedings of the ACM Global on Computing Education Conference 2025 Vol 1}, pages = {85–91}, year = {2025}, isbn = {9798400719295}, doi = {10.1145/3736181.3747154}, url = {https://doi.org/10.1145/3736181.3747154}, author = {Korsah, G. Ayorkor and Adjei, Joseph K. and Mensah, Joseph Agyapong and Arthur, Linda and Ariff, A. Sabit and Awuah, Rebecca}, keywords = {artificial intelligence, computer science education, generative ai, ghana, higher education, impact}, abstract = {This paper explores the current and potential impact of Artificial Intelligence (AI) on higher education from the perspective of computer science faculty and students across three universities in Accra, Ghana. With participation from 248 students and 21 faculty, the study employed surveys and focus group discussions to capture participants' perceptions on AI, how AI is currently integrated into teaching, learning and assessment, and the impact of AI on these activities. The results show that both students and faculty perceive that AI is transforming higher education, and faculty are exploring AI integration in their work. However, many faculty have concerns about the negative effects of students' use of generative AI and continue to educate them on responsible use of AI for learning. Such concerns influence how faculty assess student learning reverting to in-person assessments among other strategies. Overall, the consensus is that AI is transforming higher education, and there is a need for institutions, faculty and students to adapt to this reality.} }
@article{10.1145/3715977, title = {From Academia to UX: Embodied Cognition, Creativity, and Generative AI}, journal = {Interactions}, volume = {32}, pages = {7–8}, year = {2025}, issn = {1072-5520}, doi = {10.1145/3715977}, url = {https://doi.org/10.1145/3715977}, author = {Rashedi, Roxanne N.}, abstract = {The Interactions website (interactions.acm.org) hosts a stable of bloggers who share insights and observations on HCI, often challenging current practices. Each issue we'll publish selected posts from some of the leading and emerging voices in the field.} }
@article{10.1145/3736711, title = {Artificial Intelligence in Networking Research in the Arab World}, journal = {Commun. ACM}, volume = {68}, pages = {78–82}, year = {2025}, issn = {0001-0782}, doi = {10.1145/3736711}, url = {https://doi.org/10.1145/3736711}, author = {Shihada, Basem and Abdallah, Mohamed}, keywords = {Artificial Intelligence, Arab World, Non-Terrestrial Networks, IoT, Smart Grids}, abstract = {The rapid evolution of networking technologies, driven by artificial intelligence (AI) and machine learning (ML), is reshaping the landscape of digital connectivity across the Arab world. As the region undergoes a notable digital transformation, fueled by the adoption of edge and cloud computing, 5/6G, and the Internet of Things, AI and ML applications are being leveraged to optimize network performance, enhance security, and ensure scalability. This article offers a sample of the contribution of the Arab world in the integration of AI/ML to modernize networks, focusing on two key areas: intelligent wireless connectivity and intelligent secure networking systems. The article also addresses challenges, including the need for skilled professionals, regulatory frameworks, and data availability for training AI/ML models. Overall, this work showcases the Arab world’s growing influence in networking research, offering solutions with the potential for transformative economic and technological impact.This sampling of the current state of networking research in the Arab world emphasizes two primary areas: intelligent wireless connectivity and intelligent secure networking systems.} }
@inproceedings{10.1145/3726010.3726018, title = {Certain Investigations on Application and Challenges of Artificial Intelligence in Prefabricated Scene Design for 3D Animation}, booktitle = {Proceedings of the 2024 International Conference on Artificial Intelligence, Digital Media Technology and Interaction Design}, pages = {54–61}, year = {2025}, isbn = {9798400712845}, doi = {10.1145/3726010.3726018}, url = {https://doi.org/10.1145/3726010.3726018}, author = {Gu, Jie and Si, Yiming and Li, Peifang and Li, Mengyang}, keywords = {3D Animation, Artificial Intelligence (AI), Automation, Creative Expression, Deep Learning, Generative Adversarial Networks (GANs), Reinforcement Learning, Scene Design}, abstract = {The 3D animation is emerging with the fast improvement in the artificial intelligence (AI) technology. This paper presents a complete survey about the application of AI in the pre-fabricated design of 3D animation scenes. Also includes the challenges and opportunities in the role of AI in 3D animation. The basic applications of AI in scene design, including automated element layout, maintenance of stylistic consistency, and dynamic environment simulation are also discussed. We discuss the use of AI methods such as, deep learning, generative adversarial networks (GANs), and reinforcement learning in helping the animators with creative ideas and enhancing the scene design. The difficulties in the scene design process like limitations in creative expression, difficulties in technological integration are also discussed in this paper along with the challenges with respect to traditional animation techniques. In addition, the paper discusses the research directions in the automation features of AI and the human artistic creation.} }
@inproceedings{10.1145/3750069.3757878, title = {Workshop on supporting CRIme reSolution Through Artificial INtelligence (CRISTAIN) - 1st edition}, booktitle = {Proceedings of the 16th Biannual Conference of the Italian SIGCHI Chapter}, year = {2025}, isbn = {9798400721021}, doi = {10.1145/3750069.3757878}, url = {https://doi.org/10.1145/3750069.3757878}, author = {Cal\`o, Eleonora and Caruccio, Loredana and Vitiello, Autilia}, keywords = {Crime Domain, Artificial Intelligence, Natural Language Processing, Fairness, Topic extraction, Computer forensics}, abstract = {This document provides a summary of the First Workshop on ‘supporting CRIme reSolution Through Artificial INtelligence’ (CRISTAIN 2025), which has been accepted at the 6th Biannual Conference of the Italian SIGCHI Chapter (CHItaly 2025).} }
@inbook{10.1145/3756423.3756547, title = {Construction and Practice of the Network Information Security System in Colleges and Universities Based on Artificial Intelligence}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing}, pages = {752–757}, year = {2025}, isbn = {9798400714351}, url = {https://doi.org/10.1145/3756423.3756547}, author = {Tang, Dong and Li, Fang and Chen, Haoyang}, abstract = {In the digital age, the network information security of colleges and universities is of utmost importance. The rapid development of artificial intelligence technology has provided new opportunities for constructing a network information security system in colleges and universities. This article takes colleges and universities as the background, deeply explores how to construct a network information security system by combining artificial intelligence technology, and elaborates on practical experiences in detail. It aims to provide valuable references for the current network security construction in colleges and universities. By analyzing the challenges faced by the network information security in colleges and universities, expounding on the application advantages of artificial intelligence in network security, proposing specific system construction strategies, and illustrating its effectiveness with practical cases, this article provides theoretical support and practical guidance for colleges and universities to improve their network information security levels.} }
@inproceedings{10.1145/3745238.3745296, title = {Research on optimization of Guangdong sericulture industry chain driven by artificial intelligence}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {344–349}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745296}, url = {https://doi.org/10.1145/3745238.3745296}, author = {Lin, Zhaoxuan and Zhang, Jian}, keywords = {Artificial intelligence, Inventory management, Market demand forecast, Production efficiency, Sericulture industry chain}, abstract = {This study is dedicated to studying the application of artificial intelligence in the silk industry in Guangdong Province, evaluating its role in production efficiency, cost control, logistics process improvement, market demand data forecasting and inventory control, collecting and interpreting data from six representative sericulture companies in the Pearl River Delta region of Guangdong, integrating intelligent breeding monitoring systems, logistics allocation platforms, demand estimation methods and AI-assisted inventory control systems, and studying the actual effectiveness of artificial intelligence in improving the industrial chain. The analysis results show that the use of artificial intelligence technology has significantly improved the annual output and quality of silk cocoons, reduced the production cost input-to-amount ratio, improved the timeliness and economy of logistics distribution, and improved the accuracy of market demand forecasting. After the inventory management system was upgraded, the inventory holding cost was significantly reduced, and the inventory turnover rate was improved. Inheriting these achievements, it is finally concluded that artificial intelligence technology has greatly promoted the efficiency enhancement and cost reduction of Guangdong's silk industry, providing key impetus for the intelligent and green transformation of the industry.} }
@article{10.1145/3768312, title = {Artificial Intelligence in Educational Technology: A Systematic Review of Datasets and Applications}, journal = {ACM Comput. Surv.}, volume = {58}, year = {2025}, issn = {0360-0300}, doi = {10.1145/3768312}, url = {https://doi.org/10.1145/3768312}, author = {Topham, Luke and Atherton, Peter and Reynolds, Tom and Hussain, Yasir and Hussain, Abir and Kolivand, Hoshang and Khan, Wasiq}, keywords = {Education, virtual learning, artificial intelligence, machine learning, chatbots, large language models, AIEd}, abstract = {Artificial Intelligence (AI) has the potential to impact a diverse range of domains. For instance, AI for the education domain has received increasing interest with various applications, including predicting performance, curating learning materials, and automated assessment and feedback. Despite the developments, some imbalances appear in the literature; for example, traditional classrooms and non-scientific academic subjects received little attention. This survey provides a systematic review of the current trends in AI research for education, specifically addressing applications within secondary education (ages 11+) through to higher education (HE), and offers a detailed compilation of datasets and methods, facilitating a deeper understanding of the field and encouraging further investigation. It includes a thorough review of the datasets available to encourage and enable future research, development, and collaboration, as well as the establishment of performance benchmarks. Furthermore, this survey provides an overview of issues and problems arising from recent developments, which may aid policymakers in their decision-making and addressing ethical concerns and standards. For example, many AI in Education (AIEd) platforms are not grounded in educational theory. We also present several guidelines to aid future developments in AIEd, guiding long-term impactful projects and investments.} }
@inproceedings{10.1145/3635638.3635663, title = {Swarm GAN: Stabilizing Training of Generative Adversarial Networks via Swarm Intelligence}, booktitle = {Proceedings of the 6th International Conference on Machine Learning and Machine Intelligence}, pages = {171–177}, year = {2024}, isbn = {9798400709456}, doi = {10.1145/3635638.3635663}, url = {https://doi.org/10.1145/3635638.3635663}, author = {Li, Zihao and Zhou, Yuan and Wang, Zhiyuan and Li, Minne}, keywords = {Generative Adversarial Networks, Swarm Intelligence, machine learning, unsupervised learning}, abstract = {Generative adversarial networks (GANs) have seen significant research interest over the past decade, yet core issues of training instability and mode collapse persist. This work proposes SwarmGAN, a novel GAN framework incorporating swarm intelligence to address these limitations. Specifically, swarm intelligence exhibits properties well-suited to enhance GAN training: emergent complex behaviors arising from simple individual agents, decentralized adaptability to instantaneous data and hyperparameters, and robustness through simple iterative interactions. SwarmGAN incorporates a particle swarm optimization algorithm to guide generator and discriminator updates. Convolutional neural network architectures and gradient penalties further ensure baseline generation quality and diversity. Extensive experiments over diverse image datasets demonstrate the effectiveness of SwarmGAN. Quantitative evaluations using Fr\'echet Inception Distance, Inception Score, Peak Signal-to-Noise Ratio, and Structural Similarity Index Score validate performance improvements across stability, sample quality, and convergence speed. The proposed integration of swarm intelligence into adversarial networks shows promising capability to address long-standing GAN challenges.} }
@inproceedings{10.1145/3657604.3664699, title = {Scaling Up Mastery Learning with Generative AI: Exploring How Generative AI Can Assist in the Generation and Evaluation of Mastery Quiz Questions}, booktitle = {Proceedings of the Eleventh ACM Conference on Learning @ Scale}, pages = {310–314}, year = {2024}, isbn = {9798400706332}, doi = {10.1145/3657604.3664699}, url = {https://doi.org/10.1145/3657604.3664699}, author = {Hutt, Stephen and Hieb, Grayson}, keywords = {content evaluation, content generation, generative ai, large language models, mastery learning}, abstract = {Generative AI has the potential to scale a number of educational practices, previously limited by resources. One such instructional approach is mastery learning, a pedagogy emphasizing proficiency before progression that is highly resource (teacher time, materials) intensive. The rise of computer-based instruction offered partial solutions, tailoring student progression and automating some facets of the mastery learning process. This work in progress considers the application of large language models for content generation tailored to mastery learning. We present a paired framework for analyzing and evaluating the generated content relative to rubrics designed by the teacher. Recognizing the potential of large language models, we critically assess the potential of improving mastery-based instruction. We close our discussion by considering the applications and limitations of this approach.} }
@inproceedings{10.1145/3677525.3678677, title = {"Artificial intelligence is a very broad term": How educators perceive Artificial Intelligence?}, booktitle = {Proceedings of the 2024 International Conference on Information Technology for Social Good}, pages = {315–323}, year = {2024}, isbn = {9798400710940}, doi = {10.1145/3677525.3678677}, url = {https://doi.org/10.1145/3677525.3678677}, author = {Kasinidou, Maria and Kleanthous, Styliani and Otterbacher, Jahna}, keywords = {AI Education, Artificial Intelligence, educators, perception of AI}, abstract = {Artificial intelligence (AI) has seamlessly integrated into our daily routines. Recognizing the significance of AI, educators are expected not only to teach about AI but also to utilize AI tools in their teaching, ensuring that their students are equipped with the necessary knowledge to navigate the AI-driven society. Thus, it is important to understand how educators perceive AI. In this study, we explore educators’ perceptions of AI, as well as whether they teach AI, the challenges they face in teaching AI, potential support for teaching AI in the future, and possible differences between Cypriot and international educators. Two online surveys, involving 197 educators, followed by interviews with 10 Cypriot educators were conducted to examine how they perceive AI. The findings revealed a shared conceptualization of AI as “human-like”, intelligent machines, capable of learning and solving problems using data. Notable differences in the definitions of AI emerged between Cypriot educators and their international counterparts. Furthermore, the findings showed that even though educators perceive teaching AI as important, they lack formal training and specialized educational tools on AI, underscoring the urgent need for targeted interventions and tailored resources.} }
@inbook{10.1145/3727648.3727758, title = {The practical application and development of artificial intelligence in medical imaging}, booktitle = {Proceedings of the 4th International Conference on Computer, Artificial Intelligence and Control Engineering}, pages = {680–685}, year = {2025}, isbn = {9798400712647}, url = {https://doi.org/10.1145/3727648.3727758}, author = {Wang, Chenwang and Wang, Xinning and Zhang, Jiangbao and Jiang, Yichi and Ren, Tianle and Lv, Zhonghao and Li, Jingyu and Wang, Yuheng}, abstract = {Medical imaging is a discipline based on image recognition. With the continuous progress of technology, the application of artificial intelligence in the field of medical imaging is becoming more and more extensive, and gradually becoming an important part of research and clinical practice. Artificial intelligence has greatly changed our way of life on the one hand; on the other hand, AI-assisted quantitative image analysis algorithms have great potential for development in improving the efficiency and accuracy of medical imaging diagnosis. This paper firstly outlines the basic model and architecture of AI, and then focuses on its application in the field of medical imaging. Next, several major categories and fields that are widely used in AI will be analyzed in depth, their research progress will be comprehensively analyzed, current major research directions will be summarized, and future de-velopment trends will be predicted.} }
@inbook{10.1145/3729706.3729732, title = {A Comprehensive Review on the Applications of Artificial Intelligence in Cybersecurity}, booktitle = {Proceedings of the 2025 4th International Conference on Cyber Security, Artificial Intelligence and the Digital Economy}, pages = {172–179}, year = {2025}, isbn = {9798400712715}, url = {https://doi.org/10.1145/3729706.3729732}, author = {Zeng, Qinghao}, abstract = {Recently, we have realized rapid advancement of technology in several fields due to the emergence of Artificial Intelligence (AI). AI plays a significant role in enhancing the cyber security field to hasten the detection of threat and response, systematize the repetitive tasks, and enhance the accuracy of the cyber security team's actions. It is used to strengthen the security measures against several security challenges and cyber-attacks. This article presents a comprehensive review on the applications of AI in cyber security by elaborating the strengths, limitations, and potential risks. This work discusses various kinds of AI algorithms used to enhance the cyber security. This work also examines the roles of AI in intrusion detection, malware detection, and vulnerability discovery. The potential risks and challenges associated with AI approaches in cyber security is elucidated. Finally, this paper suggests the way to battle AI-based vulnerabilities and threats, and suggests future directions.} }
@inproceedings{10.1145/3708359.3712125, title = {Mental Models of Generative AI Chatbot Ecosystems}, booktitle = {Proceedings of the 30th International Conference on Intelligent User Interfaces}, pages = {1016–1031}, year = {2025}, isbn = {9798400713064}, doi = {10.1145/3708359.3712125}, url = {https://doi.org/10.1145/3708359.3712125}, author = {Wang, Xingyi and Wang, Xiaozheng and Park, Sunyup and Yao, Yaxing}, keywords = {Mental Models, Generative AI Chatbots, Privacy and Security, Human Computer Interaction}, abstract = {The capability of GenAI-based chatbots, such as ChatGPT and Gemini, has expanded quickly in recent years, turning them into GenAI Chatbot Ecosystems. Yet, users’ understanding of how such ecosystems work remains unknown. In this paper, we investigate users’ mental models of how GenAI Chatbot Ecosystems work. This is an important question because users’ mental models guide their behaviors, including making decisions that impact their privacy. Through 21 semi-structured interviews, we uncovered users’ four mental models towards first-party (e.g., Google Gemini) and third-party (e.g., ChatGPT) GenAI Chatbot Ecosystems. These mental models centered around the role of the chatbot in the entire ecosystem. We further found that participants held a more consistent and simpler mental model towards third-party ecosystems than the first-party ones, resulting in higher trust and fewer concerns towards the third-party ecosystems. We discuss the design and policy implications based on our results.} }
@inproceedings{10.1145/3746273.3760211, title = {The Analysis of Cultural Heritage in the Age of Artificial Intelligence}, booktitle = {Proceedings of the 7th International Workshop on AnalySis, Understanding and ProMotion of HeritAge Contents}, pages = {1–2}, year = {2025}, isbn = {9798400720550}, doi = {10.1145/3746273.3760211}, url = {https://doi.org/10.1145/3746273.3760211}, author = {Jim\'enez-Badillo, Diego}, keywords = {artificial intelligence in archaeology, machine learning in archaeology, shape analysis in archaeology, information retrieval from historical documents}, abstract = {Abstract In an era increasingly shaped by artificial intelligence, the analysis and preservation of cultural heritage are undergoing a profound transformation. AI is redefining how we document, interpret and engage with archaeological sites, cultural artifacts and historical narratives. This conference presents a selection of AI and machine learning applications in the humanities, with a particular focus on archaeology. It examines the conditions under which advanced learning models are being adopted in this field, addressing the unique challenges posed by the nature and availability of archaeological data. Unlike disciplines that benefit from large, continuously generated datasets, archaeology often relies on data that is produced slowly and in limited quantities. This scarcity presents both challenges and opportunities for adapting AI methodologies. Among the most compelling applications are the recognition and retrieval of 3D models for shape-based search engines, the extraction of information from historical documents and the classification of archaeological artifacts. During the conference two studies are presented: The first explores a common need of cultural institutions: facilitating access to digital 3D models of cultural heritage objects. This challenge is met through the development of MeshAnalizer, a shape-based search engine for 3D models. Unlike traditional systems that rely on textual input, MeshAnalizer uses geometric shape descriptors to identify and retrieve similar models from local or online repositories. The system follows a four-step methodology: calculating shape descriptors, comparing them using mathematical metrics, constructing an index, and enabling query-by-example retrieval. MeshAnalizer enables users to search by uploading a representative 3D model, returning ranked results based on geometric similarity. This approach enhances access to digital heritage collections and supports research, education, and public engagement (Jim\'enez-Badillo et al., 2021, 2024). The second study explores the integration of Natural Language Processing (NLP) and Corpus Linguistics in the analysis of historical documents. The project developed innovative methodologies and software tools for the automated analysis of digitized texts, particularly the Relaciones Geogr\'aficas de Nueva Espa\~na, a XVI century corpus that contains important information on social, economic and territorial organization. Central to the methodology was a Named Entity Recognition and Disambiguation (NERD) model built using convolutional neural networks and word embeddings via SpaCy, which identified and categorized historically significant entities within the corpus. Achieving nearly 90% accuracy, the model was trained and validated on manually annotated datasets. The project also applied a methodology called Geographical Text Analysis (GTA). The combination of NERD and GTA allow implementing two key software tools that facilitate spatial analysis and support complex historical inquiries, offering new insights into colonial territorial organization, resource management, and sociocultural dynamics (Jim\'enez-Badillo et al. 2020, 2021; Murrieta-Flores et al., 2022, 2025). The goal of the presentation is to demonstrate that cutting-edge learning models can unlock new insights in domains traditionally limited by data scarcity. The conference will showcase both the technical hurdles and the innovative solutions that emerge when applying AI in these contexts. Through case studies and emerging methodologies, this conference invite scholars, technologists, and cultural practitioners to reflect on the evolving relationship between machine intelligence specialists and cultural heritage professionals –and to envision a future where cultural heritage is not only preserved but dynamically reimagined through innovative collaboration.} }
@inproceedings{10.1145/3689187.3709614, title = {Beyond the Hype: A Comprehensive Review of Current Trends in Generative AI Research, Teaching Practices, and Tools}, booktitle = {2024 Working Group Reports on Innovation and Technology in Computer Science Education}, pages = {300–338}, year = {2025}, isbn = {9798400712081}, doi = {10.1145/3689187.3709614}, url = {https://doi.org/10.1145/3689187.3709614}, author = {Prather, James and Leinonen, Juho and Kiesler, Natalie and Gorson Benario, Jamie and Lau, Sam and MacNeil, Stephen and Norouzi, Narges and Opel, Simone and Pettit, Vee and Porter, Leo and Reeves, Brent N. and Savelka, Jaromir and Smith, David H., IV and Strickroth, Sven and Zingaro, Daniel}, keywords = {artificial intelligence, computing education, genai, generative ai, large language models, pedagogical practices, teaching computing}, abstract = {Generative AI (GenAI) is advancing rapidly, and the literature in computing education is expanding almost as quickly. Initial responses to GenAI tools were mixed between panic and utopian optimism. Many were fast to point out the opportunities and challenges of GenAI. Researchers reported that these new tools are capable of solving most introductory programming tasks and are causing disruptions throughout the curriculum. These tools can write and explain code, enhance error messages, create resources for instructors, and even provide feedback and help for students like a traditional teaching assistant. In 2024, new research started to emerge on the effects of GenAI usage in the computing classroom. These new data involve the use of GenAI to support classroom instruction at scale and to teach students how to code with GenAI. In support of the former, a new class of tools is emerging that can provide personalized feedback to students on their programming assignments or teach both programming and prompting skills at the same time. With the literature expanding so rapidly, this report aims to summarize and explain what is happening on the ground in computing classrooms. We provide a systematic literature review; a survey of educators and industry professionals; and interviews with educators using GenAI in their courses, educators studying GenAI, and researchers who create GenAI tools to support computing education. The triangulation of these methods and data sources expands the understanding of GenAI usage and perceptions at this critical moment for our community.} }
@inproceedings{10.1145/3750069.3757881, title = {COL-SAI: COllaboration and Learning through Symbiotic Artificial Intelligence}, booktitle = {Proceedings of the 16th Biannual Conference of the Italian SIGCHI Chapter}, year = {2025}, isbn = {9798400721021}, doi = {10.1145/3750069.3757881}, url = {https://doi.org/10.1145/3750069.3757881}, author = {Calvano, Miriana and Curci, Antonio and Cantone, Andrea Antonio and Perillo, Francesca}, keywords = {Artificial Intelligence (AI), Symbiosis, Learning, Human-Centered AI}, abstract = {The COllaboration and Learning through Symbiotic Artificial Intelligence (COL-SAI) workshop explores how humans and AI can effectively collaborate, learn from each other, and co-adapt symbiotically. Through discussions and the exchange of perspectives, the workshop aims to investigate the key components of the interaction between the two parties. Participants will gain deeper insights into the implications of integrating AI into various aspects of daily life, facilitating problem-solving activities such as learning, decision-making, and co-creation.} }
@inproceedings{10.1145/3745238.3745454, title = {Research on Classroom Management and Student Engagement Analysis System Based on Artificial Intelligence}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1385–1391}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745454}, url = {https://doi.org/10.1145/3745238.3745454}, author = {Yang, Rihan}, keywords = {artificial intelligence, classroom management, convolutional neural network (CNN), multimodal sensors, student engagement, support vector machine (SVM)}, abstract = {This study designs and implements an artificial intelligence based classroom management and student engagement analysis system which combines computer vision, deep learning and multimodal sensor technology to analyze students’ facial expression, behavioral action and physiological signal in real time. This study uses improved convolutional neural network method (CNN) based facial expression recognition (accuracy &gt;85%), OpenPose based posture estimation combined with support vector machine algorithm (SVM) based application for classroom behavior analysis (accuracy 90%), and eye- tracking and heart rate sensor information to measure attention level (HRV formula: ( = sqrt frac1rmN - 1mathop sum limits_mathfraki = 1^rmN ( mathcalRmathcalR_rangle - overline mathcalRmathcalR )^2 )) (accuracy 85%). Experimental results show that the system can improve classroom engagement (15% increase in experimental class compared with control class) and has high stability (≥85% in complex environment) based on modular design (such as Apache Kafka for real-time data stream) . This research work offers a practical application of based on artificial intelligence for educational intelligence, which has great application value.} }
@article{10.1145/3759243, title = {Underwater Optical Object Detection in the Era of Artificial Intelligence: Current, Challenge, and Future}, journal = {ACM Comput. Surv.}, volume = {58}, year = {2025}, issn = {0360-0300}, doi = {10.1145/3759243}, url = {https://doi.org/10.1145/3759243}, author = {Chen, Long and Huang, Yuzhi and Dong, Junyu and Xu, Qi and Kwong, Sam and Lu, Huimin and Lu, Huchuan and Li, Chongyi}, keywords = {Underwater optical object detection, artificial intelligence, literature review}, abstract = {Underwater optical object detection (UOD), aiming at identifying and localising objects in underwater optical images or videos, presents significant challenges due to the optical distortion, water turbidity, and changing illumination in underwater scenes. In recent years, artificial intelligence (AI) based methods, especially deep learning methods, have shown promising performance in UOD. To further facilitate future advancements, we comprehensively study AI-based UOD. In this survey, we first categorise existing algorithms into traditional machine learning-based methods and deep learning-based methods, and summarise them by considering learning strategies, experimental datasets, learning stages, employed features or techniques, and underlying frameworks. Next, we discuss the potential challenges and suggest possible solutions and new directions. We also perform both quantitative and qualitative evaluations of mainstream algorithms across multiple benchmark datasets, taking into account the diversity and biases in experimental setups. Finally, we introduce two off-the-shelf detection analysis tools, Diagnosis and TIDE, which will examine the effects of object characteristics and various types of errors on detector performance. These tools help identify the strengths and weaknesses of different detectors, providing insights for further improvement. The source code, trained models, utilised datasets, detection results, and detection analysis tools are publicly available at and will be regularly updated.} }
@inproceedings{10.1145/3745238.3745449, title = {Educational Reform in the Era of Artificial Intelligence: From Autonomous Learning to Competition in Calculation Power}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1355–1360}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745449}, url = {https://doi.org/10.1145/3745238.3745449}, author = {Wang, Bing and Gong, Jiewei}, keywords = {artificial intelligence, computing power competition, educational equity, personalized learning}, abstract = {Socio-technological developments are driving the profound impact of artificial intelligence (AI) technologies on education. Driven by the “disruptive technology revolution” in different fields, AI is reshaping knowledge acquisition, teaching and learning, from personalized learning systems to the development of brain-computer interfaces (BCIs), the traditional “factory” mode of education is difficult to adapt to personalized learning development needs. Learning development needs, and artificial intelligence-driven systems in support of adaptive learning, intelligent tutoring and automatic assessment, in addition to greatly improve the learning efficiency “liberation of productivity”, but also to change the way of knowledge transfer, affect human cognition and independent learning. It is foreseeable that the competition for education in the age of artificial intelligence may shift from knowledge accumulation to computational power domination. That is to say, individuals with more computing power will have access to better learning resources, which will definitely increase the “digital education gap” on the other hand. This paper analyzes the current situation, challenges, and future trends of AI in education, and explores the impact of AI on self-directed learning, cognitive processes, and the possible evolution of education towards computational competitiveness. Using research methods such as literature review, case study, and technology trend prediction, this study combines the theories of cognitive science, philosophy of AI, and pedagogy to assess the educational impact of AI and propose policy recommendations to promote equity and optimize AI cooperation.} }
@inproceedings{10.1145/3644713.3644797, title = {Lecturers perceptions of using Artificial Intelligence in Tertiary Education in Uzbekistan}, booktitle = {Proceedings of the 7th International Conference on Future Networks and Distributed Systems}, pages = {570–578}, year = {2024}, isbn = {9798400709036}, doi = {10.1145/3644713.3644797}, url = {https://doi.org/10.1145/3644713.3644797}, author = {Shakib Kotamjani, Sedigheh and Shirinova, Sojida and Fahimirad, Mehrnaz}, abstract = {Artificial intelligence (AI) has revolutionized different aspects of society, including higher education. This paper investigates faculty members' perceptions of using artificial intelligence, chatbots, and generative AI in teaching and learning in Uzbeki's higher education contexts. The researchers employed a qualitative study using semi-structured interviews to collect the data. Purposeful sampling was employed to select participants in this study, and the interviews were conducted face-to-face at the university campus. The study's underlying theory is the Unified Theory of Acceptance and Use of Technology (UTAUT) model, including effort expectancy, performance expectancy, social influence, and facilitating conditions, which were employed as a lens to direct the research. The data were transcribed and analyzed using the deductive approach for the thematic analysis. The findings revealed that lecturers have a positive attitude to adopt and use AI for content creation, assessment and feedback, and doing research in their institution. Some instructors may see it as a valuable tool for generating creative content and aiding student learning. In contrast, others may have concerns about its potential to replace human creativity or biases in generated materials. Lecturers also view AI as a technology to achieve accessibility and equity after overcoming the challenges. Findings revealed that some measurements should be taken about the facilitating conditions and the perceived risks of using AI.} }
@article{10.1145/3709367, title = {Explainable Artificial Intelligence for Medical Applications: A Review}, journal = {ACM Trans. Comput. Healthcare}, volume = {6}, year = {2025}, doi = {10.1145/3709367}, url = {https://doi.org/10.1145/3709367}, author = {Sun, Qiyang and Akman, Alican and Schuller, Bj\"orn W.}, keywords = {Explainable Artificial Intelligence (XAI), Machine Learning, Literature Review, Medical Information System}, abstract = {The continuous development of artificial intelligence (AI) theory has propelled this field to unprecedented heights, owing to the relentless efforts of scholars and researchers. In the medical realm, AI takes a pivotal role, leveraging robust machine learning (ML) algorithms. AI technology in medical imaging aids physicians in X-ray, computed tomography (CT) scans, and magnetic resonance imaging (MRI) diagnoses, conducts pattern recognition and disease prediction based on acoustic data, delivers prognoses on disease types and developmental trends for patients, and employs intelligent health management wearable devices with human-computer interaction technology to name but a few. While these well-established applications have significantly assisted in medical field diagnoses, clinical decision-making, and management, collaboration between the medical and AI sectors faces an urgent challenge: How to substantiate the reliability of decision-making? The underlying issue stems from the conflict between the demand for accountability and result transparency in medical scenarios and the black-box model traits of AI. This article reviews recent research grounded in explainable artificial intelligence (XAI), with an emphasis on medical practices within the visual, audio, and multimodal perspectives. We endeavor to categorize and synthesize these practices, aiming to provide support and guidance for future researchers and healthcare professionals.} }
@inproceedings{10.1145/3744367.3744427, title = {A Study of the Use of Artificial Intelligence in Differentiated Instruction}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Educational Systems}, pages = {381–386}, year = {2025}, isbn = {9798400715068}, doi = {10.1145/3744367.3744427}, url = {https://doi.org/10.1145/3744367.3744427}, author = {Xu, Rong}, keywords = {Artificial Intelligence, Challenges, Differentiated Instruction, Pathways}, abstract = {The study builds an AI support framework based on Tomlinson's theory, and achieves accurate learning diagnosis through five-dimensional data (cognitive, behavioural, emotional, environmental and social). Practice has proven that AI can dynamically optimise teaching. However, there are challenges in AI application: over-reliance on technology undermines teacher-student interaction, algorithmic bias exacerbates educational injustice, and data privacy risks need to be regulated. The study emphasises that AI in education should be based on the essence of “educating people”, and promote the role of teachers to shift from “knowledge transfer” to “growth guidance” under the ethical framework, and synergise technological empowerment and humanistic care. The future needs to explore localised adaptation paths. In the future, it is necessary to explore the local adaptation path, not only to make use of AI to break through the bottleneck of large-scale education but also to prevent the alienation of the technology, and build an intelligent education ecology of “teaching in accordance with one's aptitude”.} }
@inproceedings{10.1145/3745238.3745395, title = {A Measurement Analysis of the Development of New Quality Productive Forces in Artificial Intelligence-Enabled Enterprises}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {999–1008}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745395}, url = {https://doi.org/10.1145/3745238.3745395}, author = {Lyu, Han and Wang, Kun}, keywords = {Artificial Intelligence, Entropy Approach, New Quality Productive Forces, Text Analysis}, abstract = {In spite of the fact that artificial intelligence(AI) has been broadly embraced as a key innovation over different areas, orderly experimental investigate on its particular components influencing the New Quality Productive Forces (NQPF) of ventures remains restricted. This paper draws on information from A-share recorded companies in Shanghai and Shenzhen between 2011 and 2022 to investigate the affect of AI on the NQPF of enterprises.The ponder to begin with builds an entropy-based assessment record framework that consolidates both labor and generation instruments to measure the NQPF of firms. Moreover, the level of AI appropriation inside a company is measured by the recurrence of AI-related watchwords showing up in its yearly reports. To test the speculations, the consider utilizes a fixed-effects show and finds a critical positive relationship between AI and the NQPF of undertakings. Advance approval through endogeneity and strength tests affirms the unwavering quality and strength of the results.Moreover, the investigation uncovers that the affect of AI on NQPF works through both coordinate and roundabout instruments, with noteworthy heterogeneous impacts over diverse sorts of firms. Based on these discoveries, the paper offers approach suggestions pointed at giving hypothetical experiences and down to earth direction for government organizations, corporate supervisors, and future scholarly investigate.} }
@inproceedings{10.1145/3745238.3745455, title = {Innovation and Practice: Artificial Intelligence Empowering Vocational English Teachers in Teaching Ability Competitions}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1392–1396}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745455}, url = {https://doi.org/10.1145/3745238.3745455}, author = {Tian, Xueqin and Yang, Ziyan and Chen, Fang}, keywords = {Artificial intelligence, Integration of Job Roles and Competitions in Teaching, Teaching Ability Competition for teachers, Vocational English}, abstract = {With the rapid development of artificial intelligence (AI) technology, its impact on higher vocational education has become increasingly significant, especially in the field of higher vocational English teaching. By analyzing the innovative cases of AI technology empowering teachers in teaching ability competitions, this paper reveals its potential and value in practical application. At the same time, this paper deeply probes into the specific data in the implementation process and proposes corresponding solutions to the problems encountered. It not only improves the technological content of the competition, but more importantly, through the deep integration of vocational English and artificial intelligence technology, it strengthens the students’ vocational English application ability and practical skills, and realizes the goal of “Competition and Teaching Integration” and “close integration of job requirements with the content of the competition”.} }
@inproceedings{10.1145/3603287.3651196, title = {Promise and Challenges of Generative AI in Healthcare Information Systems}, booktitle = {Proceedings of the 2024 ACM Southeast Conference}, pages = {223–228}, year = {2024}, isbn = {9798400702372}, doi = {10.1145/3603287.3651196}, url = {https://doi.org/10.1145/3603287.3651196}, author = {Ustymenko, Stanislav and Phadke, Abhishek}, keywords = {GPT, Healthcare IT, Large Language Models, Software Engineering}, abstract = {Large Language Models (LLMs) based on pretrained transformer architectures, such as Generative Pretrained Transformer 4 (GPT-4) from OpenAI, are on the cutting age of artificial intelligence research. Along with generating abundant academic literature, these models are the basis of numerous practical systems widely utilized by end users and organizations. In healthcare information systems, there are many case studies and research prototypes demonstrating the promise of applying GPT-like programs to numerous practical natural language processing tasks. At the same time, current limitations of LLMs prevent their safe deployments in professional environments. In this study, we give an overview of capabilities, limitations, and risks associated with current iterations of LLMs. We provide an overview of literature on using LLMs in healthcare context. Finally, we present a framework of generic healthcare IT system utilizing LLMs, and discuss avenues for future research.} }
@inproceedings{10.1145/3662739.3673681, title = {Application Research on Artificial Intelligence Generated Content in Architectural Design}, booktitle = {Proceedings of the 2024 International Conference on Machine Intelligence and Digital Applications}, pages = {671–680}, year = {2024}, isbn = {9798400718144}, doi = {10.1145/3662739.3673681}, url = {https://doi.org/10.1145/3662739.3673681}, author = {Zhang, Liwen and Tang, Mengting and Xu, Yaqi and Zhang, Chengfang}, keywords = {Artificial Intelligence generated content (AIGC), architectural design, design optimization}, abstract = {Computational design has profoundly influenced the development of the architecture industry over the past few decades, significantly enhancing the efficiency of architectural design and construction. This paper compares and studies the Mid Journey and Stable Diffusion AIGC image models to analyze their operational logic and construct a new architectural design process. Finally, the effectiveness and practicality of this process are validated through practical cases, and the future development prospects of AIGC in the field of architectural design are anticipated. This paper explores the use of Mid Journey and table Diffusion combined with generative AI text model to optimize prompt language, large model and Lora model, and Control Net-Stable Diffusion plugin and different AI software forms a complete AI-assisted architectural design process.} }
@inproceedings{10.1145/3726010.3726034, title = {Algorithm and application of artificial intelligence technology in interface image processing}, booktitle = {Proceedings of the 2024 International Conference on Artificial Intelligence, Digital Media Technology and Interaction Design}, pages = {165–169}, year = {2025}, isbn = {9798400712845}, doi = {10.1145/3726010.3726034}, url = {https://doi.org/10.1145/3726010.3726034}, author = {Zheng, Tong and Feng, Xinshuo}, keywords = {Artificial Intelligence, Computer algorithm, Image processing}, abstract = {With the integration of computer technology and human intelligence technology, various computing methods have been widely practiced in the field of image processing. The application and development of artificial intelligence in image processing is one of the hot spots in the field of science and technology. With the rapid development of information technology and the continuous improvement of computing power, artificial intelligence technology has begun to show strong application potential in the field of image processing. From traditional image processing methods to artificial intelligence-based image processing algorithms, artificial intelligence has become an important means in the field of image processing. Therefore, it is of great practical significance to explore the application and development of artificial intelligence in image processing. Based on this, combining with the technical principle of artificial intelligence algorithm in computer image processing, the paper analyzes the advantages of human intelligence algorithm in computer image processing and its price value, focusing on these four key algorithms[1]. This paper studies and discusses the application of the algorithms of "inheritance, optimization of particle group, leech ant, annealing" in image processing of human intelligence, aiming at providing reference for the development of human intelligence.} }
@inbook{10.1145/3730436.3730526, title = {Evaluation of Design Behavior Factors Weighting Based on AHP-Entropy Weighting Method in Artificial Intelligence Perspective}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Computational Intelligence}, pages = {548–552}, year = {2025}, isbn = {9798400713637}, url = {https://doi.org/10.1145/3730436.3730526}, author = {Liang, Jian}, abstract = {In the context of the information era of rapid development of artificial intelligence, the frequency of data updating and the role of data is always in dynamic change, and the research of design behavior is facing new opportunities and challenges. This paper focuses on the study of factors affecting design behavior in the view of artificial intelligence, aiming to analyze the weight of each influencing factor accurately. Through the hierarchical analysis method (AHP), its advantage of quantitative subjective judgment is brought into play, and experts' subjective perception of the importance of design behavior factors is deeply explored. At the same time, combined with the entropy weight method, the weights are determined based on the objective information entropy of the data, which effectively makes up for the lack of subjective assignment. Using the AHP-entropy weight method combined with these two methods, a comprehensive and systematic weight evaluation of design behavior factors is carried out to improve the efficiency of multi-attribute decision-making.} }
@inbook{10.1145/3745238.3745377, title = {Research on Artificial Intelligence - Assisted Financial Analysis Methods in Microgrid Project Investment Decisions}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {887–891}, year = {2025}, isbn = {9798400712791}, url = {https://doi.org/10.1145/3745238.3745377}, author = {Li, Qinze and Shi, Juncheng and Yang, Shuting and Cao, Junxian and Sun, Zhenghong and Li, Yaolin}, abstract = {Facing the globalization energy revolution, this paper proposes an intelligence analyzing system based on artificial neural network(ANN), genetic algorithm(GA) and fuzzy logic to solve the dynamic adapatability lack of the traditional financial analysis for microgrid investing, by setting up a database of more than 500 microgrid historical investing projects, and its financial performance forecasting neural network has achieved 0.0243 mean squared error(MSE).As can be seen, when a 23% improvement has been obtained in the accuracy of prediction between traditional linear regression and our hybrid model at 0243. Experimental results show that GA optimization module can improve the project net present value (NPV) by 6.7%(from ¥1.5 million to ¥1.6 million) within the budget constraint and increase renewable energy penetration to 75%. The fuzzy logic risk assessment model which is based on the 27 expert-defined rules, continues to maintain positive risk-adjusted NPV at ¥1.45 robustness gains can be achieved, with robust decisions found with greater than at 18% higher chance with our framework . The framework captures implicit factors such as equipment efficiency decay and energy price volatility, reducing the payback period from 5 years to 4.8 years and significantly improving portfolio risk resilience. .In this paper, a reasoning decision framework of investment recommendation in microgrid is conducted to realize comprehensible microgrid investment planning and decision-making system based on modular structure can be quickly and conveniently updated for multiple application regions and provide an important guidance for sustainable planning and investment of energy.} }
@inproceedings{10.1145/3663547.3759763, title = {Exploring Generative AI to Support Disability Service Professionals in Writing Image Descriptions for HCI Science Figures}, booktitle = {Proceedings of the 27th International ACM SIGACCESS Conference on Computers and Accessibility}, year = {2025}, isbn = {9798400706769}, doi = {10.1145/3663547.3759763}, url = {https://doi.org/10.1145/3663547.3759763}, author = {Iwamoto, Yugo and Raees, Muhammad and Heard, Jamison and Tigwell, Garreth W.}, keywords = {Alternative Text, Science Figures Accessibility, Writing Alt-Text, Screen Readers.}, abstract = {Alternative text (alt text) or image descriptions for scientific figures make them accessible to screen reader users. Yet, generating high-quality alt text remains a significant challenge, particularly for people lacking subject-matter expertise. Emerging AI tools, such as generative AI, can help people understand contexts where they lack subject expertise. This work explores how university Disability Services Office (DSO) professionals write alt text for scientific figures without subject expertise. We conducted a user study with 12 DSO professionals who authored alt text for scientific figures, first without and then using generative AI. We assessed the participants’ processes for alt text generation and their confidence in the resulting output. We also conducted semi-structured interviews to understand DSO professionals’ experiences with the alt text generation. Our findings reveal that AI assistance improved participants’ confidence and efficiency, but introduced new challenges for interaction and trust. Participants expressed caution about relying on AI-generated descriptions without sufficient domain knowledge and editing. These insights highlight the need for AI-augmented tools that better scaffold the alt text writing process for accessibility professionals.} }
@proceedings{10.1145/3756423, title = {ICAISM '25: Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing}, year = {2025}, isbn = {9798400714351} }
@inproceedings{10.1145/3706599.3720226, title = {BanglAssist: A Bengali-English Generative AI Chatbot for Code-Switching and Dialect-Handling in Customer Service}, booktitle = {Proceedings of the Extended Abstracts of the CHI Conference on Human Factors in Computing Systems}, year = {2025}, isbn = {9798400713958}, doi = {10.1145/3706599.3720226}, url = {https://doi.org/10.1145/3706599.3720226}, author = {Kruk, Francesco and Herath, Savindu and Choudhury, Prithwiraj}, keywords = {Generative AI, Chatbot, Code Switching, Dialect Handling, Multilingual Retrieval, Cross-Lingual Retrieval, Customer Service}, abstract = {In recent years, large language models (LLMs) have demonstrated exponential improvements that promise transformative opportunities across various industries. Their ability to generate human-like text and ensure continuous availability facilitates the creation of interactive service chatbots aimed at enhancing customer experience and streamlining enterprise operations. Despite their potential, LLMs face critical challenges, such as a susceptibility to hallucinations and difficulties handling complex linguistic scenarios, notably code switching and dialectal variations. To address these challenges, this paper describes the design of a multilingual chatbot for Bengali-English customer service interactions utilizing retrieval-augmented generation (RAG) and targeted prompt engineering. This research provides valuable insights for the human-computer interaction (HCI) community, emphasizing the importance of designing systems that accommodate linguistic diversity to benefit both customers and businesses. By addressing the intersection of generative AI and cultural heterogeneity, this late-breaking work inspires future innovations in multilingual and multicultural HCI.} }
@inproceedings{10.1145/3756423.3756523, title = {Smart Manufacturing and Artificial Intelligence Enabling New Energy Vehicle Production}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing}, pages = {609–613}, year = {2025}, isbn = {9798400714351}, doi = {10.1145/3756423.3756523}, url = {https://doi.org/10.1145/3756423.3756523}, author = {Wang, Zhenxing}, keywords = {3D sensingtechnology, artificial intelligence, digital twin, low-carbon manufacturing, new energy vehicle production, smart manufacturing}, abstract = {The new energy automobile industry faces the challenges of low manufacturing efficiency, high energy consumption and lack of flexibility. In this study, we propose the technical framework of “3D sensing + digital twin + edge computing”, which achieves 0.1mm inspection accuracy of highly reflective metal parts through the fusion of laser scanning and structured light imaging, and the defect leakage rate is reduced from 8% to 0.5%; reinforcement learning dynamically optimizes the energy consumption of coating process, and reduces the energy consumption of a single battery cell by 24%; spatiotemporal map convolutional network (STGCN) compresses the switching time of a vehicle to 18 minutes. Convolutional Network (ST-GCN) compresses model switching time to 18 minutes. Empirical studies show that Geely Xiangtan Base has reduced annual costs by 21 million RMB after applying the framework, which meets the carbon footprint traceability requirements of the European Union and pushes the industry towards the goal of “zero defects, zero carbon emissions and zero delays”.} }
@inproceedings{10.1145/3744367.3744372, title = {Real-time Optimization Research and Implementation of Personalized Learning Path Based on Multimodal Artificial Intelligence}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Educational Systems}, pages = {24–29}, year = {2025}, isbn = {9798400715068}, doi = {10.1145/3744367.3744372}, url = {https://doi.org/10.1145/3744367.3744372}, author = {Chen, Yanlun and Liu, Qian}, keywords = {Attention Mechanism, Knowledge Graph, Learning Path Optimization, Multimodal Artificial Intelligence, Personalized Learning, Reinforcement Learning}, abstract = {This research introduces a real-time optimization approach for personalized learning paths using multimodal artificial intelligence. It aims to resolve the conflict between personalized learning needs and large-scale teaching in higher education. The method employs multimodal deep learning models to perceive learners' behavioral characteristics comprehensively. It also develops a feature fusion algorithm based on the attention mechanism, which integrates multidimensional information such as text, video, and audio effectively. The system represents the relationships between learning contents using knowledge graphs and combines reinforcement learning technology to dynamically optimize learning paths. Experiments indicate that the system surpasses traditional methods in terms of feature extraction accuracy, knowledge mastery, and learning path optimization. Specifically, the feature extraction accuracy is 85.8%, and the knowledge point coverage reaches 89.7%. In practical teaching applications, the system enhances students' average learning efficiency by 35% and increases the knowledge mastery growth rate by 40%. This study offers new technical methods and practical references for the intelligent development of education.} }
@inproceedings{10.1145/3708394.3708450, title = {Artificial intelligence-based conversational exam model: a perspective for higher education}, booktitle = {Proceeding of the 2024 International Conference on Artificial Intelligence and Future Education}, pages = {325–330}, year = {2025}, isbn = {9798400710650}, doi = {10.1145/3708394.3708450}, url = {https://doi.org/10.1145/3708394.3708450}, author = {Li, Zhuang and Chu, Zheng and Zhang, Qingwei}, keywords = {Educational innovations, Examination mode, Multi-dimensional assessment, artificial intelligence (AI)}, abstract = {By analyzing the current situation of the application of artificial intelligence in college education, based on the educational assessment mode in college education at the current stage, analyzing the characteristics of the conversational examination mode, and proposing a conversational examination mode based on artificial intelligence. This model includes the examination system architecture, the question bank design, and the construction of the knowledge graph, as well as the intelligent scoring mechanism. It shows great development potential and application in the change of the examination mode in college education. The model demonstrates considerable potential for further development and application in the context of evolving educational assessment practices within the college and university setting. It is particularly well-suited to the demands of the digital age.} }
@inproceedings{10.1145/3708360.3708387, title = {Review on Artificial Intelligence Generated Content in Chinese Library Based on Data Visualization}, booktitle = {Proceedings of the 2024 International Conference on Mathematics and Machine Learning}, pages = {169–173}, year = {2025}, isbn = {9798400711657}, doi = {10.1145/3708360.3708387}, url = {https://doi.org/10.1145/3708360.3708387}, author = {Xie, Quanying and Chen, Lu and Bai, Meicheng}, keywords = {AIGC, Artificial Intelligence Generated Content, Chinese libraries, Data Visualization}, abstract = {Artificial Intelligence Generated Content has been deeply researched from the aspects of “smart library construction”, “smart transformation of knowledge service”, “metaverse library construction”, “Artificial Intelligence Generated Content technology and service mode reform” in the Chinese library field. In order to summary the current development progress of Artificial Intelligence Generated Content in the Chinese library field and provide some reference for the future development of Artificial Intelligence Generated Content in the field of library. The paper comprehensively makes use of bibliometrics and data visualization technology to visualize and analyze the research papers on the application of Artificial Intelligence Generated Content in the Chinese library field from four aspects ( including “Analysis of the year of publication papers”, “Analysis of Literature Type”, “Analysis of Research Strengths” and “Analysis of the Research Hotspot” ), and looking forward to the future research content that needs to be strengthened in the Chinese library field of Artificial Intelligence Generated Content from two aspects( including “Strengthen literacy education of Artificial Intelligence Generated Content” and “Research on enhancing the deep integration of Artificial Intelligence Generated Content and library.” ).} }
@inproceedings{10.1145/3729176.3729185, title = {Cleared for Takeoff - Artificial Intelligence Acceptance in Air Traffic Control}, booktitle = {Proceedings of the 4th Annual Symposium on Human-Computer Interaction for Work}, year = {2025}, isbn = {9798400713842}, doi = {10.1145/3729176.3729185}, url = {https://doi.org/10.1145/3729176.3729185}, author = {Schon, Jasper and Bruder, Carmen and Liebherr, Magnus and Theis, Sabine}, keywords = {ATC, artificial intelligence, technology acceptance, HCI, empirical study, emotion}, abstract = {With the increasing complexity and volume of air traffic, there is growing interest in utilizing artificial intelligence (AI) support to alleviate ATCOs’ workload. Despite this potential, AI adoption in air traffic control (ATC) has been limited, partly due to low user acceptance. This study, grounded in the Technology Acceptance Model (TAM), the AI Acceptance Model (AIAM), and the Technology Acceptance in the Working Environment (TAWE) model, investigates the factors influencing AI acceptance among ATCOs. Using data from an online survey of 83 ATCOS, our model explains 78.5% of the variance in the intention to use AI. Notably, technology trust (TT) and emotion (EM) significantly influence the intention to use AI, with TT mediating the impact of perceived transparency (PT). Findings emphasize the need to address emotional and functional aspects to foster AI acceptance in ATC, marking a crucial step toward its improvement.} }
@inproceedings{10.1145/3640457.3691703, title = {KGGLM: A Generative Language Model for Generalizable Knowledge Graph Representation Learning in Recommendation}, booktitle = {Proceedings of the 18th ACM Conference on Recommender Systems}, pages = {1079–1084}, year = {2024}, isbn = {9798400705052}, doi = {10.1145/3640457.3691703}, url = {https://doi.org/10.1145/3640457.3691703}, author = {Balloccu, Giacomo and Boratto, Ludovico and Fenu, Gianni and Marras, Mirko and Soccol, Alessandro}, keywords = {Generative Artificial Intelligence., Knowledge Completion, Knowledge Graph, Knowledge Graph Embeddings, Knowledge Representation Learning, Language Model, Recommendation}, abstract = {Current recommendation methods based on knowledge graphs rely on entity and relation representations for several steps along the pipeline, with knowledge completion and path reasoning being the most influential. Despite their similarities, the most effective representation methods for these steps differ, leading to inefficiencies, limited representativeness, and reduced interpretability. In this paper, we introduce KGGLM, a decoder-only Transformer model designed for generalizable knowledge representation learning to support recommendation. The model is trained on generic paths sampled from the knowledge graph to capture foundational patterns, and then fine-tuned on paths specific of the downstream step (knowledge completion and path reasoning in our case). Experiments on ML1M and LFM1M show that KGGLM beats twenty-two baselines in effectiveness under both knowledge completion and recommendation. Source code and pre-processed data sets are available at https://github.com/mirkomarras/kgglm.} }
@inbook{10.1145/3745238.3745370, title = {Design of an automation modification of a TV backplane traversing module in the context of artificial intelligence}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {841–845}, year = {2025}, isbn = {9798400712791}, url = {https://doi.org/10.1145/3745238.3745370}, author = {Wu, Renjun and Chen, Yuexiang and Hou, Bolin and Wei, Guojun and Wang, Jing and Lv, Zhe and Hu, Yinfeng and Wei, Yi and Zhang, Fangyang}, abstract = {This study deals with the automation of TV backplane transversal modules and proposes an innovative automation solution by integrating digital economy and artificial intelligence (AI) technologies. By analysing the limitations of existing manually operated systems, this study designs an automation system that incorporates AI algorithms to achieve an intelligent and data-driven production process. The study covers automation system design, key component selection, intelligent control system development and performance optimisation. Through theoretical analysis and experimental validation, the modified automation system significantly improves productivity and product quality, while reducing labour costs and operational risks. In addition, this study explores the role of automation transformation in promoting the digital economy, providing new insights into the intelligent transformation of the home appliance manufacturing industry. The research results provide a theoretical basis and practical guidance for the automation upgrading of TV backplane transversal modules, which is of great significance for improving the intelligent level of the home appliance manufacturing industry and promoting the development of the digital economy.} }
@inbook{10.1145/3756423.3756560, title = {A Study on the Application of Artificial Intelligence in Digital Twin Monitoring of Building Structural Health}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing}, pages = {833–837}, year = {2025}, isbn = {9798400714351}, url = {https://doi.org/10.1145/3756423.3756560}, author = {Pan, Yue}, abstract = {With the continuous expansion of urban building volume and the significant increase of operational complexity, the monitoring of the health status of building structures has become a key means to ensure urban safety and sustainable operation of buildings. This paper focuses on the integration and application of artificial intelligence and digital twin technology in building structural health monitoring, and systematically constructs an intelligent monitoring system architecture driven by multi-source sensing data, with AI model as the core and digital twin platform as the carrier. Through the design of multi-dimensional sensing network, the dynamic perception of stress, vibration, displacement and other behaviors of key building components is realized; combined with AI models such as convolutional neural network (CNN), graphical neural network (GNN), and long and short-term memory network (LSTM), the ability of structural state identification, damage prediction and trend modeling is enhanced; and three-dimensional visualization and interactive display of the structural state of health is realized by relying on the BIM platform. The article selects a number of typical engineering scenarios such as high-rise steel structure buildings, underground hubs, and protective buildings to carry out experiments and case studies, and verifies the significant advantages of the AI-driven digital twin system in terms of real-time, accuracy, and adaptability.} }
@proceedings{10.1145/3730436, title = {AICI '25: Proceedings of the 2025 International Conference on Artificial Intelligence and Computational Intelligence}, year = {2025}, isbn = {9798400713637} }
@inproceedings{10.1145/3706598.3714146, title = {Exploring Multimodal Generative AI for Education through Co-design Workshops with Students}, booktitle = {Proceedings of the 2025 CHI Conference on Human Factors in Computing Systems}, year = {2025}, isbn = {9798400713941}, doi = {10.1145/3706598.3714146}, url = {https://doi.org/10.1145/3706598.3714146}, author = {Prasad, Prajish and Balse, Rishabh and Balchandani, Dhwani}, keywords = {artificial intelligence, generative AI, large language models, multimodality, co-design, design principles, learning environment}, abstract = {Multimodal large language models (MLLMs) are Generative AI models that take different modalities such as text, audio, and video as input and generate appropriate multimodal output. Since such models will be integrated into future educational tools, a human-centered design approach that takes students’ perspectives into account is essential while designing such applications.This paper describes two co-design workshops which were conducted with 79 student groups to examine how they design and prototype future educational tools integrated with MLLMs. Through various activities in the workshops, students discussed relevant educational problems, created journey maps, storyboards and low fidelity prototypes for their applications, and evaluated their applications based on relevant design principles. We found that students’ applications used MLLMs for important learning environment design features such as multimodal content creation, personalization, and feedback. Based on these findings, we discuss future research directions for the design of multimodality in generative AI educational applications.} }
@inproceedings{10.1145/3711403.3711481, title = {A Research-Oriented Model for Artificial Intelligence Education: Integrating Multidisciplinary Approaches to Foster Innovation and Holistic Learning}, booktitle = {Proceedings of the 2024 7th International Conference on Educational Technology Management}, pages = {480–485}, year = {2025}, isbn = {9798400717468}, doi = {10.1145/3711403.3711481}, url = {https://doi.org/10.1145/3711403.3711481}, author = {Li, Liang and Huang, Weihua and Liu, Bin}, keywords = {Artificial intelligence education, Curriculum design, Innovation capability, Interdisciplinary integration, Research-oriented perspective}, abstract = {With the rapid advancement of technology, artificial intelligence (AI) has become an indispensable part of various industries. To keep pace with this trend, higher education institutions must cultivate AI professionals with interdisciplinary thinking and research capabilities. This paper, based on the “Double First-Class” program construction in Chinese universities, explores an AI course education model within the context of interdisciplinary integration. By analyzing existing issues in current AI course education, we propose a research-oriented perspective to build a comprehensive interdisciplinary knowledge integration system and establish a collaborative platform that bridges both in-class and external connections. This approach is intended to enhance students’ innovative and problem-solving abilities. By combining theory with practice, this paper advocates for deep reforms in AI courses for graduate students, aiming to improve their overall quality and cultivate “passionate, technology-driven, high-quality” talent for society.} }
@inproceedings{10.1145/3760269.3760290, title = {Research on the Application of Artificial Intelligence in Ship Lifecycle Assurance}, booktitle = {Proceedings of the 2025 5th International Conference on Automation Control, Algorithm and Intelligent Bionics}, pages = {136–140}, year = {2025}, isbn = {9798400714313}, doi = {10.1145/3760269.3760290}, url = {https://doi.org/10.1145/3760269.3760290}, author = {Cao, Li and Shen, Jie and Bian, Dawei}, keywords = {Artificial Intelligence (AI), Data Analysis, Design Optimization, Operational Efficiency, Predictive Maintenance, Ship Lifecycle Assurance}, abstract = {This study examines the practical integration of artificial intelligence (AI) throughout ship lifecycles, covering design, construction, and operational maintenance. Using data analysis approaches, intelligent optimization and simulation methods, automated manufacturing processes, and predictive maintenance techniques, the research demonstrates how these technologies can improve design quality, construction efficiency, and operational reliability. We analyze specific solutions—including intelligent monitoring systems, enhanced fault diagnosis accuracy, and digital quality control—to evaluate their impact on transforming conventional shipbuilding practices. The results propose implementation pathways for the industry's intelligent transition, offering practical solutions for achieving lifecycle reliability and sustainable shipping operations.} }
@inproceedings{10.1145/3745238.3745332, title = {Research on the Imbalance of China's Housing Market from the Perspective of Artificial Intelligence}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {596–602}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745332}, url = {https://doi.org/10.1145/3745238.3745332}, author = {Zhou, Ying and Song, Hongling}, keywords = {Artificial Intelligence, Housing market, Hyperbolic aggregation equation, Non-equilibrium theory}, abstract = {Artificial intelligence has been deeply integrated into all areas of social production. At present, China's housing market has long faced the problems of supply-demand imbalance and structural imbalance, and there is an urgent need to use the technological advantages of AI to promote the intelligent transformation of supply-demand balance management. This paper constructs a theoretical model of imbalance from a national perspective, measures the scale of housing supply and demand using hyperbolic aggregation equations, quantifies the degree of regional supply-demand imbalance, and puts forward policy proposals to regulate the imbalance in the housing market by combining with AI technology, which provides a new way of thinking and a practical way for the government to realize intelligent management of the housing market.} }
@inbook{10.1145/3724504.3724635, title = {Research on the Application of Artificial Intelligence in Basic Education: A Global Trend Analysis Based on Bibliometrics}, booktitle = {Proceedings of the 2024 2nd International Conference on Information Education and Artificial Intelligence}, pages = {799–806}, year = {2025}, isbn = {9798400711732}, url = {https://doi.org/10.1145/3724504.3724635}, author = {He, Hao and Gao, Chaobang}, abstract = {Based on the bibliometric method, this study visually analyzed the application research literature of artificial intelligence (AI) in the domain of basic education in the Web of Science Core Collection from January 1, 2014 to November 1, 2024 with the help of VOSviewer and CiteSpace. A total of 344 valid articles were selected from 588 institutions and 1090 authors in 60 countries. They were published in 126 journals and cited 13855 references from 6922 journals. The study found that the number of papers published in China is the largest, and the quality recognition of papers in the United States is high. The journals with the greatest number of articles were mostly related to educational technology and computational intelligence, and Computers &amp; Education had the greatest number of citations per article. Keyword co-occurrence analysis shows that the study focuses on deep learning and the application of large language models (LLMs) has increased. The timeline analysis shows that the domain research has moved from the stage of data driven and educational technology application exploration to the stage of AI empowerment and personalized education. The co-citation analysis reveals that the interdisciplinary characteristics of educational technology research are obvious, and the literature co-citation network reflects the multi-dimensional application research type of AI in basic education. All in all, the popularity of AI application research in basic education is increasing, the research focus has changed, and generative AI is developing rapidly.} }
@inbook{10.1145/3724504.3724523, title = {Artificial Intelligence Based Teaching Reform of C Language with High Proportion of Overseas Students}, booktitle = {Proceedings of the 2024 2nd International Conference on Information Education and Artificial Intelligence}, pages = {107–111}, year = {2025}, isbn = {9798400711732}, url = {https://doi.org/10.1145/3724504.3724523}, author = {Ke, Yiming and Ke, Yuanzheng and Li, Wei and Liu, Fangtong}, abstract = {The internationalization of higher education is the key measures in the strategy of rejuvenating the country through science and education. It is the route one must take to promote the quality and efficiency of higher education. The number of overseas students is one of critical indexes to evaluate the internationalization of higher education. However, with the increasing proportion of overseas students, how to balance the contradiction between personalized and generalized education has become the focus of current curriculum teaching reform. This study selects Jinan University, the highest institution for overseas Chinese in China, as the research location, and treats the C language course for electrical engineering and automation major as a case. The differences in learning foundation and training targets between mainland and overseas students are discussed. Furthermore, a six-step teaching reform framework using artificial intelligence technology is proposed. It not only comprehensively analyzes the key points in teaching, but also uses artificial intelligence technology to compensate the difficulty in meeting students' personalized needs in large courses, which may provide reference for educators and lecturers.} }
@inproceedings{10.1145/3747912.3747914, title = {A Novel Artificial Intelligence Voice Electronic Medical Record Based on Blockchain}, booktitle = {Proceedings of the 2025 International Conference on Software Engineering and Computer Applications}, pages = {12–19}, year = {2025}, isbn = {9798400715136}, doi = {10.1145/3747912.3747914}, url = {https://doi.org/10.1145/3747912.3747914}, author = {Xie, Weiming and Yao, Zhaomin and Bai, Xiaozhou and Mai, Lang and Zhan, Ying and Wu, Xiaodan and Dai, Yingxin and Pei, Yusong and Zhang, Guoxu and Wang, Zhiguo}, keywords = {Artificial Intelligence, Blockchain, Digital Healthcare, Electronic Medical Records, Speech Recognition}, abstract = {Nowadays, the global digital transformation of healthcare is advancing rapidly with the help of technologies such as electronic medical records, telemedicine, and mobile medical applications. However, there are still challenges in EMR interoperability, security, and data exchange. To address these existing limitations, This study proposes a voice electronic medical record system driven by artificial intelligence and blockchain, which is designed to improve clinical records and nursing coordination. This system adopts a dedicated deep learning architecture. It transcribe the conversations between doctors and patients into text, and then uses natural language processing to extract the relevant medical information. At the same time, it also provides diagnostic prompts, which can reduce the risk of misdiagnosis. Doctors can view and edit these summaries generated by artificial intelligence. Then safely record them on the decentralized crypto blockchain ledger. With federated learning, the model can be continuously improved in multiple centers without infringing on data privacy. This solution integrates automatic speech recognition, distributed ledger technology, and collaborative deep learning, aiming to enhance the EMR efficiency, security, data integrity, and care continuity of medical institutions. The combination of blockchain technology and artificial intelligence technology holds great potential. It can transform fragmented health data into portable and interoperable records under patient control, thus bringing strategic advantages to the health system that is undergoing a comprehensive digital transformation.} }
@inproceedings{10.1145/3673805.3673828, title = {ChatGPT in education: Teachers’ and Students’ views}, booktitle = {Proceedings of the European Conference on Cognitive Ergonomics 2024}, year = {2024}, isbn = {9798400718243}, doi = {10.1145/3673805.3673828}, url = {https://doi.org/10.1145/3673805.3673828}, author = {S\"oderstr\"om, Ulrik and Hedstr\"om, Elsa and Lambertsson, Karl and Mejtoft, Thomas}, keywords = {ChatGPT, Education, Generative Artificial Intelligence, Higher education, Perceived Learning, Sweden, Upper Secondary School}, abstract = {Since its launch just over two years ago, the conversational chatbot ChatGPT developed by OpenAI has become integrated into the studies of students within the Swedish educational system. This paper investigates both teachers’ perspectives and attitudes toward students using ChatGPT and the students’ view of their perceived learning. Insights were gathered through questionnaires (for both students and teachers) and a learning session for students, revealing concerns and enthusiasm regarding ChatGPT’s integration. Teachers express a lack of understanding on incorporating ChatGPT into education and perceive a lack of support from school leadership and the Swedish National Agency for Education. Identified scenarios suggest ChatGPT’s potential for understanding concepts, but resistance toward its use in examinations. The students have a statistically significant enhancement in confidence and understanding of magnetism concepts after engaging with ChatGPT. This research contributes to ongoing discussions about integrating AI tools in education, emphasizing benefits and addressing ethical concerns and learning outcomes.} }
@inproceedings{10.1145/3745238.3745384, title = {Research on the Application of Artificial Intelligence Technology from the Perspective of Low-cost Development of Equipment}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {931–938}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745384}, url = {https://doi.org/10.1145/3745238.3745384}, author = {Li, Jijun and Zhang, Ruijin and Li, Chujun and Huang, Zichen}, keywords = {Low-cost device development, artificial intelligence technology, combat effectiveness, full life cycle}, abstract = {The core of low-cost equipment development is to reduce equipment costs by optimising resource allocation while ensuring equipment performance and combat capability. This paper discusses in detail the application of artificial intelligence technology at all stages of the equipment life cycle to reduce costs and improve efficiency, which provides a reference for realising low-cost equipment development and improving the quality and efficiency of national defence construction.} }
@inproceedings{10.1145/3718491.3718523, title = {A Review of the Application and Development of Artificial Intelligence Technology in Museums}, booktitle = {Proceedings of the 4th Asia-Pacific Artificial Intelligence and Big Data Forum}, pages = {187–193}, year = {2025}, isbn = {9798400710865}, doi = {10.1145/3718491.3718523}, url = {https://doi.org/10.1145/3718491.3718523}, author = {Liu, Jun}, keywords = {AI Art, Artificial Intelligence, Museums, Virtual Museums, Virtual Reality}, abstract = {With the rapid development of artificial intelligence (AI) technology, its applications in museums have become increasingly widespread. However, systematic discussions of its specific applications and future development trends remain limited. Based on literature analysis from CNKI, Scopus, and Google Scholar databases, this study systematically examines the current applications of AI in museums, identifies existing challenges and solutions, and explores future development trends. The findings reveal that AI technology has significantly enhanced museum visitor experiences by providing personalized, immersive, and accessible diversified services. In terms of operational management, AI demonstrates notable advantages in cost control and efficiency optimization, effectively improving museum resource utilization. Furthermore, AI promotes the integration of culture and technology, innovation in education and research, and interdisciplinary collaboration. This study provides theoretical support for AI applications in the museum sector, helping researchers, funding agencies, and practitioners understand current status and development directions.} }
@inbook{10.1145/3756423.3756568, title = {Standardized Use of English Terminology in AI and Smart Manufacturing: Challenges, Strategies, and Practices}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing}, pages = {877–882}, year = {2025}, isbn = {9798400714351}, url = {https://doi.org/10.1145/3756423.3756568}, author = {Zhang, Yiyang}, abstract = {This paper systematically discuss the English standardization in artificial intelligence (AI) and smat manunfacturing, focuses on related problems, means and practises. Aimed at the AI and smart manufacturing's development in large volume and speed, English standard terminology could make international cooperation, information communication and industry standard more effective. There are main difficulties include the multidenoting and the obscurity of words, the rate new jargon, the confusion difference of industry and in language-culture difficulties, particularly language not English.For these challenges, the paper suggests ways: to establish authoritative terminology databases (i.e., ISO/TC 37) that regularly updates their definitions and use for latest version; to intensify collaborations of global and national standard bodies to create one single norm, develop one single standard to define their terminologies usage, to design for the rules of translation to reflect consistent terminologies use, and professionals’ terminology literacy in using terms. Three cases from authors show how those approaches help cross-industry and cross-cultural communications, including use of digital twin to advance the development of aircrafts, automotive, and automobile production in detail.Our research attempt combining linguistics, artificial intelligence, and manufacturing engineering in the presentation of not only theoretical foundations but also practical references for assisting effective international cooperation and normalized development of AI and smart manufacturing. In the further study, we expand the case of study on larger ranges of cultures and industrial fields in order to realize more effective terminology standardization for the current emerging technologies.} }
@article{10.1145/3711837, title = {LLMs and Their Applications in Medical Artificial Intelligence}, journal = {ACM Trans. Manage. Inf. Syst.}, volume = {16}, year = {2025}, issn = {2158-656X}, doi = {10.1145/3711837}, url = {https://doi.org/10.1145/3711837}, author = {Mao, Wenji and Qiu, Xipeng and Abbasi, Ahmed}, keywords = {Large language models, LLMs, medical AI, health, artificial intelligence}, abstract = {Medical artificial intelligence (AI) is a cross-disciplinary field focused on developing advanced computing and AI technologies to benefit medicine and healthcare. Globally, medical AI has tremendous potential to support the United Nations’ sustainable development goals pertaining to health and well-being. In particular, large language models (LLMs) afford opportunities for positively disrupting medical AI-related research and practice. We present a research framework for LLMs in medical AI. Our framework considers the interplay between health and well-being goals, disease lifecycle stages, and the important emerging role of LLMs in medical AI processes related to various lifecycle stages. As part of our framework, we describe the LLM multiplex—important multimodal, multi-model, multicultural, and multi-responsibility considerations for LLMs in medical AI. We discuss how the five articles in the special issue relate to this framework and are helping us learn about the opportunities and challenges for LLMs in medical AI.} }
@inproceedings{10.1145/3712623.3712664, title = {Research on the Application of Artificial Intelligence in Scenic Mobility Scooter Context-Aware Design Methodology}, booktitle = {Proceedings of the 2024 2nd International Conference on Advances in Artificial Intelligence and Applications}, pages = {238–244}, year = {2025}, isbn = {9798400712883}, doi = {10.1145/3712623.3712664}, url = {https://doi.org/10.1145/3712623.3712664}, author = {Liu, Liping and Zhang, Jie and Yang, Jiayi and Cai, Yucao}, keywords = {Artificial Intelligence, Context awareness, Design methods, Scenic mobility scooter}, abstract = {In order to explore the design application of context-awareness in the scenic mobility scooter under the background of artificial intelligence. Firstly, the concepts of artificial intelligence, context-awareness and scenic mobility scooter are introduced, and the current status of theoretical research and existing problems of scenic mobility scooter are sorted out. Secondly, combining the advantages and characteristics of context-aware intervention in the scenic scooter, the context information related to the scenic scooter under the view of artificial intelligence is divided into four categories: user, environment, task, and equipment, and at the same time, different stages of the use of the context information hierarchy is categorized and corresponds to the pain point needs, and the corresponding artificial intelligence access points are analyzed. Finally, four design strategies are proposed based on the above four contextual factors. It is concluded that the application of context-awareness theory to the design of scenic mobility scooters under the perspective of artificial intelligence strengthens the ability to perceive multidimensional contextual information, explores the user's needs in dynamic situations from a multidimensional perspective, simplifies the user's operation path, and enhances the user's touring experience.} }
@inproceedings{10.1145/3715275.3732020, title = {Robustness and Cybersecurity in the EU Artificial Intelligence Act}, booktitle = {Proceedings of the 2025 ACM Conference on Fairness, Accountability, and Transparency}, pages = {283–295}, year = {2025}, isbn = {9798400714825}, doi = {10.1145/3715275.3732020}, url = {https://doi.org/10.1145/3715275.3732020}, author = {Nolte, Henrik and Rateike, Miriam and Finck, Mich\`ele}, keywords = {Robustness, Cybersecurity, AIA, Regulation, European Union, EU law}, abstract = {The EU Artificial Intelligence Act (AIA) establishes different legal principles for different types of AI systems. While prior work has sought to clarify some of these principles, little attention has been paid to robustness and cybersecurity. This paper aims to fill this gap. We identify legal challenges and shortcomings in provisions related to robustness and cybersecurity for high-risk AI systems (Art.&nbsp;15&nbsp;AIA) and general-purpose AI models (Art.&nbsp;55&nbsp;AIA). We show that robustness and cybersecurity demand resilience against performance disruptions. Furthermore, we assess potential challenges in implementing these provisions in light of recent advancements in the machine learning (ML) literature. Our analysis informs efforts to develop harmonized standards, guidelines by the European Commission, as well as benchmarks and measurement methodologies under Art.&nbsp;15(2)&nbsp;AIA. With this, we seek to bridge the gap between legal terminology and ML research, fostering a better alignment between research and implementation efforts.} }
@inproceedings{10.1145/3672758.3672763, title = {Development and Research of Generative Animation Based on AIGC}, booktitle = {Proceedings of the 3rd International Conference on Computer, Artificial Intelligence and Control Engineering}, pages = {22–28}, year = {2024}, isbn = {9798400716942}, doi = {10.1145/3672758.3672763}, url = {https://doi.org/10.1145/3672758.3672763}, author = {Peng, Li and Fu, Bihan}, abstract = {AIGC is the automatic generation of relevant content from a large number of models and databases based on the input of relevant instructions. This paper mainly discusses the development of artificial intelligence generation technology in animation in the form of images. With the rapid development of artificial intelligence technology, generative animation content has become an important way of animation production and production in modern society. Artificial intelligence-based generative animation is taking up an increasing proportion of the film, game, advertising and other industries, AIGC has been rapidly developed by virtue of its lower production cost and higher production efficiency, which constantly impacts the development of the animation industry and attracts the widespread attention of animation creators. This paper analyses the Generative Adversarial Network and Diffusion Model through the deep learning principle of AIGC technology, and explores the important impact of AIGC technology on the form of generative animation. Using computer discipline thinking to analyse the type of AIGC model for animation creation, and according to the model simulation results of AIGC technology used in generative animation production of technology, market aspects of its development prospects are summarised.} }
@inproceedings{10.1145/3736251.3757101, title = {Developing Young Children Artificial Intelligence Literacy through Community-Based Approaches}, booktitle = {Proceedings of the ACM Global on Computing Education Conference 2025 Vol 2}, pages = {419}, year = {2025}, isbn = {9798400719424}, doi = {10.1145/3736251.3757101}, url = {https://doi.org/10.1145/3736251.3757101}, author = {Mabberi, Enock}, keywords = {artificial intelligence literacy, computational thinking, informal learning spaces, k-12 education, machine learning}, abstract = {This study contributes to the global conversation on artificial intelligence (AI) literacy, with a specific focus on Uganda, a sub-Saharan African region that has received limited attention in AI education research. As AI becomes increasingly pervasive, it is essential to introduce AI concepts to young children to promote their AI literacy. However, the current landscape of AI education in Uganda is not well understood. To address this gap, this research explores a community-based approach to teaching AI and machine learning (ML) to young children in out-of-school settings. The study aims to discover the kinds of AI-related knowledge and skills that young children can develop and how they can learn about AI in a non-formal educational context. The study will employ a design science methodology (DSM) to create and evaluate practical solutions, including methods, models, and concepts, that can inform AI education in Uganda. By combining systematic literature reviews, qualitative and quantitative methods, the research will generate robust empirical evidence that supports the effectiveness and relevance of the proposed solutions. Through these methods, the research will explore the role of community-based approaches in fostering young children's AI literacy. The study will investigate the effectiveness of informal learning environments, including homes, museums, churches, and children's camps. This will provide design recommendations for community-based AI education programs and in doing so, the study addresses the limited initiatives on promoting AI education for everyone, particularly in developing regions using informal settings.} }
@article{10.1145/3712016, title = {Artificial Intelligence as a Service (AIaaS) for Cloud, Fog and the Edge: State-of-the-Art Practices}, journal = {ACM Comput. Surv.}, volume = {57}, year = {2025}, issn = {0360-0300}, doi = {10.1145/3712016}, url = {https://doi.org/10.1145/3712016}, author = {Syed, Naeem and Anwar, Adnan and Baig, Zubair and Zeadally, Sherali}, keywords = {Artificial intelligence, artificial intelligence as a service, AIaaS, cloud computing, edge, IoT}, abstract = {Artificial Intelligence (AI) fosters enormous business opportunities that build and utilize private AI models. Implementing AI models at scale and ensuring cost-effective production of AI-based technologies through entirely in-house capabilities is a challenge. The success of the Infrastructure as a Service (IaaS) and Software as a Service (SaaS) Cloud Computing models can be leveraged to facilitate a cost-effective and scalable AI service paradigm, namely, “AI as a Service” (AIaaS). We summarize current state-of-the-art solutions for AIaaS, and we discuss its prospects for growth and opportunities to advance the concept. To this end, we perform a thorough review of recent research on AI and various deployment strategies for emerging domains considering both technical as well as survey articles. Next, we identify various characteristics and capabilities that need to be met before an AIaaS model can be successfully designed and deployed. Based on this, we present a general framework of an AIaaS architecture that integrates the required aaS characteristics with the capabilities of AI. We also compare various approaches for offering AIaaS to end users. Finally, we illustrate several real-world use cases for AIaaS models, followed by a discussion of some of the challenges that must be addressed to enable AIaaS adoption.} }
@inproceedings{10.1145/3646548.3672584, title = {Modelling Engineering Processes in Natural Language: A Case Study}, booktitle = {Proceedings of the 28th ACM International Systems and Software Product Line Conference}, pages = {170–178}, year = {2024}, isbn = {9798400705939}, doi = {10.1145/3646548.3672584}, url = {https://doi.org/10.1145/3646548.3672584}, author = {Kogler, Philipp and Chen, Wei and Falkner, Andreas and Haselb\"ock, Alois and Wallner, Stefan}, keywords = {Domain-specific Languages, Generative Artificial Intelligence, Large Language Models, Process Management, Process Modelling, Reliable Code Generation}, abstract = {Engineering process management aims to formally specify processes which are executable, measurable, and controllable. Common representations include text-based domain-specific languages (DSLs) or graphical notations such as the Business Process Modelling Notation (BPMN). The specification itself can be seen as a Software Product Line (SPL), building upon concepts such as tasks, UI forms, fields and actions. Domain experts provide requirements for processes but often lack the technical programming skills to formalize them in a process specification language. We present an interactive SPL application prototype that allows domain experts to model simple processes in natural language. Our framework for the reliable generation of formal specifications with Large Language Models (LLMs) supports the machine-translation from natural language to a JSON-based process DSL. In this case study, five domain experts were asked to model any process of their choice through natural-language interactions. As a result, the user interface corresponding to the process DSL was shown as immediate feedback. We documented their perceived translation quality and interviewed them on their impressions of this methodology. An average user-assessed performance rating of 68% was achieved. Even though the modelling strategies differed greatly between individuals, the tool was able to adequately capture the majority of instructions, leaving an overall positive impression on the participants. More context awareness and additional conventional interaction elements were the main aspects found to be improved for a productive implementation.} }
@inbook{10.1145/3724504.3724637, title = {Artificial Intelligence in Educational Systems: An Assessment of Applicative Technologies and Innovations}, booktitle = {Proceedings of the 2024 2nd International Conference on Information Education and Artificial Intelligence}, pages = {814–820}, year = {2025}, isbn = {9798400711732}, url = {https://doi.org/10.1145/3724504.3724637}, author = {Wang, Zhong}, abstract = {The research explores the applications of artificial intelligence (AI) applications in the education. The role of AI as mobile learning technology, blended learning technology and personalized learning technology has been studied in this paper. These learning techniques with the integration of AI technology enhance prediction of usefulness of teaching methods and increase the interaction between students and their teachers. This paper identifies some challenges and limitation of using AI in education. Little access to internet, and resource allocation for diverse requirements of a learning management system are identified as key problems of AI based educational systems. This study emphasizes the improvement of teachers' digital literacy, including the cultivation of AI integration ability, data analysis ability and innovative thinking, so as to meet the challenges brought by rapid technology iteration, such as the difficulty of technology integration, the need for updating teaching concepts and the differentiated management of students.} }
@article{10.1145/3757600, title = {FamilyDittos: Reimagining Intergenerational Interaction through Mimetic Agents}, journal = {Proc. ACM Hum.-Comput. Interact.}, volume = {9}, year = {2025}, doi = {10.1145/3757600}, url = {https://doi.org/10.1145/3757600}, author = {Tanprasert, Teerapaun and Dai, Jiamin and Tang, John and Inkpen, Kori and Cutrell, Edward and McGrenere, Joanna}, keywords = {embodied agents, family, generative artificial intelligence, human-ai interaction, intergenerational communication, remote interaction}, abstract = {Technology-mediated communication tools are widely used to maintain connections between older adults and their remote family members. However, challenges often arise due to differences in their life rhythms and communication preferences, such as time zones, daily routines, or preferred platforms. To address these challenges, we propose using Ditto, a mimetic embodied agent, in video-call-like interactions between an older adult and a remote family member. When direct interaction is difficult, FamilyDitto can represent either party, providing a strong social presence and a personalized experience to the other person. To explore the potential of Ditto in supporting intergenerational communication, we conducted seven co-design workshops (n=27) with older adults and younger family members. Our thematic analysis reveals Ditto's potential roles as both a temporal bridge and an emotional proxy, identifies the tension between faithful and idealized representations, and emphasizes the importance of personalization to support unique family dynamics. We distill our findings into implications for designing mediated semi-synchronous communication, using idealized representation responsibly, and addressing the asymmetric motivations and comfort levels with Ditto across roles and ages. This study provides a foundation for mimetic AI technologies that enhance, rather than replace, human connection in remote intergenerational relationships.} }
@article{10.1145/3690639, title = {Artificial Intelligence of Things: A Survey}, journal = {ACM Trans. Sen. Netw.}, volume = {21}, year = {2025}, issn = {1550-4859}, doi = {10.1145/3690639}, url = {https://doi.org/10.1145/3690639}, author = {Siam, Shakhrul Iman and Ahn, Hyunho and Liu, Li and Alam, Samiul and Shen, Hui and Cao, Zhichao and Shroff, Ness and Krishnamachari, Bhaskar and Srivastava, Mani and Zhang, Mi}, keywords = {Artificial Intelligence of Things, AIoT, edge AI}, abstract = {The integration of the Internet of Things (IoT) and modern Artificial Intelligence (AI) has given rise to a new paradigm known as the Artificial Intelligence of Things (AIoT). In this survey, we provide a systematic and comprehensive review of AIoT research. We examine AIoT literature related to sensing, computing, and networking &amp; communication, which form the three key components of AIoT. In addition to advancements in these areas, we review domain-specific AIoT systems that are designed for various important application domains. We have also created an accompanying GitHub repository, where we compile the papers included in this survey: https://github.com/AIoT-MLSys-Lab/AIoT-Survey. This repository will be actively maintained and updated with new research as it becomes available. As both IoT and AI become increasingly critical to our society, we believe that AIoT is emerging as an essential research field at the intersection of IoT and modern AI. It is our hope that this survey will serve as a valuable resource for those engaged in AIoT research and act as a catalyst for future explorations to bridge gaps and drive advancements in this exciting field.} }
@inbook{10.1145/3756423.3756558, title = {Research on Ethical Risks and Response Strategies in the Development of Artificial Intelligence}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Smart Manufacturing}, pages = {824–828}, year = {2025}, isbn = {9798400714351}, url = {https://doi.org/10.1145/3756423.3756558}, author = {Wang, Weina and Luo, Qiufang and Huang, Yinrong and Feng, Jingsheng}, abstract = {On the one hand, the development of AI technology has promoted the transformation of human production and lifestyle. On the other hand, it has also triggered ethical risks such as data abuse, algorithm discrimination, and employment structure transformation. &nbsp;These risks are increasingly showing up in the form of high-frequency words and multi-dimensional phenomena. We must correctly recognize the various risks and challenges in the process of AI development. We should build a governance model of the five-element synergy from the aspects of emphasizing individual self-discipline, government regulation, corporate accountability, public supervision global co-governance, thus promoting the common development of AI technology and society.} }
@inproceedings{10.1145/3726010.3726091, title = {Intelligent Campus Environment Construction: Applications and Educational Roles of Artificial Intelligence Technology}, booktitle = {Proceedings of the 2024 International Conference on Artificial Intelligence, Digital Media Technology and Interaction Design}, pages = {514–521}, year = {2025}, isbn = {9798400712845}, doi = {10.1145/3726010.3726091}, url = {https://doi.org/10.1145/3726010.3726091}, author = {Liu, Liang}, keywords = {Artificial intelligence, Big data, Education, Intelligent campus environment, Machine learning, Smart learning}, abstract = {This work studies the development of the intelligent campus construction process. The demand for constructing modern institutions is high due to the development of Artificial Intelligence (AI) technologies. It uses the emerging technologies such as cloud computing, big data, computer vision, and other technologies to acquire the physical environment. This can detect the learning environment of students and teachers. Establishing an intelligent, open, and comfortable campus environment not only improves the hardware environment of the campus but also promotes the development of academic research, the improvement of teaching quality, the enhancement of the educational environment, and the acquisition of happiness of teachers and students. This paper examines the applications of AI in the campus environment development. This focuses on the contributions towards academic development, campus safety, and human-centric design. This research work represents the outcome of intelligent campus construction in modern education with the help of detailed reviews and case studies. This work also discusses the role of AI in sustainable educational institutions and future trends in intelligent campus development.} }
@article{10.1145/3711118, title = {Data-centric Artificial Intelligence: A Survey}, journal = {ACM Comput. Surv.}, volume = {57}, year = {2025}, issn = {0360-0300}, doi = {10.1145/3711118}, url = {https://doi.org/10.1145/3711118}, author = {Zha, Daochen and Bhat, Zaid Pervaiz and Lai, Kwei-Herng and Yang, Fan and Jiang, Zhimeng and Zhong, Shaochen and Hu, Xia}, keywords = {Artificial intelligence, machine learning, data-centric AI}, abstract = {Artificial Intelligence (AI) is making a profound impact in almost every domain. A vital enabler of its great success is the availability of abundant and high-quality data for building machine learning models. Recently, the role of data in AI has been significantly magnified, giving rise to the emerging concept of data-centric AI. The attention of researchers and practitioners has gradually shifted from advancing model design to enhancing the quality and quantity of the data. In this survey, we discuss the necessity of data-centric AI, followed by a holistic view of three general data-centric goals (training data development, inference data development, and data maintenance) and the representative methods. We also organize the existing literature from automation and collaboration perspectives, discuss the challenges, and tabulate the benchmarks for various tasks. We believe this is the first comprehensive survey that provides a global view of a spectrum of tasks across various stages of the data lifecycle. We hope it can help the readers efficiently grasp a broad picture of this field, and equip them with the techniques and further research ideas to systematically engineer data for building AI systems. A companion list of data-centric AI resources will be regularly updated on .} }
@inproceedings{10.1145/3745238.3745512, title = {The Impact of Artificial Intelligence on IoT Application Technology Talent Demand}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1753–1758}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745512}, url = {https://doi.org/10.1145/3745238.3745512}, author = {Su, Zijie}, keywords = {Artificial Intelligence, Competency Framework, IoT, Skill Gap, Talent Demand}, abstract = {AI's deployment across IoT has transformed several industries and introduced complexities requiring a drastic shift in the skill set expected from IoT application technology talent. Within this context, this paper, contributing to the literature by providing a comprehensive review on the impact of AI on IoT talent demand, covering the evolving skills requirements, industry trends and educational implications. By synthesizing the output of existing literature, industry reports, and case studies, we highlight the essential skills sought after and some hurdles to the industry adapting to this change. We also provide actionable steps to bridge the skill gap and outline the future directions for research and practice in this space. Key Features of AI: Machine Learning, Deep Learning, Natural Language Processing, Image Processing. AI technologies such as machine learning, deep learning, natural language processing, and more have made it possible to create advanced IoT applications. From predictive maintenance and autonomous decision-making to real-time data analytics, these applications have disrupted traditional industries and spawned new business models. AI in Manufacturing. For example, in manufacturing, AI is being used for predictive maintenance and quality control, which minimizes equipment downtime and improvements in product quality. In the medical field, AI-empowered IoT devices provide remote patient monitoring and predictive analysis, enhancing patient care and enhancing medical outcomes. In retail, IoT devices that use AI offer personalized recommendations and intelligent inventory management, improving customer experiences while making stores more efficient. While AI and the Internet of Things (IoT) present powerful opportunities in terms of their applications, the industry also has its challenges, especially in terms of talent demand. There is a rising demand for AI and IoT professionals in the integration of IoT systems with AI. There is, however, a significant skills gap, as many organizations face difficulties in finding candidates with the necessary interdisciplinary expertise. This skills gap mean therefore lead to project implementation time delays and added costs, preventing the realization of the full potential of AI that powers IoT applications. The findings of this study not only highlight the key trends of AI influence on IoT but also assist major stakeholders including educators, practitioners and students to develop matching strategies to adapt to the transformations brought by the AI technologies in the specific context of the IoT. As AI and IoT continue to evolve and converge, their potential for creating impactful and transformative solutions will only grow.} }
@article{10.1145/3696009, title = {Artificial Intelligence and the Future of Evaluation: From Augmented to Automated Evaluation}, journal = {Digit. Gov.: Res. Pract.}, volume = {6}, year = {2025}, doi = {10.1145/3696009}, url = {https://doi.org/10.1145/3696009}, author = {Jacob, Steve}, keywords = {Public policy evaluation, artificial intelligence, skills and competencies, generative AI}, abstract = {The recent developments in artificial intelligence (AI) are revolutionizing professional practices across various professional fields, including evaluation. With its advanced automation and learning capabilities, AI is bringing significant changing to the way organizations and societies function. Evaluation is no exception to this trend, even though evaluators are adopting AI at a slower pace. This article examines ongoing applications that already improve and enhance the evaluation practice. We advance our discussion by exploring the potential impact of AI on the policy cycle. Subsequently, we analyze the potential incorporation of evaluation into autonomous AI systems that could design, implement, and evaluate public policies with minimal to no human supervision.} }
@inproceedings{10.1145/3716489.3728433, title = {Review: The Role of Artificial Intelligence in Building Information Modeling}, booktitle = {Proceedings of the 2025 Computers and People Research Conference}, year = {2025}, isbn = {9798400714979}, doi = {10.1145/3716489.3728433}, url = {https://doi.org/10.1145/3716489.3728433}, author = {Mirindi, Derrick and Mirindi, Frederic and Bezabih, Tajebe and Sinkhonde, David and Kiarie, Winfred}, keywords = {Artificial Intelligence, Building Information Modeling, construction management, technological information}, abstract = {The integration of Artificial Intelligence (AI) and Building Information Modeling (BIM) represents a paradigm shift in the architecture, engineering, and construction (AEC) industry. This change revolutionizes how buildings are designed, constructed, and managed throughout their lifecycle. Using secondary data, this research aims to evaluate the current advances in the role of AI in BIM, emphasizing leveraging AI to improve organizational performance, decision-making, and project delivery. Based on the four classifications of AI systems which are Reactive AI, Limited Memory AI, Theory of Mind AI, and Self-Aware AI, this study analyzed the prospects of BIM applications. Results show that AI in BIM applications offers capabilities across domains such as generative design, predictive maintenance, energy optimization, automated code compliance, clash detection, construction simulation, sustainability analysis, project risk assessment, facility management, and natural language processing. The research also presents major issues arising from the integration of AI in a BIM setting, including Interoperability problems, Data management problems, and Technological resistance problems. Based on the evaluation of the current status and challenges associated with AI integration, this work expects to offer practical recommendations for the stakeholders interested in implementing AI technologies within their BIM projects. In addition, the research reveals new directions in integrating AI into BIM, including digital twins and generative design that will likely transform the project lifecycle. This research fills the gap in the current literature by exploring the AI limitations and opportunities in BIM environments and thereby provides insights into the application of these technologies to enhance innovation and sustainability in construction. Finally, this research provides a basis for new and more rational approaches to construction that would meet the needs of contemporary society.} }
@inproceedings{10.1145/3708359.3712078, title = {DreamDirector: Designing a Generative AI System to Aid Therapists in Treating Clients' Nightmares}, booktitle = {Proceedings of the 30th International Conference on Intelligent User Interfaces}, pages = {553–578}, year = {2025}, isbn = {9798400713064}, doi = {10.1145/3708359.3712078}, url = {https://doi.org/10.1145/3708359.3712078}, author = {Zhao, Yijun and Li, Zhengke and Wang, Yicheng and Cai, Xueyan and Zhou, Xiaojing and Yan, Yifan and Jin, Kecheng and Ding, Shiying and Shao, Yilin and Cao, Jiacheng and Wang, Pinhao and Yao, Cheng and Tao, Ye and Wang, Guanyun}, keywords = {Nightmare therapy, System assist therapist, System design, Generative AI}, abstract = {Nightmares can adversely affect individuals’ mental health and well-being, necessitating timely psychological intervention. Current nightmare therapy has set high requirements for therapists, appeared abstract to clients, and showed poor interaction between them, due to its extensive information input, lack of sensory stimulation, and exclusive reliance on one-on-one conversation. We propose DreamDirector, a visual-interactive and narrative generative system powered by generative AI. Based on Imagery Rehearsal Therapy (IRT) and Nightmare Deconstruction and Reprocessing (NDR), the system can (1) recollect the nightmare scene, (2) interpret the dream with LLM, (3) reprocess the nightmare by generating therapeutic dream visuals using AI painting alongside meditative texts, and feedback with a picture book. Finally, we verified the usability of this system in terms of efficiency enhancement and interaction promotion through a user study with 2 therapists and 16 clients. The results revealed emotional relief among clients, with a positive and impressive attitude toward visual interaction.} }
@inproceedings{10.1145/3709026.3709097, title = {Evolution and Implementation of Artificial Intelligence Literacy by Using BERT-Based System}, booktitle = {Proceedings of the 2024 8th International Conference on Computer Science and Artificial Intelligence}, pages = {563–567}, year = {2025}, isbn = {9798400718182}, doi = {10.1145/3709026.3709097}, url = {https://doi.org/10.1145/3709026.3709097}, author = {Li, Xianghe and Ding, Chenye and Zhu, Yudi and Wang, Haibo and Qin, Bo and Li, Yan}, keywords = {Artificial Intelligence Literacy, BERT Model, Knowledge Engineering}, abstract = {The proliferation of Artificial Intelligence (AI), academic landscapes in research and learning are shifting from traditional to innovative strategies, greatly impacting students’ learning and teachers’ pedagogies in High Education (HE). There is an increasing number of universities worldwide engaging in AI exploration, aiming to adopt it as a lifelong learning objective. This study investigates the progression of AI literacy in engineering domain for both students and researchers in Higher Education. Some innovative teaching activities, such as AI working workshops and seminars, are implemented to enhance the researchers’ and students’ AI literacy. The 'conversational framework' serves as the theoretical foundation for the teaching methodologies and engineering pedagogy applied in this research. A novel BERT-based system is introduced and utilized within a real teaching context. The experimental results and collected teaching data indicate that the BERT-based system can enhance students’ and researchers’ learning to a certain extent.} }
@inproceedings{10.1145/3708394.3708414, title = {Design of English Teaching System Based on Artificial Intelligence Technology}, booktitle = {Proceeding of the 2024 International Conference on Artificial Intelligence and Future Education}, pages = {109–113}, year = {2025}, isbn = {9798400710650}, doi = {10.1145/3708394.3708414}, url = {https://doi.org/10.1145/3708394.3708414}, author = {Meng, Yanrong}, keywords = {English, artificial intelligence technology, teaching system}, abstract = {The design of English teaching system based on artificial intelligence technology aims to solve the problems of insufficient training time and low efficiency in traditional English teaching. The research adopts advanced technologies such as intelligent speech recognition, natural language processing and machine learning to build an efficient and interactive learning environment. By integrating virtual reality and augmented reality technology, the system strengthens students' language application ability and encourages communication in simulated or actual contexts. The classroom teaching mode covers three stages: before class, during class and after class, to ensure the comprehensive improvement of students' listening and speaking skills. ITS module design includes student model, teacher model and expert model, which realizes the adjustment of personalized teaching content, evaluates learning results and provides targeted solutions. The realization of system functions depends on three core components: remote courseware service system, customer mobile learning system and data agent system, providing a stable and efficient personalized adaptive learning experience. The test results show that the system can still maintain high efficiency and stability when the number of concurrent users increases, which is superior to the traditional teaching system. In addition, intelligent composition correction, oral practice feedback, personalized learning plan recommendation and intelligent translation tools have significantly improved students' English writing ability and oral fluency.} }
@inproceedings{10.1145/3744367.3744391, title = {Research Hotspots and Frontier Trends of Vocational Education in China under the Influence of Artificial Intelligence: A Bibliometric Analysis Based on CiteSpace}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Educational Systems}, pages = {148–152}, year = {2025}, isbn = {9798400715068}, doi = {10.1145/3744367.3744391}, url = {https://doi.org/10.1145/3744367.3744391}, author = {Du, Ruiyun and Tan, Ninghui and Jiang, Fangying and Weng, Qingchun}, keywords = {Artificial Intelligence, Bibliometrics, CiteSpace, Vocational Education}, abstract = {This study uses China National Knowledge Infrastructure (CNKI) as the data source platform, focuses on high-quality papers published in Peking University core journals and CSSCI journals. It uses CiteSpace, a bibliometric software that looks at research, to find out about the most popular topics, how research has developed, and the latest trends in vocational and technical education in China in the era of artificial intelligence.} }
@inproceedings{10.1145/3756580.3756658, title = {Application and Practice of Artificial Intelligence in French Language Teaching}, booktitle = {Proceedings of the 2025 6th International Conference on Education, Knowledge and Information Management}, pages = {474–478}, year = {2025}, isbn = {9798400715624}, doi = {10.1145/3756580.3756658}, url = {https://doi.org/10.1145/3756580.3756658}, author = {Yu, Peiting}, keywords = {Application, Artificial intelligence, French language teaching}, abstract = {Under the background of artificial intelligence, the importance of French is becoming more and more prominent, especially the research on French curriculum construction based on artificial intelligence. This paper mainly studies artificial intelligence. In the French course, especially for Wuhan University of Arts and Sciences, the course of basic French 3. Research its application in the course, especially the intelligent processing of teaching resources and the intelligent design of teaching methods. Build an immersive teaching scene through artificial intelligence to strengthen the learning effect. The related problems and challenges are analyzed, including language processing ability, lack of data and lack of immersion. Through the construction of relevant artificial intelligence model, man-machine cooperation and big data processing methods and means to effectively solve the above problems, promote the benign development of French teaching.} }
@inproceedings{10.1145/3745238.3745450, title = {Research On Optimization Strategies Of Artificial Intelligence Algorithms In The Construction Of Smart Cities Under The Digital Economy}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1361–1367}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745450}, url = {https://doi.org/10.1145/3745238.3745450}, author = {Zhuang, Kunzhi}, keywords = {Algorithm optimization strategy, Artificial intelligence algorithm, Digital economy, Smart city construction, Urban sustainable development}, abstract = {With the advent of the digital economy era, the construction of smart cities has become an important means to promote the sustainable development of cities. Focusing on the development needs of smart cities in the context of the digital economy, this paper analyzes the application of artificial intelligence algorithms in the optimization strategies of smart cities. By systematically summarizing artificial intelligence algorithms, it elaborates on their key roles in the construction of smart cities. Through introducing various algorithm optimization techniques and actually analyzing their specific applications in functional areas of smart cities such as traffic management, public security, and environmental protection, it reveals effective ways for algorithm optimization to expand the service capabilities of smart cities. This not only helps deepen the understanding of the integration of smart cities and artificial intelligence but also provides a scientific basis for urban managers to make intelligent decisions under the conditions of the digital economy.} }
@inproceedings{10.1145/3708394.3708452, title = {Exploring Educational Transformation and Innovation Pathways in the Era of Artificial Intelligence}, booktitle = {Proceeding of the 2024 International Conference on Artificial Intelligence and Future Education}, pages = {338–346}, year = {2025}, isbn = {9798400710650}, doi = {10.1145/3708394.3708452}, url = {https://doi.org/10.1145/3708394.3708452}, author = {Sun, Weiwei and Qin, Minwu and Yin, Zhenyao}, keywords = {Artificial Intelligence, Educational Transformation, Intelligent Classroom, Personalized Learning, Teaching Innovation}, abstract = {This study analyzes the profound impact of artificial intelligence (AI) on education, exploring the applications of educational reform theory, technological innovation theory, and the theory of equal educational opportunities in the context of AI-driven educational transformation. The rapid advancement of AI technologies—particularly deep learning, intelligent image recognition, big data, and educational robotics—is driving education from traditional models toward personalization, lifelong learning, and intelligent approaches. Technological innovation not only revamps teaching tools and resources but also enables differentiated instruction through intelligent data analysis, promoting educational equity and enhancing students’ self-learning abilities. AI demonstrates immense potential in supporting lifelong learning, optimizing educational processes, and enriching the educational ecosystem; however, it also raises ethical challenges, including privacy concerns and risks of educational alienation. Consequently, educators should focus on the responsible application of AI technologies, leveraging the intrinsic strengths of education, to improve teaching quality and foster harmonious development between humans and machines.} }
@inproceedings{10.1145/3745034.3745097, title = {Applications and Challenges of Artificial Intelligence in Orthodontics}, booktitle = {Proceedings of the 4th International Conference on Biomedical and Intelligent Systems}, pages = {404–413}, year = {2025}, isbn = {9798400714399}, doi = {10.1145/3745034.3745097}, url = {https://doi.org/10.1145/3745034.3745097}, author = {Shuai, Xingyue and Li, Xiaolu and Wen, Liming and Qi, Mengchun}, keywords = {Artificial intelligence, Convolutional neural networks, Deep learning, Medical imaging analysis, Multimodal data fusion, Orthodontics}, abstract = {The integration of artificial intelligence (AI) into orthodontic practice has evolved from supporting roles to becoming pivotal in clinical decision-making. This review examines recent AI breakthroughs, evaluates their real-world performance in treatment settings, and addresses unresolved obstacles facing adoption. Key barriers involve persistent interdisciplinary collaboration gaps, algorithmic adaptability constraints across diverse patient profiles, and underdeveloped oversight protocols. Emerging solutions focus on integrating multimodal data streams, developing edge-computing platforms for chairside analytics, and establishing balanced governance models addressing technical, ethical, and legal dimensions.} }
@proceedings{10.1145/3708394, title = {AIFE '24: Proceeding of the 2024 International Conference on Artificial Intelligence and Future Education}, year = {2024}, isbn = {9798400710650} }
@article{10.1145/3711682, title = {Generative AI Empowered Network Digital Twins: Architecture, Technologies, and Applications}, journal = {ACM Comput. Surv.}, volume = {57}, year = {2025}, issn = {0360-0300}, doi = {10.1145/3711682}, url = {https://doi.org/10.1145/3711682}, author = {Li, Tong and Long, Qingyue and Chai, Haoye and Zhang, Shiyuan and Jiang, Fenyu and Liu, Haoqiang and Huang, Wenzhen and Jin, Depeng and Li, Yong}, keywords = {Generative AI, digital twins, mobile networks, network monitoring, network simulation, network operation}, abstract = {The rapid advancement of mobile networks highlights the limitations of traditional network planning and optimization methods, particularly in modeling, evaluation, and application. Network Digital Twins, which simulate networks in the digital domain for evaluation, offer a solution to these challenges. This concept is further enhanced by generative AI technology, which promises more efficient and accurate AI-driven data generation for network simulation and optimization. This survey provides insights into generative AI-empowered network digital twins. We begin by outlining the architecture of a network digital twin, which encompasses both digital and physical domains. This architecture involves four key steps: data processing and network monitoring, digital replication and network simulation, designing and training network optimizers, Sim2Real, and network control. Next, we systematically discuss the related studies in each step and make a detailed taxonomy of the problem studied, the methods used, and the key designs leveraged. Each step is examined with a focus on the role of generative AI, from estimating missing data and simulating network behaviors to designing control strategies and bridging the gap between digital and physical domains. Finally, we discuss the open issues and challenges of generative AI-based network digital twins.} }
@inbook{10.1145/3745238.3745466, title = {Mathematical Model Construction and Innovative Management Strategies for Tourism Development in the Post Pandemic Era Empowered by Artificial Intelligence}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1457–1461}, year = {2025}, isbn = {9798400712791}, url = {https://doi.org/10.1145/3745238.3745466}, author = {Chan, Zijian and Hu, Shaoming and Yan, Zhihang}, abstract = {Using artificial intelligence and mathematical models, the current study takes a hybrid approach at developing some scientific solutions for the restoration of tourism during the post-epidemic period. It is expected that the combination of Python crawlers, GIS software, and machine learning algorithm to improve and use the scenic spot evaluation and carrying capacity model, combined with AI optimization strategy, can help the tourism industry to recover better and develop sustainably. The findings of this research will provide strong suggestions for managing scenic spots and governmental decision-making, as well as help the tourism industry to adapt to the post-epidemic era.} }
@inproceedings{10.1145/3745238.3745401, title = {Mapping the Research Trends and Hotspots in “Artificial Intelligence and Environmental Sustainability” ——A Comprehensive Bibliometric Analysis}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1042–1047}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745401}, url = {https://doi.org/10.1145/3745238.3745401}, author = {Li, Xiaoyu and Meng, Haohan and Sun, Jinhua and Gu, Lei}, keywords = {Artificial intelligence, Bibliometrics, Environmental sustainability, Web of Science}, abstract = {With the theme of “Artificial Intelligence and Environmental Sustainability”, this study uses bibliometrics to find the latest research hotspots and development trends. We carefully selected literatures published between 2014 and 2025 from the Web of Science Core Collection (WOSCC) database. We then used VOSviewer, CiteSpace, and Scimago Graphica to conduct an analysis of global research profiles, research collaboration networks, and emerging research topics and trends. It turns out that after 2019, there is a growing global interest in the field. China, the United Kingdom, the United States and India are particularly active. A network of research collaborations is emerging, although the intensity of collaboration remains low. The latest research focuses on applying AI technology to environmental areas such as climate change mitigation, smart city development and renewable energy development.} }
@inproceedings{10.1145/3748825.3748840, title = {Research on the optimization of intelligent path for higher vocational English vocabulary learning driven by artificial intelligence}, booktitle = {Proceedings of the 2025 2nd International Conference on Digital Society and Artificial Intelligence}, pages = {81–87}, year = {2025}, isbn = {9798400714337}, doi = {10.1145/3748825.3748840}, url = {https://doi.org/10.1145/3748825.3748840}, author = {Xu, Fang and Li, Jiping}, keywords = {Artificial intelligence, higher vocational English, innovative algorithm, intelligent path, vocabulary learning}, abstract = {This study is dedicated to optimizing the intelligent path of higher vocational English vocabulary learning driven by artificial intelligence. The opening chapter points out the key role of higher vocational English vocabulary in English course learning and the rapid development of artificial intelligence in the field of education. The current status of related research at home and abroad is deeply analyzed, and the core goal is to design innovative algorithms and build intelligent paths. The vocabulary recommendation algorithm based on the fusion of deep learning and reinforcement learning is explained in detail. Its principle is based on the deep mining of vocabulary features and learners' 2-year historical data, and the strategy is dynamically adjusted according to feedback. The algorithm process covers data preprocessing (collecting 10,000 words, 50,000 examples, etc.), model training (learning rate 0.001, 500 iterations) and recommendation updates, with an accuracy rate of 92%. A personalized intelligent learning path based on learner-centered principles is constructed. 200 students from a higher vocational college were selected and randomly divided into an experimental group and a control group. The experimental group used the new algorithm intelligent learning system to study for 45 minutes a day for 12 weeks, and the control group used the traditional method. Before and after the experiment, the vocabulary and vocabulary memory accuracy were tested, and the independent sample t test was used to analyze the data. The results showed that the vocabulary of the experimental group increased by 31.7% on average to 3950, and the vocabulary memory accuracy increased to 88%; the control group increased by 10.7% to 3300 and increased to 75%, respectively. The difference between the two groups was extremely significant (p &lt; 0.01). It is fully confirmed that the new algorithm and intelligent learning path can effectively improve the English vocabulary learning effect of higher vocational students, and provide strong support for the reform of higher vocational English teaching.} }
@inproceedings{10.1145/3727648.3727674, title = {Research Hotspot and Trend of Artificial Intelligence Applied in Supply Chain Resilience—Visual Analysis Based on CiteSpace}, booktitle = {Proceedings of the 4th International Conference on Computer, Artificial Intelligence and Control Engineering}, pages = {146–151}, year = {2025}, isbn = {9798400712647}, doi = {10.1145/3727648.3727674}, url = {https://doi.org/10.1145/3727648.3727674}, author = {Ding, Libo and Wang, Guangliang}, keywords = {Artificial Intelligence, CiteSpace, Supply Chain Resilience}, abstract = {Based on the CiteSpace knowledge map analysis method, this paper visually analyzes the core research literature on artificial intelligence in the field of supply chain resilience in the Web of Science database from 2014 to 2024 on the number of papers, research authors, research institutions, research hotspots and research trends. The research results show that this field has been in a stage of rapid development since 2019. Many scholars and institutions have participated and achieved rich research results. The research hotspots focus on big data analytics, management practices, and firm performance. In the future, the research of artificial intelligence in the field of supply chain resilience will move towards deep integration, practical application, predictive analysis, improve corporate competitiveness and develop in the direction of operational optimization and intelligence. However, due to possible issues such as uneven resource allocation, restricted knowledge exchange, and differences in research quality, many scholars and institutions are not very active in cooperation. In response to this, relevant departments should introduce targeted policies to encourage collaboration among scholars and institutions, such as providing funds for cooperative research and establishing cooperative research projects, in order to promote balanced resource distribution and extensive knowledge exchange.} }
@inproceedings{10.1145/3744367.3744394, title = {Research on Academic Early Warning Model Construction and Intervention Strategy Optimization for University Students Based on Artificial Intelligence}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Educational Systems}, pages = {169–175}, year = {2025}, isbn = {9798400715068}, doi = {10.1145/3744367.3744394}, url = {https://doi.org/10.1145/3744367.3744394}, author = {Liu, Jing and Liu, Qian}, keywords = {Academic Early Warning, Artificial Intelligence, Deep Learning, Education Management, Intervention Strategy, Reinforcement Learning}, abstract = {Now that university education is growing in size and more developed information technology, the old academic early warning methods have not kept up with the situation, and it is difficult to find problems accurately and in time. This paper has designed a new model of academic early warning using artificial intelligence technology, and also improved the specific practice of intervention strategies. This early warning model uses deep learning technology, which can integrate various data from different channels, especially using graph neural networks to analyze social relationships between students, and add a special attention mechanism module to deal with time changes.The model's prediction accuracy rate reached 92.1%, which is much stronger than the old method. The problem-solving rate of the group with intelligent intervention was 76.4%, nearly 24 percentage points higher than the manual intervention. These research results have brought real help to the education management of colleges and universities, and it is very meaningful to improve the quality of teaching.} }
@inproceedings{10.1145/3745238.3745315, title = {Research on the Entropy Weight Measurement of the Contribution of Digital Economy Supported by Artificial Intelligence Computing Power to New Quality Productivity}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {470–474}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745315}, url = {https://doi.org/10.1145/3745238.3745315}, author = {Liu, Jian and Liu, Qi}, keywords = {artificial intelligence computing power, digital economy, entropy weight measurement, new quality productivity}, abstract = {Under the new economic form where artificial intelligence computing power is accelerating its penetration, quantitatively analyzing the driving mechanism of the digital economy on new quality productivity has become a key issue. This study constructs a three-dimensional analytical framework of “computing power infrastructure - activation of data elements - embedding of intelligent technologies”, and based on the panel data of 30 provinces in China from 2012 to 2022, uses the improved entropy weight method to measure the development level of the digital economy supported by artificial intelligence computing power. It innovatively introduces the technology empowerment coefficient to correct the traditional productivity evaluation system. This study provides theoretical basis and strategic tools for the cross-domain allocation of AI computing power resources and the intelligent transformation of productivity.} }
@inproceedings{10.1145/3726010.3726013, title = {Visualisation and Trend Analysis of Human-Computer Interaction System Design in the Context of Artificial Intelligence}, booktitle = {Proceedings of the 2024 International Conference on Artificial Intelligence, Digital Media Technology and Interaction Design}, pages = {14–21}, year = {2025}, isbn = {9798400712845}, doi = {10.1145/3726010.3726013}, url = {https://doi.org/10.1145/3726010.3726013}, author = {Jiang, Xuemei and Wang, Meiling and Deng, Tingyin}, keywords = {Artificial Intelligence, CiteSpace, Knowledge Graph, System Design, VOSviewer, human-computer interaction}, abstract = {Driven by artificial intelligence, human-computer interaction system design is evolving towards intelligence, automation, and diversification. OBJECTIVE: The objective of this study is to systematically present the progress of human-computer interaction design research in the AI era over the past 20 years. In order to achieve this, the study firstly highlights the current state of development, trends and current research hotspots. It then provides a reference for further research. METHODS: Using visualisation tools such as CiteSpace, VOSviewer, and Scimago Graphica, this study analysed 887 documents included in the Web of Science database from 2004 to 2024, covering knowledge mapping in terms of countries, author institutions, keywords, etc., in order to comprehensively grasp the research dynamics of HCISD of the AI model. The results show that the development of the research field of HCISD in the context of artificial intelligence shows a steady growth trend; the issuing countries are spread all over the world, mainly concentrated in economically developed countries with strong scientific research strength; the current research hot spots are concentrated in four main aspects of intelligent innovation system, intelligent interconnected management, intelligent technical task, and intelligent technical interaction; the future development trend tends to promote user experience through advanced visual expression; and the future trend tends to enhance user experience, promote collaboration, and provide richer information interaction through advanced visual expression and interaction technologies. Conclusion: Research in human-computer interaction design follows the rapid development of artificial intelligence technologies and continuously explores new theoretical and practical approaches to meet the increasing demand for intelligent systems.} }
@article{10.1145/3757308.3757310, title = {Rewriting the Margins: Can Artificial Intelligence Agents Reshape the Power Dynamics in Social Media?}, journal = {SIGMIS Database}, volume = {56}, pages = {7–12}, year = {2025}, issn = {0095-0033}, doi = {10.1145/3757308.3757310}, url = {https://doi.org/10.1145/3757308.3757310}, author = {Weng, Qin and Ren, Jie and Mattson, Tom and Deng, Xuefei (Nancy) and Joshi, K. D.}, keywords = {artificial intelligence agents., marginalized groups, social media, stigmatization}, abstract = {Artificial intelligence (AI) agents have infiltrated most aspects of our lives and the technologies we use, including social media. Content (and moderation thereof) on many popular social media platforms is currently a mix of human- and AI-generated. Depending on many technical, situational, and contextual factors, both human- and AI-generated content on social media platforms have the potential to either fuel or fight the stigmatization of marginalized groups. Stigmatization is the social process of stereotyping, devaluing, or marginalizing individuals or groups based on certain characteristics such as gender, ethnicity, religion, or race, which can lead to social exclusion and psychological damage. The impact of AI agents on either reinforcing or reducing existing stigmatization on social media platforms remains largely unknown within our scholarly community. We have even less understanding of how to design and implement AI agents that can actively help transform social media platforms into spaces for combating stigmatization while empowering marginalized groups. In this editorial, we call on the information systems (IS) community to conduct empirical, design, and theoretical research at the intersection of social media, specific AI systems or agents, and marginalized contexts. Advancing research in this space is critical to understanding and shaping the future of social media platforms as a social good and as places to reduce stigmatization.} }
@inproceedings{10.1145/3703187.3703227, title = {Information Risk Analysis and Global Ethical Governance in the Application of Artificial Intelligence}, booktitle = {Proceedings of the 2024 7th International Conference on Computer Information Science and Artificial Intelligence}, pages = {238–243}, year = {2024}, isbn = {9798400707254}, doi = {10.1145/3703187.3703227}, url = {https://doi.org/10.1145/3703187.3703227}, author = {Jia, Jinrun and Ma, Zhiguo}, keywords = {Artificial intelligence, Ethical governance, Information security, People-oriented}, abstract = {Artificial intelligence technology has gradually matured and will become the research and development foundation of digital applications in many industries. However, the technical limitations of the machine itself or the deluge of false information that may be generated under the malicious manipulation of others may worsen the Internet ecology. Based on the cross-national, cross-cultural, cross-regional nature of artificial intelligence technology, we should improve the ethical governance framework of global artificial intelligence information content risk, embed the concept of people-oriented ethical governance in the development of technology, and improve the ethical review mechanism of the whole process of technology and the international coordination mechanism based on value consensus.} }
@inbook{10.1145/3730436.3730537, title = {Empowering New Quality Productive Forces through Artificial Intelligence Technology: A Study on the Pathways and Threshold Effects in High-Quality Development of Manufacturing Industry}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Computational Intelligence}, pages = {620–624}, year = {2025}, isbn = {9798400713637}, url = {https://doi.org/10.1145/3730436.3730537}, author = {Zhao, Jingzhou and Hu, Yu and Deng, Wei and Duan, Wenxiao and Chang, Xiaokang and Zhou, Junru}, abstract = {This paper first conducts a regression analysis on the direct effect of artificial intelligence on the high-quality development of the manufacturing industry, and then uses the panel threshold model to explore the threshold effect of artificial intelligence on the high-quality development of the manufacturing industry. The Hausman test confirms the rationality of the fixed effect regression model, indicating that artificial intelligence has a significant positive impact on the high-quality development of the manufacturing industry. Subsequently, the panel threshold model is used for analysis, and it is found that there is a double threshold effect, with the first threshold value being 6.5575 and the second being 14.4800. The marginal effect of the improvement of artificial intelligence level on high-quality development shows a rapid increase at first, a slowdown after crossing the first threshold, and an acceleration again after crossing the second threshold. This nonlinear relationship is further verified through the adjustment ratio change and internal validity test for robustness. The results show that high-end and low-end manufacturing enterprises are more likely to benefit from artificial intelligence technology due to advantages in cost-effectiveness, market demand, and technological adaptability, thereby promoting their high-quality development. In contrast, mid-range manufacturing enterprises may face a series of challenges, resulting in less significant or even inhibitory effects of artificial intelligence technology on them.} }
@inproceedings{10.1145/3726302.3730008, title = {Information Retrieval in the Age of Generative AI: The RGB Model}, booktitle = {Proceedings of the 48th International ACM SIGIR Conference on Research and Development in Information Retrieval}, pages = {602–612}, year = {2025}, isbn = {9798400715921}, doi = {10.1145/3726302.3730008}, url = {https://doi.org/10.1145/3726302.3730008}, author = {Garetto, Michele and Cornacchia, Alessandro and Galante, Franco and Leonardi, Emilio and Nordio, Alessandro and Tarable, Alberto}, keywords = {automation bias, information quality, large language models, retrieval-augmented generation, stack exchange, web answering}, abstract = {The advent of Large Language Models (LLMs) and generative AI is fundamentally transforming information retrieval and processing on the Internet, bringing both great potential and significant concerns regarding content authenticity and reliability. This paper presents a novel quantitative approach to shed light on the complex information dynamics arising from the growing use of generative AI tools. Despite their significant impact on the digital ecosystem, these dynamics remain largely uncharted and poorly understood. We propose a stochastic model to characterize the generation, indexing, and dissemination of information in response to new topics. This scenario particularly challenges current LLMs, which often rely on real-time Retrieval-Augmented Generation (RAG) techniques to overcome their static knowledge limitations. Our findings suggest that the rapid pace of generative AI adoption, combined with increasing user reliance, can outpace human verification, escalating the risk of inaccurate information proliferation across digital resources. An in-depth analysis of Stack Exchange data confirms that high-quality answers inevitably require substantial time and human effort to emerge. This underscores the considerable risks associated with generating persuasive text in response to new questions and highlights the critical need for responsible development and deployment of future generative AI tools.} }
@inproceedings{10.1145/3746175.3746208, title = {Does ChatGPT know what robots are thinking? - Evaluation of human-created vs. AI-generated scientific ideas}, booktitle = {Proceedings of the 36th Annual Conference of the European Association of Cognitive Ergonomics}, year = {2025}, isbn = {9798400720338}, doi = {10.1145/3746175.3746208}, url = {https://doi.org/10.1145/3746175.3746208}, author = {Eymann, Vera and Lachmann, Thomas and Hekele, Felix and Ashok, Ashita and Berns, Karsten and Czernochowski, Daniela}, keywords = {Scientific Creativity, Artificial Intelligence, Creativity, Generative AI}, abstract = {Scientific creativity (i.e., to innovate new scientific approaches or ideas) is a fundamental ability for scientists. Since the rise of artificial intelligence (AI), researchers have used specialized models to improve or automatize scientific processes and even scientific discoveries. In our study, we investigate whether proprietary large language models such as ChatGPT, though not specifically trained for scientific work, are able to convincingly perform or even surpass human scientific creativity. We asked graduate students and prompted ChatGPT to write an abbreviated version of a research proposal for a fictious scenario from the field of computer science/ robotics. Four specific assignments were given (i.e., formulating a hypothesis, designing an experiment, listing the required equipment, and justifying the chosen method). Human and AI-generated answers were rated by two scientists from the field, following a peer-review approach. Our results indicate that ChatGPT reached significantly higher overall scores relative to the human sample. However, closer inspection of the data and the rating procedure revealed that the evaluation of performance especially in regards to the originality of ideas differed considerably between the two raters. We evaluate these findings in more detail, as well as qualitative differences between human and AI- generated responses. Lastly, we discuss the impact of raters’ knowledge about and attitudes towards Generative AI and emphasize the importance of meta-cognitive skills to identify Generative AI.} }
@inproceedings{10.1145/3749566.3749627, title = {Artificial Intelligence-Driven Supply Chain Risk Mitigation}, booktitle = {Proceedings of the 2025 5th International Conference on Internet of Things and Machine Learning}, pages = {316–323}, year = {2025}, isbn = {9798400713927}, doi = {10.1145/3749566.3749627}, url = {https://doi.org/10.1145/3749566.3749627}, author = {Tian, Ye and Yang, Shuili and Li, Nan}, keywords = {Artificial Intelligence, Intelligent Supply Chain, Resource Allocation Efficiency, Supply Chain Risk}, abstract = {This study is grounded in the resource-based view theory and examines 1,367 Chinese A-share listed companies from 2008 to 2023. Use the Python language to crawl words related to "supply chain risk" in corporate annual reports, which are used to calculate the supply chain risk of enterprises. The fixed effects model and the intermediary effect model are used to empirically test the impact of Artificial Intelligence (AI) in mitigating the supply chain risk. Through empirical research, we find that: AI plays an instrumental role in mitigating corporate supply chain risk. AI mitigates the supply chain risk by improving resource allocation efficiency of enterprises. The heterogeneity analysis shows that for enterprises in highly competitive industries, the mitigating effect of AI on the supply chain risk is more evident; in non-technology-oriented enterprises, the mitigating effect of artificial intelligence on supply chain risk is more significant. This study contributes to the research on intelligent supply chain management and supply chain risk management.} }
@inbook{10.1145/3718491.3718558, title = {Global and Domestic Perspectives on AI in Education: A Knowledge Mapping Analysis Using CiteSpace}, booktitle = {Proceedings of the 4th Asia-Pacific Artificial Intelligence and Big Data Forum}, pages = {404–415}, year = {2025}, isbn = {9798400710865}, url = {https://doi.org/10.1145/3718491.3718558}, author = {Pan, Shengwu and Zeng, Qi and Li, Mingjiang and Yan, Na}, abstract = {This study utilized CiteSpace to perform an extensive knowledge mapping analysis of 826 Chinese articles and 488 international publications on artificial intelligence in education, obtained from the China National Knowledge Infrastructure (CNKI) and Web of Science databases, spanning the years 2014 to 2024. The research employed visualization methods, such as co-citation analysis and network algorithms, to comprehensively assess research hotspots, key topics, and emerging trends in the field, both domestically and internationally. Research indicates that overseas studies prioritize the incorporation of AI technology in education and the development of theoretical frameworks, whereas domestic research focuses on the practical use of AI in curriculum development and educational administration. Despite these divergent perspectives, both international and domestic research entities share a mutual interest in the use and advancement of AI in education, especially in relation to personalized learning and educational equity. To promote AI in global education, Future research should prioritize AI-driven digital transformation and standardization in education, Interdisciplinary collaboration and leveraging big data analytics will enhance innovation and decision-making, Addressing ethical issues, privacy, and societal acceptance is crucial for equitable and sustainable educational development.} }
@inproceedings{10.1145/3716553.3750793, title = {StoryDiffusion: How to Support UX Storyboarding With Generative-AI}, booktitle = {Proceedings of the 27th International Conference on Multimodal Interaction}, pages = {135–144}, year = {2025}, isbn = {9798400714993}, doi = {10.1145/3716553.3750793}, url = {https://doi.org/10.1145/3716553.3750793}, author = {Liang, Zhaohui and Zhang, Xiaoyu and Ma, Kevin and Liu, Zhao and Ren, Xipei and Goucher-Lambert, Kosa and Liu, Can}, keywords = {LLM, Text-to-image Model, Storyboard, Design}, abstract = {Storyboarding is an established method for designing user experiences. Generative AI can support this process by helping designers quickly create visual narratives. However, existing tools mainly focus on improving the accuracy of text-to-image generation. There is a lack of understanding on how to effectively support the entire creative process of storyboarding and how to develop AI-powered tools to be integrated into designers’ diverse workflows. In this work, we designed and developed StoryDiffusion, a system that integrates text-to-text and text-to-image models, to support the generation of narratives and images in a single pipeline. In a user study, we observed 12 UX design students using the system for both concept ideation and illustration tasks. Our findings identified AI-directed vs. user-directed creative strategies in both tasks and revealed the importance of supporting the interchange between narrative iteration and image generation. We also found effects of the design tasks on their strategies and preferences, providing insights for future development.} }
@inproceedings{10.1145/3703187.3703220, title = {Can the application of artificial intelligence technology promote enterprise green technology innovation?}, booktitle = {Proceedings of the 2024 7th International Conference on Computer Information Science and Artificial Intelligence}, pages = {195–200}, year = {2024}, isbn = {9798400707254}, doi = {10.1145/3703187.3703220}, url = {https://doi.org/10.1145/3703187.3703220}, author = {Wang, Ting}, keywords = {Artificial intelligence technology, Executives' academic background, Green technology innovation}, abstract = {As the core engine of the fourth scientific and technological revolution, artificial intelligence technology provides a good opportunity to drive the green technology innovation (GTI) of enterprises. At the same time, executives' academic background has an important impact on enterprises' GTI. This paper collects the word frequency data of artificial intelligence technology keywords through the jieba word segmentation function of Python software, and empirically tests the relationship between the application of artificial intelligence technology, the academic background of executives and the green technology innovation of enterprises in China's A-share listed enterprises from 2001 to 2021. The study finds that the application of artificial intelligence technology can effectively promote the ability of enterprises to improve the green technology innovation, and the higher the proportion of academic executives is, the stronger the role of the application of artificial intelligence technology in promoting the green technology innovation ability is. This study provides empirical evidence for exploring the application of artificial intelligence technology to achieve high-quality development from the perspective of executive personal characteristics.} }
@inproceedings{10.1145/3729706.3729787, title = {Research on the Factors of Innovation and Development of Artificial Intelligence Technology}, booktitle = {Proceedings of the 2025 4th International Conference on Cyber Security, Artificial Intelligence and the Digital Economy}, pages = {506–512}, year = {2025}, isbn = {9798400712715}, doi = {10.1145/3729706.3729787}, url = {https://doi.org/10.1145/3729706.3729787}, author = {Rong, Shuang and Huang, Dengjia and Gao, Yue and Li, Xiaobing and Wan, Wenfei}, keywords = {Artificial Intelligence technology, Causation, Factors, Technological innovation capability}, abstract = {The development of AI technology is changing with each passing day and attracting global attention. Given the current fewer research and judgments on the latest developments and trends of AI technology, this paper fully studies the focus of AI technology in major countries, comprehensively proposes important factors that affect the innovation and development of AI technology, and uses the Vensim model to perform simulation. The results show that Dynamic changes in policy factors, investment in innovation factor, cross-border cooperation, market demand, R&amp;D investment and testing capabilities can affect the technological innovation results of enterprises, especially innovation factors, market demand and testing capabilities. This article can provide reference for all sectors of society to carry out innovation in AI technology.} }
@inproceedings{10.1145/3745238.3745508, title = {The Dual Effects of Artificial Intelligence on Employment Structure: An Empirical Study Based on Regression and Clustering Models}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1722–1728}, year = {2025}, isbn = {9798400712791}, doi = {10.1145/3745238.3745508}, url = {https://doi.org/10.1145/3745238.3745508}, author = {Li, Jingchun and Dong, Shizhu}, keywords = {Artificial Intelligence, Clustering Analysis, Employment Structure, Regression Analysis}, abstract = {With the rapid development and widespread application of artificial intelligence technologies, the global employment landscape is undergoing profound changes. The demand for low-skilled jobs is decreasing, while the demand for high-skilled jobs is increasing. This study selects labour market data for major economies from 2015 to 2024, and applies regression and cluster analysis to explore the intrinsic link between the degree of technology penetration and labour restructuring.The study finds that AI technologies have replaced around a quarter of low-skilled jobs in manufacturing, while at the same time high-skilled jobs such as data science and AI development have grown rapidly, further widening the skills gap. Moreover, the impact has varied significantly across countries and industries. Developed countries have experienced a massive loss of low-skill jobs and a dramatic increase in high-skill jobs, while developing countries have experienced similar trends, but with relatively smaller changes.Based on the above findings, the study makes a series of targeted policy recommendations, including strengthening vocational training, promoting industrial upgrading, improving the social security system, and enhancing the adaptability of the labour force, with the aim of mitigating the impact of AI on the employment structure and promoting the smooth development of the labour market.} }
@inproceedings{10.1145/3641554.3701958, title = {Enhancing Cybersecurity Education with Artificial Intelligence Content}, booktitle = {Proceedings of the 56th ACM Technical Symposium on Computer Science Education V. 1}, pages = {158–164}, year = {2025}, isbn = {9798400705311}, doi = {10.1145/3641554.3701958}, url = {https://doi.org/10.1145/3641554.3701958}, author = {Brito, Fernando and Mekdad, Yassine and Ross, Monique and Finlayson, Mark A. and Uluagac, Selcuk}, keywords = {artificial intelligence, cybersecurity, education, integration, machine learning, natural language processing}, abstract = {Artificial Intelligence (AI) has become a fundamental tool for cybersecurity researchers and practitioners. It is frequently used to address major security problems such as supply chain attacks, ransomware threats, and social engineering. In this context, integrating AI into cybersecurity workflows requires incorporating AI-driven approaches into the educational training of the cybersecurity workforce. This paradigm shift in academic settings will introduce the necessary skills for cybersecurity professionals to operate modern AI-based systems. Yet, the current cybersecurity curriculum still suffers from the absence of AI resources, particularly the detailed understanding of the appropriate AI mechanisms. Such absence leaves skill gaps for future professionals and practitioners in the industry. To address this, we designed an academic lecture module on AI covering both theory and practice. Then, we taught the module across six cybersecurity courses in our institution. To assess the effectiveness of integrating AI materials into cybersecurity education, we collected data by presenting two surveys before and after the lecture (concluding 81 participants per survey). Specifically, we utilized widely accepted models for unbiased analysis of our data. Our experimental results show positive AI knowledge improvement by 30% of the participants, demonstrating the beneficial impact of the lecture. Then, we observed a high similarity score between the survey responses and the lecture content, reaching 84%. Moreover, our sentiment analysis results reflect positive feedback from the participants with a positive score of 0.50. Overall, our study serves as a reference for instructional designers for developing educational curricula aiming to integrate AI into cybersecurity education.} }
@inproceedings{10.1145/3719160.3728626, title = {Designing Age-Inclusive Interfaces: Emerging Conversational and Generative AI to Support Interactions across the Life Span}, booktitle = {Proceedings of the 7th ACM Conference on Conversational User Interfaces}, year = {2025}, isbn = {9798400715273}, doi = {10.1145/3719160.3728626}, url = {https://doi.org/10.1145/3719160.3728626}, author = {Munteanu, Cosmin and Sarcar, Sayan and Sin, Jaisie and Ziying Wei, Christina and Sayago, Sergio and Zhao, Wei and Waycott, Jenny and Boostani, Rezvan and Ghafurian, Moojan and Getson, Cristina}, abstract = {We are concurrently witnessing two significant shifts: voice and chat-based conversational user interfaces (CUIs) are becoming ubiquitous (especially more recently due to advances in generative AI and LLMs - large language models), and older people are becoming a very large demographic group (and increasingly adopting of mobile technology on which such interfaces are present). However, despite the recent increase in research activity, age-relevant and inter/cross-generational aspects continue to be underrepresented in both research and commercial product design. Therefore, the overarching aim of this workshop is to increase the momentum for research within the space of hands-free, mobile, and conversational interfaces that centers on age-relevant and inter- and cross-generational interaction. For this, we plan to create an interdisciplinary space that brings together researchers, designers, practitioners, and users, to discuss and share challenges, principles, and strategies for designing such interfaces across the life span. We thus welcome contributions of empirical studies, theories, design, and evaluation of hands-free, mobile, and conversational interfaces designed with aging in mind (e.g. older adults or inter/cross-generational). We particularly encourage contributions focused on leveraging recent advances in generative AI or LLMs. Through this, we aim to grow the community of CUI researchers across disciplinary boundaries (human-computer interaction, voice and language technologies, geronto-technologies, information studies, etc.) that are engaged in the shared goal of ensuring that the aging dimension is appropriately incorporated in mobile / conversational interaction design research.} }
@inproceedings{10.1145/3718751.3718856, title = {Research on the application of artificial intelligence technology in the field of supply chain logistics}, booktitle = {Proceedings of the 2024 4th International Conference on Big Data, Artificial Intelligence and Risk Management}, pages = {657–662}, year = {2025}, isbn = {9798400709753}, doi = {10.1145/3718751.3718856}, url = {https://doi.org/10.1145/3718751.3718856}, author = {Zheng, Linyan}, keywords = {artificial intelligence, logistics, supply chain}, abstract = {With the advent of the intelligent era, the emergence of big data, cloud computing, artificial intelligence, etc. has brought earth-shaking changes to people's lives and production, and also provided technical support for the transformation and upgrading of the logistics and supply chain fields, greatly improving the operational efficiency of China's logistics and supply chain industries. This article starts from the definition and basic design framework of artificial intelligence, deeply studies the specific applications of artificial intelligence in the field of supply chain logistics, analyzes its application limitations, and proposes the future development trend of artificial intelligence in the field of supply chain logistics. It is hoped to play a certain reference role in improving the development of China's supply chain logistics.} }
@inproceedings{10.1145/3703323.3703325, title = {Audio-Visual Speech Synthesis Leveraging Capsule-Enhanced Generative Adversarial Network}, booktitle = {Proceedings of the 8th International Conference on Data Science and Management of Data (12th ACM IKDD CODS and 30th COMAD)}, pages = {10–18}, year = {2025}, isbn = {9798400711244}, doi = {10.1145/3703323.3703325}, url = {https://doi.org/10.1145/3703323.3703325}, author = {Ghosh, Subhayu and Jana, Nanda Dulal}, keywords = {Audio-Visual Speech Synthesis (AVSS), Voice Conversion (VC), Audio-Visual Synthesis (AVS), Generative Adversarial Network (GAN), Capsule Network (Caps-Net).}, abstract = {Audio-visual speech synthesis (AVSS) is a rapidly gaining prominence in artificial intelligence (AI) due to its widespread applications. AVSS refers to generating the audio-visual speech of a target speaker from the audio input of a source speaker, while preserving the speech content. The sequential steps of the AVSS process include voice conversion (VC) for altering vocal characteristics from the source to the target speaker and audio-visual synthesis (AVS) to generate a video stream from the converted speech of the target speaker. However, with the progressive development of deep generative models, the domain of AVSS remains relatively unexplored. The existing literature predominantly focuses on autoencoder-based AVSS models, potentially limiting their ability to capture the diversity of the vocal features and also synthesize the realistic video output. This paper introduces a novel AVSS approach utilizing a generative adversarial network (GAN) model, enriched with a capsule network (Caps-Net) architecture to effectively capture relevant input speech features and produce more natural audio-visual output. The proposed framework is trained and tested on VoxCeleb2 and LRS3-TED datasets and the experimental outcomes exhibit the superiority of the proposed approach over the state-of-the-art (SOTA) models.} }
@inproceedings{10.1145/3715275.3732091, title = {A Trustworthiness-based Metaphysics of Artificial Intelligence Systems}, booktitle = {Proceedings of the 2025 ACM Conference on Fairness, Accountability, and Transparency}, pages = {1360–1370}, year = {2025}, isbn = {9798400714825}, doi = {10.1145/3715275.3732091}, url = {https://doi.org/10.1145/3715275.3732091}, author = {Ferrario, Andrea}, keywords = {artificial intelligence, machine learning, deep learning, identity, metaphysics, ontology, change, time}, abstract = {Modern AI systems are man-made objects that leverage machine learning to support our lives across a myriad of contexts and applications. Despite extensive epistemological and ethical debates, their metaphysical foundations remain relatively under explored. The orthodox view simply suggests that AI systems, as artifacts, lack well-posed identity and persistence conditions—their metaphysical kinds are no real kinds. In this work, we challenge this perspective by introducing a theory of metaphysical identity of AI systems. We do so by characterizing their kinds and introducing identity criteria—formal rules that answer the questions “When are two AI systems the same?” and “When does an AI system persist, despite change?” Building on Carrara and Vermaas’ account of fine-grained artifact kinds, we argue that AI trustworthiness provides a lens to understand AI system kinds and formalize the identity of these artifacts by relating their functional requirements to their physical make-ups. The identity criteria of AI systems are determined by their trustworthiness profiles—the collection of capabilities that the systems must uphold over time throughout their artifact histories, and their effectiveness in maintaining these capabilities. Our approach suggests that the identity and persistence of AI systems is sensitive to the socio-technical context of their design and utilization via their trustworthiness, providing a solid metaphysical foundation to the epistemological, ethical, and legal discussions about these artifacts.} }
@inproceedings{10.1145/3706599.3720145, title = {Exploring the Potential of Generative AI for Supporting Middle-Aged Individuals in Retirement Transitions}, booktitle = {Proceedings of the Extended Abstracts of the CHI Conference on Human Factors in Computing Systems}, year = {2025}, isbn = {9798400713958}, doi = {10.1145/3706599.3720145}, url = {https://doi.org/10.1145/3706599.3720145}, author = {Park, Minseo and Lim, Youn-kyung}, keywords = {generative AI, retirement transitions, middle-aged people, design opportunities}, abstract = {The widespread adoption of Generative AI (GenAI) has fueled research exploring applications across diverse age groups and domains. However, its potential to support individuals in retirement transitions remains underexplored. This study aims to uncover the potential of GenAI in this context by examining its current and anticipated roles through semi-structured interviews with 10 middle-aged individuals navigating retirement transitions. The findings highlight three key roles of GenAI as an identity navigator, self-actualization facilitator, and connection catalyst. Building on the findings, the study identifies novel design opportunities for GenAI-based systems that assist an integrated journey from self-discovery to self-actualization, and support both direct and indirect connection to the world. The research aims to inspire the HCI community to further investigate these new design possibilities and attract attention to the unique context of retirement transition.} }
@inproceedings{10.1145/3709026.3709108, title = {Evolutionary Game Analysis of Universities and Enterprises in Empowering Vocational Education with Artificial Intelligence}, booktitle = {Proceedings of the 2024 8th International Conference on Computer Science and Artificial Intelligence}, pages = {456–465}, year = {2025}, isbn = {9798400718182}, doi = {10.1145/3709026.3709108}, url = {https://doi.org/10.1145/3709026.3709108}, author = {Fang, Wenchao}, keywords = {Artificial intelligence, Enterprise, Evolutionary game, University, Vocational education}, abstract = {Empowering vocational education with artificial intelligence (AI) is a new development direction for vocational education in the new era. Universities and enterprises are two important entities in the research and development (R&amp;D) activities of empowering vocational education with AI. Under the condition of bounded rationality, this paper constructs a university-enterprise evolutionary game model, studies the game interaction process between the two parties in different situations, and verifies the evolutionarily stable strategies through numerical simulation analysis. The results show that: The difference between supervision cost and non-supervision cost of universities, and the difference between high-quality R&amp;D cost and low-quality R&amp;D cost of enterprises are the key factors affecting the evolutionarily stable strategies of universities and enterprises respectively; Strengthening supervision of AI projects by government departments and all sectors of society can effectively promote supervision of universities and high-quality R&amp;D of enterprises; It is also an effective incentive method for universities to appropriately increase the government incentives allocated to enterprises.} }
@inproceedings{10.1145/3722212.3724482, title = {Eighth International Workshop on Exploiting Artificial Intelligence Techniques for Data Management (aiDM)}, booktitle = {Companion of the 2025 International Conference on Management of Data}, pages = {870–871}, year = {2025}, isbn = {9798400715648}, doi = {10.1145/3722212.3724482}, url = {https://doi.org/10.1145/3722212.3724482}, author = {Borovica-Gajic, Renata and Luthra, Manisha and Marcus, Ryan and Bordawekar, Rajesh and Shmueli, Oded}, keywords = {artificial intelligence, data management, machine learning}, abstract = {Recent advances in AI techniques, along with the development of enabling hardware and infrastructure, have facilitated the integration of AI across a wide range of domains and tasks. Specifically, AI has been employed to process various types of data, including numerical, textual, and image data, and has been adopted in large-scale distributed systems. From a data management perspective, this requires the use of cutting-edge AI solutions for data management tasks and systems.aiDM is a full-day workshop that provides a platform for innovative interdisciplinary research on the interaction between AI and data management, and the development of new AI technologies for data-related tasks. This year, aiDM'25 will focus on the transparent application of AI techniques (e.g., utilizing Generative AI frameworks) for data management in enterprise-level workloads.} }
@inproceedings{10.1145/3722237.3722244, title = {Knowledge Map Analysis of Artificial intelligence and Higher Education Based on Citespace}, booktitle = {Proceedings of the 2024 3rd International Conference on Artificial Intelligence and Education}, pages = {34–37}, year = {2025}, isbn = {9798400712692}, doi = {10.1145/3722237.3722244}, url = {https://doi.org/10.1145/3722237.3722244}, author = {Cai, Lihong}, keywords = {Artificial intelligence, Visual Analysis, citespace, higher education}, abstract = {With the advent of the artificial intelligence era, more and more scholars have launched relevant research on the application of artificial intelligence in higher education. With the help of CiteSpace software, this paper conducts econometric analysis on 347 documents related to artificial intelligence and higher education in the core collection of Web of Science. It is found that summarizing the annual publication volume of related research can be divided into three stages, the preliminary exploration stage (2014-2018), the rapid development stage (2019-2021), and the centralized outbreak stage (2022 to present); the hot topics of research on AI and higher education are the AI to higher education The hot topics of research on AI and higher education are the impact of AI on higher education, the system construction and resource construction of AI in higher education, and the application of AI-related technologies in higher education.} }
@inproceedings{10.1145/3706468.3706490, title = {Applying IRT to Distinguish Between Human and Generative AI Responses to Multiple-Choice Assessments}, booktitle = {Proceedings of the 15th International Learning Analytics and Knowledge Conference}, pages = {817–823}, year = {2025}, isbn = {9798400707018}, doi = {10.1145/3706468.3706490}, url = {https://doi.org/10.1145/3706468.3706490}, author = {Strugatski, Alona and Alexandron, Giora}, keywords = {Cheating with AI, separating AI from humans, Item-response theory, person-fit statistics}, abstract = {Generative AI is transforming the educational landscape, raising significant concerns about cheating. Despite the widespread use of multiple-choice questions (MCQs) in assessments, the detection of AI cheating in MCQ-based tests has been almost unexplored, in contrast to the focus on detecting AI-cheating on text-rich student outputs. In this paper, we propose a method based on the application of Item Response Theory (IRT) to address this gap. Our approach operates on the assumption that artificial and human intelligence exhibit different response patterns, with AI cheating manifesting as deviations from the expected patterns of human responses. These deviations are modeled using Person-Fit Statistics (PFS). We demonstrate that this method effectively highlights the differences between human responses and those generated by premium versions of leading chatbots (ChatGPT, Claude, and Gemini), but that it is also sensitive to the amount of AI cheating in the data. Furthermore, we show that the chatbots differ in their reasoning profiles. Our work provides both a theoretical foundation and empirical evidence for the application of IRT to identify AI cheating in MCQ-based assessments.} }
@inproceedings{10.1145/3664647.3689331, title = {Optimizing the Baseline Approach for the 2024 ACM Multimedia Grand Challenge in Artificial Intelligence Generated Image Detection}, booktitle = {Proceedings of the 32nd ACM International Conference on Multimedia}, pages = {11478–11481}, year = {2024}, isbn = {9798400706868}, doi = {10.1145/3664647.3689331}, url = {https://doi.org/10.1145/3664647.3689331}, author = {Chen, Jin}, keywords = {artificial intelligence generation content, convolutional neural networks, generated content detection}, abstract = {This paper presents an optimized approach for the AI-generated image detection task in the 2024 ACM Multimedia Grand Challenge. Given the rapidly evolving capabilities of generative models, traditional detection methods often struggle with accuracy and generalization. The proposed solution builds upon the baseline by integrating advanced model architecture enhancements and novel detection modules, specifically designed to address the complexities of AI-generated images. Through rigorous testing, the approach demonstrates significant improvements in precision, particularly in identifying images produced by unknown generative models. This work highlights the critical need for adaptable and robust detection methods to keep pace with the advancements in AI-generated content and sets a new standard for future research and development in this domain.} }
@inproceedings{10.1145/3700297.3700327, title = {Research on Artificial Intelligence Enabling Accurate Ideological and Political Education for College Students}, booktitle = {Proceedings of the 2024 International Symposium on Artificial Intelligence for Education}, pages = {171–175}, year = {2024}, isbn = {9798400707100}, doi = {10.1145/3700297.3700327}, url = {https://doi.org/10.1145/3700297.3700327}, author = {Zhang, Qi and Xu, Shiting}, keywords = {Artificial-intelligence, Ideological and political education, University students}, abstract = {In the current context of informatization and intelligence, artificial intelligence technology is gradually empowering various industries, including the field of education. In his paper we explored an approach to use artificial intelligence to empower the accurate ideological and political education of college students. We also use artificial intelligence technology to provide personalized and precise ideological and political education programs. How to achieve precise education for college students and promote the overall improvement of their ideological and political quality with the help of data mining, personalized recommendation algorithms, intelligent tutoring systems and other technical means is also explored in this paper. We provide new ideas and methods for the current ideological and political education of college students, and promote the in-depth application of artificial intelligence in the field of education, in order to meet the personalized and diversified learning needs of college students nowadays, and to improve the pertinence and effectiveness of ideological and political education.} }
@inproceedings{10.1145/3719487.3719519, title = {A Systematic Literature Review of the Practical Applications of Artificial Intelligence-Generated Content (AIGC) Using OpenAI ChatGPT, Copilot, and Codex in Programming Education}, booktitle = {Proceeding of the 2024 8th International Conference on Education and E-Learning}, pages = {13–19}, year = {2025}, isbn = {9798400717413}, doi = {10.1145/3719487.3719519}, url = {https://doi.org/10.1145/3719487.3719519}, author = {Chang, Chi-In and Choi, Wan-Chong and Choi, Iek-Chong and Lei, Huey}, keywords = {Applications of AIGC, Artificial intelligence generated content, ChatGPT, Chatbots, Educational technology, Large language models, Programming education, Systematic literature review}, abstract = {This systematic literature review investigated the practical applications of Artificial Intelligence-Generated Content (AIGC) tools, specifically ChatGPT, Copilot, and Codex, in programming education. The review synthesized current research trends and key applications, with a particular focus on large language models (LLMs) and advanced chatbots. The findings revealed a significant increase in research interest following the release of ChatGPT, with most studies concentrating on university-level programming education. The review identified eight primary applications of AIGC tools in programming education, including evaluating AI performance in solving programming tasks, providing AI-driven code generation and assistance, automating assessment review and feedback, delivering personalized learning and tutoring, supporting educators and instructional design, investigating educator and student perceptions, checking for plagiarism, and exploring AI's impact on curriculum design. Despite the promising potential of these tools, critical gaps remained, particularly in primary and secondary education, as well as in online and blended learning environments. Future research was recommended to address these areas and to explore a broader range of AIGC tools to unlock their full potential for enhancing programming education.} }
@inbook{10.1145/3724504.3724582, title = {Applying Artificial Intelligence Techniques to Build a Singing Evaluation System in Choral Singing}, booktitle = {Proceedings of the 2024 2nd International Conference on Information Education and Artificial Intelligence}, pages = {469–475}, year = {2025}, isbn = {9798400711732}, url = {https://doi.org/10.1145/3724504.3724582}, author = {Xu, Yang}, abstract = {In the past decade, chorus as a form of musical art performance, in the more developed areas of China, young people interested in chorus and retired elderly people have joined the choir for learning. This paper seeks to understand the problems of teaching choral tunes in a way that the singing voice is not easy to learn and the students are not easy to understand the instructor's verbal comments and corrections, and proposes the use of artificial intelligence technology to assist the teaching of choral tunes and to solve the above problems. This paper proposes an auxiliary system for choral teaching, for which this study collects a collection of a cappella vocal recordings of a choral seven-word tune, and uses music information retrieval and machine learning algorithms to analyze and compare the recording files, and outputs style similarity scores and visualization charts of the user's input and the collection of sound files. Teachers of choral teaching can use this system to solve the difficulties in teaching choral promotion, and the karaoke game-like tune teaching system has a higher willingness to learn for choral beginners.} }
@inproceedings{10.1145/3746709.3746969, title = {Optimization design and application of artificial intelligence in intelligent transportation system}, booktitle = {Proceedings of the 2025 6th International Conference on Computer Information and Big Data Applications}, pages = {1508–1514}, year = {2025}, isbn = {9798400713163}, doi = {10.1145/3746709.3746969}, url = {https://doi.org/10.1145/3746709.3746969}, author = {Qiu, Bibo}, keywords = {Intelligent traffic systems, artificial intelligence, computational models, machine learning, performance evaluation, traffic optimization}, abstract = {The Intelligent Transportation System (ITS) is the effective and comprehensive application of advanced science and technology (information technology, computer technology, data communication technology, sensor technology, electronic control technology, automatic control theory, operations research, artificial intelligence, etc.) in transportation, service control, and vehicle manufacturing to strengthen the connection between vehicles, roads, and users. The result is an integrated transport system that ensures safety, enhances efficiency, improves the environment and saves energy. It is an effective means to solve the current urban traffic problems and an important indicator for the construction of a "smart city". Through the construction of an urban traffic collection system, the system solves the existing traffic problems, further integrates all kinds of road traffic resources, builds the urban "traffic brain", mines the huge amount of traffic data, deconstructs the operation law and characteristics of urban traffic, realizes the accurate urban traffic management, scientific management and normal management, and makes people's traffic travel safer. It makes people's traffic travel safer, more convenient and more efficient. This study introduces innovative computational models and algorithms specifically designed for traffic optimisation, as well as data-driven machine learning techniques to improve the operational efficiency of intelligent transport systems. An extensive analysis of case studies on the integration of AI into traffic management systems provides empirical evidence of their impact, focusing on the performance evaluation of these AI applications as well as potential scalability issues that may arise in different urban environments.} }
@inproceedings{10.1145/3748825.3748845, title = {Research on the role transformation of higher vocational education teachers in the era of artificial intelligence}, booktitle = {Proceedings of the 2025 2nd International Conference on Digital Society and Artificial Intelligence}, pages = {115–120}, year = {2025}, isbn = {9798400714337}, doi = {10.1145/3748825.3748845}, url = {https://doi.org/10.1145/3748825.3748845}, author = {He, Jing and Zhang, Dan and Xu, Xiaoyang and Su, Yan}, keywords = {Artificial intelligence, Higher vocational education, Role, Teacher, Transformation}, abstract = {With the deepening application of artificial intelligence technology in the field of vocational education, the role previously played by teachers will inevitably be impacted by the times, and teachers will be given new roles in the era. Based on the theory of transformational learning, this article uses a combination of questionnaire surveys and semi-structured interviews to gain a deeper understanding of the current situation of the role transformation of vocational college teachers, analyze the internal and external factors that constrain their transformation, and propose strategies from three aspects: guiding teachers to engage in self dialogue, building a teacher professional development community, and creating a teacher training model that integrates research and training. These strategies aim to help teachers surpass traditional roles and achieve role consistency in the process of educational reform and development.} }
@inproceedings{10.1145/3696630.3727255, title = {DevCoach: Supporting Students Learning the Software Development Life Cycle with a Generative AI powered Multi-Agent System}, booktitle = {Proceedings of the 33rd ACM International Conference on the Foundations of Software Engineering}, pages = {987–998}, year = {2025}, isbn = {9798400712760}, doi = {10.1145/3696630.3727255}, url = {https://doi.org/10.1145/3696630.3727255}, author = {Wang, Tianjia and Trimble, Matthew and Brown, Chris}, keywords = {software development life cycle, generative AI, multi-agent system}, abstract = {The software development life cycle (SDLC) is vital for ensuring the quality of software systems. However, learning SDLC concepts presents unique challenges, such as the need for effective collaboration, real-time interaction, and access to diverse skill sets represented in software development teams. To address these problems, we present DevCoach, a generative AI powered multi-agent system designed to support students learning the SDLC. DevCoach allows students to interact with generative AI agents simulating the different roles in the software development team, engaging in tasks across different phases of SDLC. Through a user study (n = 20), we evaluate the system's effectiveness in enhancing learning, impact on SDLC deliverables, and support for Community of Inquiry (CoI) elements necessary for effective and supportive learning environments. Our results reveal that students using DevCoach achieved significantly higher learning gains and improved task completion rates across all SDLC phases. The system also supports CoI elements, particularly perceived social presence. Participants also lauded the immediate context-aware feedback, interactive learning environment, and diverse expertise provided by the roles within the multi-agent team. These findings demonstrate the potential of generative AI to enhance software engineering education by making it more effective, engaging, and interactive, providing students with collaborative and practical learning experiences.} }
@inproceedings{10.1145/3702163.3702180, title = {The Role of Artificial Intelligence Tools in Knowledge Generation: Implications for Education}, booktitle = {Proceedings of the 2024 16th International Conference on Education Technology and Computers}, pages = {117–124}, year = {2025}, isbn = {9798400717819}, doi = {10.1145/3702163.3702180}, url = {https://doi.org/10.1145/3702163.3702180}, author = {Pe\~nafiel, Myriam Guadalupe and V\'asquez-Pe\~nafiel, Mar\'a-Stefanie and Pe\~nafiel, Diego Alberto V\'asquez}, keywords = {AI, AI tools, Artificial intelligence, Knowledge Generation, Mathematics of computing, education}, abstract = {The research delves into the transformative potential of artificial intelligence (AI) tools in knowledge generation and their pivotal role in the educational process. It illuminates which AI tools can revolutionize educational activities, harnessing their power through the myriad AI tools available for diverse educational functions and the most popular AI tools that amplify student learning. Insights from both public and private higher education institutions, encompassing students from various semesters, have unveiled significant revelations. A key finding was the students' awareness of the available AI tools and their preferences. These findings underscore the imperative of effectively integrating diverse AI tools within the educational environment to bolster knowledge generation among students, thereby highlighting areas for improvement and potential in educational practices related to AI.} }
@inproceedings{10.1145/3706598.3713093, title = {A Scoping Review of Gender Stereotypes in Artificial Intelligence}, booktitle = {Proceedings of the 2025 CHI Conference on Human Factors in Computing Systems}, year = {2025}, isbn = {9798400713941}, doi = {10.1145/3706598.3713093}, url = {https://doi.org/10.1145/3706598.3713093}, author = {Duan, Wen and Li, Lingyuan and Freeman, Guo and McNeese, Nathan}, keywords = {Artificial Intelligence, Gender stereotypes, Anthropomorphism}, abstract = {People often apply gender stereotypes to Artificial Intelligence (AI), and AI design frequently reinforces these stereotypes, perpetuating traditional gender ideologies in state-of-the-art technology. Despite growing interests in investigating this phenomenon, there is little conceptual clarity or consistency regarding what actually constitutes a "gender stereotype" in AI. Therefore, it is critical to provide a more comprehensive image of existing understandings and ongoing discussions of gender stereotypes of AI to guide AI design that reduces the harmful effects of these stereotypes. In doing so, this paper presents a scoping review of over 20 years of research across HCI, HRI and various social science disciplines on how gender stereotypes are applied to AI. We outline the methods and contexts of this growing body of work, develop a typology to clarify these stereotypes, highlight under-explored approaches for future research, and offer guidelines to improve rigor and consistency in this field that may inform responsible AI design in the future.} }
@article{10.1145/3656588, title = {Image Generative AI to Design Public Spaces: a Reflection of How AI Could Improve Co-Design of Public Parks}, journal = {Digit. Gov.: Res. Pract.}, volume = {6}, year = {2025}, doi = {10.1145/3656588}, url = {https://doi.org/10.1145/3656588}, author = {Guridi, Jose A. and Cheyre, Cristobal and Goula, Maria and Santo, Duarte and Humphreys, Lee and Souras, Achilleas and Shankar, Aishwarya}, keywords = {Image-generative AI, landscape architecture, design, public spaces}, abstract = {Image generative AI (IGAI) could change how policymakers engage with the public to design public spaces, facilitating how designers translate the public’s desires into features. However, using IGAI has challenges, such as encoded biases, which might reinforce stereotypes and harm underrepresented communities. We conducted a case study to explore how using IGAI alters the co-design process of public parks through public engagement. We use data collected from interviews with immigrants discussing the Puente Hills Landfill Park design in Los Angeles, which will re-purpose a former landfill into a new public park. We use Dream Studio as a Design Probe, generating images from the interviewees’ insights and critically reflecting on the design process through internal interviews and a reflective workshop. We analyze our case in three domains: Opportunities, Risks and Challenges, and Features and Requirements. In the opportunities domain, we discuss how the enhanced translation of words to images changes the relationship between stakeholder engagement, multiplicity, and efficiency. In the risks and challenges domain, we discuss how IGAI might enhance power imbalances and biases. Finally, we reflect on what features would ease the safe and responsible use of IGAI to engage citizens in co-designing public parks.} }
@inproceedings{10.1145/3688671.3688772, title = {Artificial Intelligence in Education: Ethical Considerations and Insights from Ancient Greek Philosophy}, booktitle = {Proceedings of the 13th Hellenic Conference on Artificial Intelligence}, year = {2024}, isbn = {9798400709821}, doi = {10.1145/3688671.3688772}, url = {https://doi.org/10.1145/3688671.3688772}, author = {Karpouzis, Kostas}, keywords = {Artificial Intelligence, Education, Philosophy, Ethics, Explainability, Autonomy, Teaching}, abstract = {This paper explores the ethical implications of integrating Artificial Intelligence (AI) in educational settings, from primary schools to universities, while drawing insights from ancient Greek philosophy to address emerging concerns. As AI technologies increasingly influence learning environments, they offer novel opportunities for personalized learning, efficient assessment, and data-driven decision-making. However, these advancements also raise critical ethical questions regarding data privacy, algorithmic bias, student autonomy, and the changing roles of educators. This research examines specific use cases of AI in education, analyzing both their potential benefits and drawbacks. By revisiting the philosophical principles of ancient Greek thinkers such as Socrates, Aristotle, and Plato, we discuss how their writings can guide the ethical implementation of AI in modern education. The paper argues that while AI presents significant challenges, a balanced approach informed by classical philosophical thought can lead to an ethically sound transformation of education. It emphasizes the evolving role of teachers as facilitators and the importance of fostering student initiative in AI-rich environments.} }
@inbook{10.1145/3676641.3716009, title = {PAPI: Exploiting Dynamic Parallelism in Large Language Model Decoding with a Processing-In-Memory-Enabled Computing System}, booktitle = {Proceedings of the 30th ACM International Conference on Architectural Support for Programming Languages and Operating Systems, Volume 2}, pages = {766–782}, year = {2025}, isbn = {9798400710797}, url = {https://doi.org/10.1145/3676641.3716009}, author = {He, Yintao and Mao, Haiyu and Giannoula, Christina and Sadrosadati, Mohammad and G\'omez-Luna, Juan and Li, Huawei and Li, Xiaowei and Wang, Ying and Mutlu, Onur}, abstract = {Large language models (LLMs) are widely used for natural language understanding and text generation. An LLM model relies on a time-consuming step called LLM decoding to generate output tokens. Several prior works focus on improving the performance of LLM decoding using parallelism techniques, such as batching and speculative decoding. State-of-the-art LLM decoding has both compute-bound and memory-bound kernels. Some prior works statically identify and map these different kernels to a heterogeneous architecture consisting of both processing-in-memory (PIM) units and computation-centric accelerators (e.g., GPUs). We observe that characteristics of LLM decoding kernels (e.g., whether or not a kernel is memory-bound) can change dynamically due to parameter changes to meet user and/or system demands, making (1) static kernel mapping to PIM units and computation-centric accelerators suboptimal, and (2) one-size-fits-all approach of designing PIM units inefficient due to a large degree of heterogeneity even in memory-bound kernels.In this paper, we aim to accelerate LLM decoding while considering the dynamically changing characteristics of the kernels involved. We propose PAPI (PA rallel Decoding with PI M), a PIM-enabled heterogeneous architecture that exploits dynamic scheduling of compute-bound or memory-bound kernels to suitable hardware units. PAPI has two key mechanisms: (1) online kernel characterization to dynamically schedule kernels to the most suitable hardware units at runtime and (2) a PIM-enabled heterogeneous computing system that harmoniously orchestrates both computation-centric processing units (GPU) and hybrid PIM units with different computing capabilities. Our experimental results on three broadly-used LLMs (i.e., LLaMA-65B, GPT-3 66B, and GPT-3 175B) show that PAPI achieves 1.8 and 11.1 speedups over a state-of-the-art heterogeneous LLM accelerator (i.e., GPU and PIM) and a state-of-the-art PIM-only LLM accelerator, respectively.} }
@inproceedings{10.1145/3669754.3669802, title = {Applications of Artificial Intelligence in Disaster Management}, booktitle = {Proceedings of the 2024 10th International Conference on Computing and Artificial Intelligence}, pages = {313–318}, year = {2024}, isbn = {9798400717055}, doi = {10.1145/3669754.3669802}, url = {https://doi.org/10.1145/3669754.3669802}, author = {Gupta, Tanu and Roy, Sudip}, keywords = {Artificial Intelligence, Disaster Management, Human-made Disaster, Natural disaster}, abstract = {In post-disaster scenarios, government and non-governmental organizations (NGOs) work to coordinate disaster relief efforts efficiently, aiming to meet humanitarian needs promptly. Utilizing available data such as satellite imagery, sensor data, and social media, alongside data mining and big data analytics, can significantly enhance disaster management efforts. However, timely access to these often fragmented and incomplete data presents challenges. Artificial Intelligence (AI) techniques, including machine learning, deep learning, and natural language processing, offer solutions by enabling faster response times for government agencies, NGOs, and other authorities. This research examines 72 studies on the application of AI in various stages of disaster management, including the simulation, detection, prediction, and handling of post-disaster situations. AI-based systems improve the issuance of early warnings, streamline risk communication, optimize relief logistics, and support evacuation plans while also aiding in the decision-making processes for the issuance of building permits and grants. The analysis highlights the transformative potential of AI across all disaster management phases, from preparedness and response to prevention/mitigation and recovery, and identifies future challenges in this domain. In conclusion, the study underscores the power of AI not only in forecasting the occurrence and impact zones of disasters but also in identifying the most vulnerable communities and assessing the viability of disaster response strategies.} }
@inproceedings{10.1145/3722237.3722285, title = {Analysis of Vocal Music Teaching in University Based on Artificial Intelligence}, booktitle = {Proceedings of the 2024 3rd International Conference on Artificial Intelligence and Education}, pages = {281–285}, year = {2025}, isbn = {9798400712692}, doi = {10.1145/3722237.3722285}, url = {https://doi.org/10.1145/3722237.3722285}, author = {Han, Jinshan}, keywords = {Artificial intelligence, Teaching Analysis, Vocal teaching}, abstract = {With the increasing development of electronic computer information technology, concepts such as intelligent networking and big data have been introduced one after another. In this study, With the help of python tools, this paper explores optimization strategies for vocal music teaching based on machine learning, such as using convolutional neural network (CNN) and long short-term memory network (LSTM) to process and analyze audio data, and classifying and sentiment analysis of students' feedback through natural language processing technology. This article introduces the concept of artificial intelligence and its relationship with vocal music teaching. From the perspectives of intelligent teaching equipment, archival textbook materials, and auxiliary training methods, it aims to provide new ideas and development directions for the integration of artificial intelligence into vocal music teaching in universities.} }
@inbook{10.1145/3745238.3745463, title = {Research on Internet of Things Monitoring and Defense Technology Based on Artificial Intelligence}, booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence}, pages = {1437–1442}, year = {2025}, isbn = {9798400712791}, url = {https://doi.org/10.1145/3745238.3745463}, author = {Yu, Kechun}, abstract = {As the Internet of Things (IoT) is widely used in smart campuses of higher education institutions, the heterogeneity of devices, resource limitations and complexity of security threats have become urgent issues to be addressed. In this research, an IoT monitoring and defence technology solution that integrates edge AI and deep learning is proposed for a specific higher education institution. By constructing a hybrid model that combines a multilayer perceptron (MLP) and a convolutional neural network (CNN), anomalous traffic is detected with 98.7% accuracy. A lightweight encryption protocol aims to reduce device power consumption by 30%. A dynamic defensive rule engine is developed with a response latency of less than 50ms. The experiment is validated using the CIC IoT 2022 dataset, which shows that the system outperforms traditional solutions in DDoS attack detection and side-channel attack defence, providing a reusable technology path for IoT security in education.} }
@proceedings{10.1145/3660043, title = {ICIEAI '23: Proceedings of the 2023 International Conference on Information Education and Artificial Intelligence}, year = {2023}, isbn = {9798400716157} }
@inproceedings{10.1145/3708394.3708448, title = {Research on Application Strategies of Artificial Intelligence in Intelligent Teaching of Ideological and Political Education in Higher Vocational Colleges}, booktitle = {Proceeding of the 2024 International Conference on Artificial Intelligence and Future Education}, pages = {315–319}, year = {2025}, isbn = {9798400710650}, doi = {10.1145/3708394.3708448}, url = {https://doi.org/10.1145/3708394.3708448}, author = {Feng, Song and Zhou, Quan and Chen, Shuang}, keywords = {Application Strategies, Artificial Intelligence, Higher Vocational Colleges, Ideological and Political Courses, Intelligent Teaching}, abstract = {With the rapid development of artificial intelligence technology, the education sector is undergoing unprecedented changes. Ideological and political theory courses in higher vocational colleges, which are crucial for cultivating students' correct values and improving their ideological and moral qualities, also face new opportunities and challenges. This paper begins with the current application status of artificial intelligence technology in the teaching of ideological and political courses in higher vocational colleges, analyzes application strategies in teaching content, methods, and evaluation, and proposes strategies such as constructing intelligent teaching platforms, developing personalized learning systems, updating teaching content intelligently, diversifying teaching methods, implementing intelligent assessment and feedback systems, and building smart campus environments. The aim is to provide references and insights for intelligent teaching in ideological and political courses in higher vocational colleges.} }
@inproceedings{10.1145/3726302.3730349, title = {Information Retrieval for Artificial General Intelligence: A New Perspective of Information Retrieval Research}, booktitle = {Proceedings of the 48th International ACM SIGIR Conference on Research and Development in Information Retrieval}, pages = {3876–3886}, year = {2025}, isbn = {9798400715921}, doi = {10.1145/3726302.3730349}, url = {https://doi.org/10.1145/3726302.3730349}, author = {Zhai, ChengXiang}, keywords = {artificial general intelligence, neurosymbolic architecture, new retrieval tasks, retrieval for agent, retrieval-augmented generation}, abstract = {Traditionally, the users of an information retrieval (IR) system have been human users. We present a new perspective on IR research in which the users of an IR system are intelligent agents instead of human users. Extending the current work on retrieval-augmented generation (RAG), we identify five novel IR tasks that an intelligent agent must be able to perform in order to achieve Human-Level Artificial Intelligence, or Artificial General Intelligence (AGI), including 1) External Information Retrieval (EIR) to access new information unseen by the agent, 2) Provenance Information Retrieval (PIR) to trace the provenance of information, 3) Curriculum Information Retrieval (CIR) to actively acquire the most useful new data and information for lifelong learning, 4) Rule Information Retrieval (RIR) to perform reasoning and problem solving, and 5) Scenario Information Retrieval (SIR) to leverage past scenarios for problem solving and decision making. We compare these new IR tasks with the traditional IR tasks performed by an IR system that serves human users and systematically examine the challenges involved in the five new IR tasks, providing a roadmap for new IR research within the broader context of AGI development.} }
@inproceedings{10.1145/3726010.3726094, title = {The Application of Artificial Intelligence in the Creation of Four Grid Painting and Product Design}, booktitle = {Proceedings of the 2024 International Conference on Artificial Intelligence, Digital Media Technology and Interaction Design}, pages = {536–543}, year = {2025}, isbn = {9798400712845}, doi = {10.1145/3726010.3726094}, url = {https://doi.org/10.1145/3726010.3726094}, author = {Xu, Linlin and Cheng, Pengfei}, keywords = {Artificial Intelligence, GAN, Midjourney, VAE, artistic creation, four-grid canvas painting, mental health}, abstract = {This research work detailed the utilization of Artificial Intelligence (AI) particularly in the development of four-grid canvas paintings. This is possible by integrating the computational power of AI with conventional art designs. AI-based approaches play a significant role in art creation which includes human emotions and exactness. Four-grid painting is an artistic approach that splits the canvas into four separate distinctive sections. Each section of the canvas illustrates a diverse topic. This work comes across the history of four-grid canvas paintings, state-of-the-art algorithms, and architectures of AI drawing methods. We discuss one of the prominent AI platforms called Midjourney and its use in creating four-grid paintings. Furthermore, this paper focuses on the employment of AI in building four-grid paintings and the product design. This illustrates the advancements in the field of amusement, healing through art, and visual aesthetics. Numerous factors have been the focus of a comparative analysis of various AI platforms. These findings will assist in the development of AI-enhanced art.} }
@inproceedings{10.1145/3756580.3756603, title = {Research on E-commerce Assisted Teaching System Based on Artificial Intelligence Technology}, booktitle = {Proceedings of the 2025 6th International Conference on Education, Knowledge and Information Management}, pages = {143–147}, year = {2025}, isbn = {9798400715624}, doi = {10.1145/3756580.3756603}, url = {https://doi.org/10.1145/3756580.3756603}, author = {Sheng, Li}, keywords = {Artificial intelligence technology, Assisted teaching system, E-commerce}, abstract = {With the rapid development of artificial intelligence technology represented by machine learning and natural language processing in the world, its application scenarios in the field of education are increasingly rich and in-depth, which is profoundly changing the traditional teaching mode and methods. This paper focuses on the field of e-commerce education, and is committed to conducting in-depth research and building an innovative e-commerce-assisted teaching system based on artificial intelligence technology. In the study, we conducted a comprehensive analysis of the system architecture to ensure its stability and scalability. The core functions are analyzed, including personalized learning path planning, intelligent question answering and teaching evaluation modules, which can customize learning plans according to students; conditions, quickly answer questions, and monitor the learning effect to guide teaching strategies. This paper deeply discusses how the artificial intelligence technology can significantly improve the quality and efficiency of e-commerce teaching. Through a large amount of data and charts, the students; study behaviors and achievements before and after using the system are relatively analyzed, which proves the effectiveness and practicability of the e-commerce assisted teaching system based on artificial intelligence.} }
@article{10.1145/3700140, title = {Generative AI is already widespread in the public sector: evidence from a survey of UK public sector professionals}, journal = {Digit. Gov.: Res. Pract.}, volume = {6}, year = {2025}, doi = {10.1145/3700140}, url = {https://doi.org/10.1145/3700140}, author = {Bright, Jonathan and Enock, Florence and Esnaashari, Saba and Francis, John and Hashem, Youmna and Morgan, Deborah}, keywords = {Generative AI, Public Sector, Productivity, Bureaucracy}, abstract = {Generative AI has the potential to transform how public services are delivered by enhancing productivity and reducing time spent on bureaucracy. But to what extent is the technology already in use? Our survey of UK public service professionals (in education, health, social work, and emergency services) seeks to answer this question. We find that use of generative AI is widespread: 45% of respondents were aware of colleagues using generative AI, while 22% use it themselves. Respondents were positive about its potential to enhance their efficiency and reduce their bureaucratic workload. For example, those working in the health service thought that time spent on bureaucracy could drop by the equivalent of one day per week, an enormous potential impact. Our survey also found a high amount of trust (61%) in generative AI and a low fear of replacement (16%). However, only a minority of respondents (32%) felt like there was clear guidance on generative AI usage in their workplaces. In other words, it is clear that generative AI is already coming into the public sector, but uptake is happening in a disorganised fashion without clear guidelines. The UK’s public sector urgently needs to develop more systematic methods for taking advantage of the technology.} }
@inproceedings{10.1145/3651671.3651681, title = {Generative-AI- and Optical-Flow-Based Aspect Ratio Enhancement of Videos}, booktitle = {Proceedings of the 2024 16th International Conference on Machine Learning and Computing}, pages = {355–362}, year = {2024}, isbn = {9798400709234}, doi = {10.1145/3651671.3651681}, url = {https://doi.org/10.1145/3651671.3651681}, author = {Palczewski, Tomasz Jan and Rao, Anirudh and Zhu, Yingnan}, keywords = {Gen-AI, aspect ratio enhancement, neural enhancement, optical-flow}, abstract = {The global ultra-wide display market is growing rapidly due to widespread consumer adoption. However, existing content, especially on TV screens, is mainly designed for lower aspect ratios like 4:3. As TVs shift towards wider formats, there’s a need to develop a method to transform legacy content and adapt to varying screen sizes. This shift holds the potential to revolutionize the content viewing experience for gamers, content creators, and streaming enthusiasts. While delivering high-quality visual content for dynamic aspect ratios remains a challenge, recent advancements in Deep Learning show promise in addressing these problems with generative approaches. Our contribution begins with a survey of prior video completion efforts, providing a foundational backdrop. We then introduce our novel solution, combining optical flow methodologies with generative latent diffusion models. These models, conditioned on an initial prompt and evolving video frame, refine content generation. We validate our approach on the DAVIS dataset, demonstrating its efficacy and robustness. In summary, our study pioneers advancements in content generation for ultra-wide displays.} }
