@inproceedings{10.1145/3744367.3744422, title = {Artificial intelligence-driven innovation in language teaching and learning in vocational colleges: paths, practices and challenges}, booktitle = {Proceedings of the 2025 International Conference on Artificial Intelligence and Educational Systems}, pages = {345–352}, year = {2025}, isbn = {9798400715068}, doi = {10.1145/3744367.3744422}, url = {https://doi.org/10.1145/3744367.3744422}, author = {Wu, Guijie and Lin, Qingqing}, keywords = {Artificial Intelligence, Ethical Governance, Multimodal Learning, Natural Language Processing, Vocational Education}, abstract = {The “Technology and Humanities Integration” teaching model aims to seamlessly integrate advanced technologies with humanities education principles, thereby enriching the experience of Chinese Language classrooms. Intelligent assessment systems powered by powerful data processing and analysis capabilities can accurately assess teaching efficiency and student learning progress. The collaborative framework between teachers and technology emphasizes the optimal use of teachers’ professional skills, supported by advanced technologies, thereby improving teaching efficiency and quality. Regarding the application of technology, semantic analysis is performed using the BERT model, the random forest algorithm is used to optimize teaching scenarios, and the structural equation model is applied to evaluate moral hazard. Chinese teaching practice using virtual reality technology shows that students' cultural perception ability has improved by 42%, a finding that has been verified by rigorous statistical tests (P &lt; 0.001) and is therefore very reliable. In addition, the writing aids used significantly improved students’ Chinese writing skills, with students using these tools increasing their average Chinese score by 15%. These empirical data firmly affirm the value of cutting-edge technology in language education in vocational schools.} }
@inproceedings{10.1145/3722237.3722249, title = {Research hotspots, current situation and trend analysis of artificial intelligence-assisted learning based on CiteSpace (2014-2024)}, booktitle = {Proceedings of the 2024 3rd International Conference on Artificial Intelligence and Education}, pages = {62–68}, year = {2025}, isbn = {9798400712692}, doi = {10.1145/3722237.3722249}, url = {https://doi.org/10.1145/3722237.3722249}, author = {Zhang, Feng and Li, Yujiao and Ruan, Mengjiao and Liu, Huan and Luo, Wenxing}, keywords = {CiteSpace, artificial intelligence, assisted learning, hot spot analysis}, abstract = {By analyzing the research literature in the field of artificial intelligence assisted learning in China in the past ten years, this paper uses CiteSpace software to explore the hotspots, current situation and future development trend in this field. The application of artificial intelligence in the field of education is becoming more and more extensive, showing great potential in personalized learning, intelligent tutoring, teaching resource recommendation and so on. At present, the research on artificial intelligence assisted learning in China is gradually deepening, which has a positive impact on the technical level and also has a positive impact on the level of educational practice. The research finds that the hotspots in this field focus on intelligent personalized teaching, automatic evaluation feedback system, intelligent tutoring and answering questions, virtual experimental teaching driven by big data, and teaching decision support. In view of this, this paper predicts the future development trend of the integration of technology, policy and market, education model and artificial intelligence.} }
